      <Layout IsNewNode="True">
        <LayoutGroup Id="RootGroup" Direction="Horizontal" Index="0" ShowCaption="False" IsNewNode="True">
          <LayoutItem Id="Image" Index="0" ShowCaption="False" RelativeSize="1" ViewItem="Image" IsNewNode="True" />
          <LayoutItem Id="Message" Index="1" ShowCaption="False" RelativeSize="99" ViewItem="Message" IsNewNode="True" />
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="AboutInfo_DetailView">
      <Items>
        <StaticText Id="AboutText" Text="{0:ProductName}&lt;br&gt;{0:Version}&lt;br&gt;{0:Copyright}&lt;br&gt;{0:Description}" />
      </Items>
    </DetailView>
  </Views>
