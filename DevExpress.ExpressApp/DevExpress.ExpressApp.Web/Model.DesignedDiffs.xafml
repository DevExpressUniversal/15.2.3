      </Layout>
    </DetailView>
    <DetailView Id="ParameterlessLogonFailedInfo_DefaultDetailView" Caption="Log On" ClassName="DevExpress.ExpressApp.Web.ParameterlessLogonFailedInfo" IsNewNode="True">
      <Items IsNewNode="True">
        <StaticImage Id="Image" ImageName="State_Validation_Invalid_48x48" SizeMode="AutoSize" VerticalAlign="Middle" IsNewNode="True" />
        <StaticText Id="Message" Text="{0} Please consider contacting an administrator about your login details." HorizontalAlign="Center" IsNewNode="True" />
