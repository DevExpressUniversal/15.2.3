<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxprg="clr-namespace:DevExpress.Xpf.PropertyGrid"
                    xmlns:dxprgt="clr-namespace:DevExpress.Xpf.PropertyGrid.Themes"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"                    
                    >


    <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=RowCommandButton}" >
        <dxn:RenderBorder Background="#FFFFFFFF" Width="10" Height="10" Margin="5,0,6,0" Padding="1">
            <dxn:RenderBorder Name="Root" Background="#ffffffff">
                <dxn:RenderPath Stretch="Fill" Fill="#bf062b63" Data="F1 M 10,10L 0,10L 0,0L 10,0L 10,10 Z M 1,1L 1,9L 5,9L 9,5L 9,1L 1,1 Z " />
            </dxn:RenderBorder>
        </dxn:RenderBorder>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CommonStates">
                <dxn:RenderStateTrigger Name="Normal"/>
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter TargetName="Root" Property="Background" Value="#7f062b63" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>            
            <dxn:RenderTrigger DependencyProperty="{x:Static dxprg:RowControl.ShouldHighlightValueProperty}" Value="True">
                <dxn:RenderSetter TargetName="Root" Property="Background" Value="#bf062b63" />
            </dxn:RenderTrigger>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>


    <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=RowCollectionButton}" >
        <dxn:RenderBorder Name="PART_Root" Background="Transparent" Opacity="0.5">
            <dxn:RenderPanel>
                    <dxn:RenderPath Name="PART_Add"
                                    Stretch="Fill" 
                                    Fill="#FF999999" 
                                    Data="F1 M 6,0C 9.31371,0 12,2.68629 12,6C 12,9.31371 9.31371,12 6,12C 2.68629,12 -6.35783e-007,9.31371 -6.35783e-007,6C -6.35783e-007,2.68629 2.68629,0 6,0 Z M 2,5L 2,7L 5,7L 5,10L 7,10L 7,7L 10,7L 10,5L 7,5L 7,2L 5,2L 5,5L 2,5 Z " 
                                    Width="12" Height="12" 
                                    Visibility="Visible"
                                    />
                    <dxn:RenderPath Name="PART_Remove" 
                                    Stretch="Fill" 
                                    Fill="#FFCC0000" 
                                    Data="F1 M 1.75736,1.75736C 4.1005,-0.585785 7.89949,-0.585785 10.2426,1.75736C 12.5858,4.10051 12.5858,7.8995 10.2426,10.2426C 7.89949,12.5858 4.1005,12.5858 1.75736,10.2426C -0.585787,7.8995 -0.585787,4.10051 1.75736,1.75736 Z M 2.46447,8.12132L 3.87868,9.53553L 6,7.41421L 8.12132,9.53553L 9.53553,8.12132L 7.41421,6L 9.53553,3.87868L 8.12132,2.46447L 6,4.58579L 3.87868,2.46447L 2.46447,3.87868L 4.58579,6L 2.46447,8.12132 Z " 
                                    Width="12" Height="12" 
                                    Visibility="Collapsed"
                                    />
            </dxn:RenderPanel>
        </dxn:RenderBorder>        
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CommonStates">
                <dxn:RenderStateTrigger Name="Normal"/>
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter TargetName="PART_Root" Property="Opacity" Value="0.75" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="ButtonKind">
                <dxn:RenderStateTrigger Name="Add"/>
                <dxn:RenderStateTrigger Name="Remove">
                    <dxn:RenderSetter TargetName="PART_Remove" Property="Visibility" Value="Visible" />
                    <dxn:RenderSetter TargetName="PART_Add" Property="Visibility" Value="Collapsed" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>            
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>

    <dxn:RenderTemplate x:Key="{dxprgt:VSViewRenderTemplateThemeKey ResourceKey=Expander}">
        <dxn:RenderBorder Background="Transparent" >
            <dxn:RenderPanel Width="6" Height="7" HorizontalAlignment="Center" VerticalAlignment="Top">
                <dxn:RenderPath Name="PART_Glyph" 
                            Stretch="Fill" 
                            Fill="#062b63" 
                            Data="F1 M109,189 L109,195 L115,192 L109,189 z"                                                         
                            />                
            </dxn:RenderPanel>
        </dxn:RenderBorder>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CheckedStates">
                <dxn:RenderStateTrigger Name="Checked">
                    <dxn:RenderSetter TargetName="PART_Glyph" Property="TransformOrigin" Value="0.5,0.5"/>
                    <dxn:RenderSetter TargetName="PART_Glyph" Property="Transform">
                        <dxn:RenderSetter.Value>
                            <RotateTransform Angle="90"/>
                        </dxn:RenderSetter.Value>
                    </dxn:RenderSetter>
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="Unchecked"/>
            </dxn:RenderStateGroup>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>

    <sys:Double x:Key="{dxprgt:PropertyGridViewThemeKey ResourceKey=RowMinHeight}">20</sys:Double>
    <SolidColorBrush Color="#00000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Editor_Normal}" />
    <SolidColorBrush Color="#00000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Editor_ReadOnly}" />
    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Editor_Selected}">
        <GradientStop Color="#1F7b9bc8" Offset="0" />
        <GradientStop Color="#1F7b9bc8" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Editor_SelectedInactive}">
        <GradientStop Color="#1F7b9bc8" Offset="0" />
        <GradientStop Color="#1F7b9bc8" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Editor_SelectedReadOnly}">
        <GradientStop Color="#1F7b9bc8" Offset="0" />
        <GradientStop Color="#1F7b9bc8" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Editor_SelectedInactiveReadOnly}">
        <GradientStop Color="#1F7b9bc8" Offset="0" />
        <GradientStop Color="#1F7b9bc8" Offset="1" />
    </LinearGradientBrush>
    <SolidColorBrush Color="#FF000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Editor_Normal}" />
    <SolidColorBrush Color="#C0000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Editor_ReadOnly}" />
    <SolidColorBrush Color="#FF000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Editor_Selected}" />
    <SolidColorBrush Color="#FF000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Editor_SelectedInactive}" />
    <SolidColorBrush Color="#FF000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Editor_SelectedReadOnly}" />
    <SolidColorBrush Color="#FF000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Editor_SelectedInactiveReadOnly}" />
    <SolidColorBrush Color="#FFFFFFFF" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Header_Normal}" />
    <SolidColorBrush Color="#FFFFFFFF" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Header_ReadOnly}" />
    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Header_Selected}">
        <GradientStop Color="#FF7b9bc8" Offset="0" />
        <GradientStop Color="#FF5c7eaf" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Header_SelectedInactive}">
        <GradientStop Color="#FFC8DCF3" Offset="0" />
        <GradientStop Color="#FFB7D1EF" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Header_SelectedReadOnly}">
        <GradientStop Color="#FF7b9bc8" Offset="0" />
        <GradientStop Color="#FF5c7eaf" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Background_Header_SelectedInactiveReadOnly}">
        <GradientStop Color="#FFC8DCF3" Offset="0" />
        <GradientStop Color="#FFB7D1EF" Offset="1" />
    </LinearGradientBrush>
    <SolidColorBrush Color="#ffc3cedc" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=BorderBrush}" />
    <SolidColorBrush Color="#FF000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Header_Normal}" />
    <SolidColorBrush Color="#C0000000" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Header_ReadOnly}" />
    <SolidColorBrush Color="#FFFFFFFF" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Header_Selected}" />
    <SolidColorBrush Color="#FFFFFFFF" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Header_SelectedInactive}" />
    <SolidColorBrush Color="#FFFFFFFF" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Header_SelectedReadOnly}" />
    <SolidColorBrush Color="#FFFFFFFF" x:Key="{dxprgt:VSViewBrushThemeKey ResourceKey=Foreground_Header_SelectedInactiveReadOnly}" />

</ResourceDictionary>