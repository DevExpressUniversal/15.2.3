<p:ResourceDictionary
    xmlns="clr-namespace:DevExpress.Diagram.Core.Shapes"
    xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:diag="clr-namespace:DevExpress.Diagram.Core"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <ShapeTemplate x:Key="{ShapeKey Process}" DefaultSize="100,75" IsQuick="True">
        <Start X="0" Y="0" />
        <Line X="1" Y="0" />
        <Line X="1" Y="1" />
        <Line X="0" Y="1" />
        <Line X="0" Y="0" />
        <ShapeTemplate.ConnectionPoints>
            <ShapePoint X="0.5" Y="0" />
            <ShapePoint X="1" Y="0.5" />
            <ShapePoint X="0.5" Y="1" />
            <ShapePoint X="0" Y="0.5" />
        </ShapeTemplate.ConnectionPoints>
    </ShapeTemplate>
    <ShapeTemplate x:Key="{ShapeKey Decision}" DefaultSize="100,75" Style="{x:Static diag:DefaultDiagramStyleId.Variant3}" IsQuick="True">
        <Start X="0.5" Y="0" />
        <Line X="1" Y="0.5" />
        <Line X="0.5" Y="1" />
        <Line X="0" Y="0.5" />
        <ShapeTemplate.ConnectionPoints>
            <ShapePoint X="0.5" Y="0" />
            <ShapePoint X="1" Y="0.5" />
            <ShapePoint X="0.5" Y="1" />
            <ShapePoint X="0" Y="0.5" />
        </ShapeTemplate.ConnectionPoints>
    </ShapeTemplate>
    <ShapeTemplate x:Key="{ShapeKey Subprocess}" DefaultSize="100,75" IsQuick="True" Columns="P0W / 4;*;P0W / 4" EditorBounds="CreateRect(P0W / 4, 0, W - P0W / 2, H)" Style="{x:Static diag:DefaultDiagramStyleId.Variant2}">
        <Start X="0" Y="0" />
        <Line X="3" Y="0" />
        <Line X="3" Y="1" />
        <Line X="0" Y="1" />
        <Line X="0" Y="0" />

        <Start X="1" Y="0" Kind="None" />
        <Line X="1" Y="1" />

        <Start X="2" Y="0" Kind="None" />
        <Line X="2" Y="1" />
        <ShapeTemplate.ConnectionPoints>
            <ShapePoint X="1.5" Y="0" />
            <ShapePoint X="3" Y="0.5" />
            <ShapePoint X="1.5" Y="1" />
            <ShapePoint X="0" Y="0.5" />
        </ShapeTemplate.ConnectionPoints>
        <ShapeTemplate.Parameters>
            <Parameter DefaultValue="0.5" 
                       Point="CreatePoint(P * W / 4, H)" 
                       Value="P.X / W * 4" 
                       Min="0" Max="1" />
        </ShapeTemplate.Parameters>
    </ShapeTemplate>
    <ShapeReference x:Key="{ShapeKey StartEnd}" DefaultSize="100,37.5" HasParameters="False" IsQuick="True"/>
    <ShapeTemplate x:Key="{ShapeKey Document}" DefaultSize="100,75" Columns="*" Rows="*" Style="{x:Static diag:DefaultDiagramStyleId.Variant2}" IsQuick="True">
        <Start X="0" Y="0" />
        <Line X="1" Y="0" />
        <Line X="1" Y="0.85" />
        <Arc X="0.5" Y="0.85" Direction="Counterclockwise" Size="CreateSize(W / 2, H)"/>
        <Arc X="0" Y="0.85" Direction="Clockwise" Size="CreateSize(W / 2, H)"/>
        <ShapeTemplate.ConnectionPoints>
            <ShapePoint X="0.5" Y="0" />
            <ShapePoint X="1" Y="0.5" />
            <ShapePoint X="0.5" Y="0.85" />
            <ShapePoint X="0" Y="0.5" />
        </ShapeTemplate.ConnectionPoints>
    </ShapeTemplate>
    <ShapeReference x:Key="{ShapeKey Data}" DefaultSize="100,75" HasParameters="False" IsQuick="True" />

    <ShapeReference x:Key="{ShapeKey Database}" DefaultSize="100,75" HasParameters="False" />
    <ShapeReference x:Key="{ShapeKey ExternalData}" DefaultSize="100,75" HasParameters="False" />
    <ShapeReference x:Key="{ShapeKey Custom1}" DefaultSize="100,75" HasParameters="False" />
    <ShapeReference x:Key="{ShapeKey Custom2}" DefaultSize="100,75" HasParameters="False" />
    <ShapeReference x:Key="{ShapeKey Custom3}" DefaultSize="100,75" HasParameters="False" />
    <ShapeReference x:Key="{ShapeKey Custom4}" DefaultSize="100,75" HasParameters="False" />
    <ShapeTemplate x:Key="{ShapeKey OnPageReference}" DefaultSize="37.5,37.5" Columns="*" Rows="*" Style="{x:Static diag:DefaultDiagramStyleId.Variant4}">
        <Start X="0" Y="0.5" />
        <Arc X="0.5" Y="0" Direction="Clockwise" />
        <Arc X="1" Y="0.5" Direction="Clockwise" />
        <Arc X="0.5" Y="1" Direction="Clockwise" />
        <Arc X="0" Y="0.5" Direction="Clockwise" />
        <ShapeTemplate.ConnectionPoints>
            <ShapePoint X="0.5" Y="0" />
            <ShapePoint X="1" Y="0.5" />
            <ShapePoint X="0.5" Y="1" />
            <ShapePoint X="0" Y="0.5" />
        </ShapeTemplate.ConnectionPoints>
    </ShapeTemplate>
    <ShapeTemplate x:Key="{ShapeKey OffPageReference}" DefaultSize="50,50" Columns="*" Rows="*" Style="{x:Static diag:DefaultDiagramStyleId.Variant4}">
        <Start X="0" Y="0" />
        <Line X="1" Y="0" />
        <Line X="1" Y="0.5" />
        <Line X="0.5" Y="1" />
        <Line X="0" Y="0.5" />
        <ShapeTemplate.ConnectionPoints>
            <ShapePoint X="0.5" Y="0" />
            <ShapePoint X="1" Y="0.5" />
            <ShapePoint X="0.5" Y="1" />
            <ShapePoint X="0" Y="0.5" />
        </ShapeTemplate.ConnectionPoints>
    </ShapeTemplate>
</p:ResourceDictionary>