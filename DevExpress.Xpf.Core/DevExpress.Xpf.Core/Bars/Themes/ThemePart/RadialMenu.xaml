<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxbt="clr-namespace:DevExpress.Xpf.Bars.Themes">

    <SolidColorBrush x:Key="RadialMenuLevelControl_BackColor" Color="#FF6886B1" />
    <SolidColorBrush x:Key="RadialMenuLevelControl_InnerColor" Color="#FFFFFFFF" />
    
    <SolidColorBrush x:Key="CenterButton_StrokeColor" Color="#FF6886B1" />
    <SolidColorBrush x:Key="CenterButton_InnerColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="CenterButton_HoverColor" Color="#3F365D90" />

    <SolidColorBrush x:Key="BarItemInRadialMenu_MenuColor" Color="#FF365D90" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathHover" Color="#FF042B5E" /> 
    
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=NormalImageColorizeColor}">#FF000000</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=HoverImageColorizeColor}">#FF000000</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=PressedImageColorizeColor}">#FF000000</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=DisabledImageColorizeColor}">#3F000000</Color>

</ResourceDictionary>