<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxtEx="clr-namespace:DevExpress.Xpf.Utils.Themes" 
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:dx="clr-namespace:DevExpress.Xpf.Core" 
    xmlns:dxc="clr-namespace:DevExpress.Xpf.Core"
    xmlns:dxt="clr-namespace:DevExpress.Xpf.Utils.Themes" 
    xmlns:dxb="clr-namespace:DevExpress.Xpf.Bars" 
    xmlns:dxbt="clr-namespace:DevExpress.Xpf.Bars.Themes" 
    xmlns:dxbc="clr-namespace:DevExpress.Xpf.Bars.Customization"
    >
    <ResourceDictionary.MergedDictionaries>
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/TextSplitterControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/BarContainerControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/BarControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/BarItems.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/BarItemLinkMenuHeader.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/GlyphSideControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/Customization.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/FloatingBarPopup.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/SubMenuBarControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/PopupControlContainer.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/DeepBlue/Gallery.xaml}" />
        
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/TextSplitterControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/BarContainerControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/BarControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/BarItems.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/BarItemLinkMenuHeader.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/Customization.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/FloatingBarPopup.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/SubMenuBarControl.xaml}" />
        <!--BEGIN WPF-->
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/BarItemsInRadialMenu.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/RadialMenuControl.xaml}" />
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/RadialMenuLevelControl.xaml}" />
        <!--END WPF-->
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/Gallery.xaml}" />
        
        <dxtEx:ResourceDictionaryEx Source="{dx:BarsResource Themes/Generic/DefaultStyles.xaml}" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource SubMenuBarControlDefaultStyle}" TargetType="{x:Type dxb:SubMenuBarControl}" />
    <Style BasedOn="{StaticResource SubMenuBarControlDefaultStyle}" TargetType="{x:Type dxb:PopupMenuBarControl}" />
    <!--BEGIN WPF-->
    <Style BasedOn="{StaticResource RadialMenuControlDefaultStyle}" TargetType="{x:Type dxb:RadialMenuControl}" />
    <Style BasedOn="{StaticResource RadialMenuLevelControlDefaultStyle}" TargetType="{x:Type dxb:RadialMenuLevelControl}" />
    <!--END WPF-->    
    <Style BasedOn="{StaticResource MenuBarControlDefaultStyle}" TargetType="{x:Type dxb:MenuBarControl}" />
    <Style BasedOn="{StaticResource BarControlDefaultStyle}" TargetType="{x:Type dxb:BarControl}" />
    <Style BasedOn="{StaticResource BarContainerControlDefaultStyle}" TargetType="{x:Type dxb:BarContainerControl}" />
    <Style BasedOn="{StaticResource FloatingBarPopupDefaultStyle}" TargetType="{x:Type dxb:FloatingBarPopup}" />
    <Style BasedOn="{StaticResource FloatingBarPopupContentControlDefaultStyle}" TargetType="{x:Type dxb:FloatingBarPopupContentControl}" />
    <Style BasedOn="{StaticResource ItemBorderControlDefaultStyle}" TargetType="{x:Type dxb:ItemBorderControl}" />
    <Style BasedOn="{StaticResource BarButtonItemLinkControlDefaultStyle}" TargetType="{x:Type dxb:BarButtonItemLinkControl}" />
    <Style BasedOn="{StaticResource BarSubItemLinkControlDefaultStyle}" TargetType="{x:Type dxb:BarSubItemLinkControl}" />
    <Style BasedOn="{StaticResource BarSplitButtonItemLinkControlDefaultStyle}" TargetType="{x:Type dxb:BarSplitButtonItemLinkControl}" />
    <Style BasedOn="{StaticResource BarCheckItemLinkControlDefaultStyle}" TargetType="{x:Type dxb:BarCheckItemLinkControl}" />
    <Style BasedOn="{StaticResource BarEditItemLinkControlDefaultStyle}" TargetType="{x:Type dxb:BarEditItemLinkControl}" />
    <Style BasedOn="{StaticResource BarStaticItemLinkControlDefaultStyle}" TargetType="{x:Type dxb:BarStaticItemLinkControl}" />
    <Style BasedOn="{StaticResource BarItemLinkSeparatorControlDefaultStyle}" TargetType="{x:Type dxb:BarItemLinkSeparatorControl}" />
    <Style BasedOn="{StaticResource BarItemLinkMenuHeaderControlDefaultStyle}" TargetType="{x:Type dxb:BarItemLinkMenuHeaderControl}" />
    <Style BasedOn="{StaticResource BarSplitCheckItemLinkControlDefaultStyle}" TargetType="{x:Type dxb:BarSplitCheckItemLinkControl}" />
    <Style BasedOn="{StaticResource RadialContextMenuDefaultStyle}" TargetType="{x:Type dxb:RadialContextMenu}">
        <Setter Property="TextElement.Foreground" Value="#FF000000" />
        <Setter Property="TextElement.FontSize" Value="12" />
        <Setter Property="TextElement.FontWeight" Value="Normal" />
        <Setter Property="TextElement.FontStyle" Value="Normal" />
        <Setter Property="TextElement.FontStretch" Value="Normal" />
        <Setter Property="TextBlock.FontFamily" Value="Segoe UI" />
    </Style>
    <Style BasedOn="{StaticResource BarItemDragElementContentDefaultStyle}" TargetType="{x:Type dxbc:BarItemDragElementContent}" />
    <Style BasedOn="{StaticResource BarDragElementPopupDefaultStyle}" TargetType="{x:Type dxbc:BarDragElementPopup}" />
    <Style BasedOn="{StaticResource BarPopupBaseDefaultStyle}" TargetType="{x:Type dxb:BarPopupBase}" />
    <Style BasedOn="{StaticResource PopupContainerDefaultStyle}" TargetType="{x:Type dxb:PopupContainer}" />
    <Style BasedOn="{StaticResource BarPopupBorderControlDefaultStyle}" TargetType="{x:Type dxb:BarPopupBorderControl}" />
    <!--BEGIN WPF-->
    <Style BasedOn="{StaticResource RadialMenuPopupBorderControlDefaultStyle}" TargetType="{x:Type dxb:RadialMenuPopupBorderControl}" />
    <Style BasedOn="{StaticResource OfficeTabletItemsControlDefaultStyle}" TargetType="{x:Type dxb:OfficeTabletItemsControl}" />
    <Style BasedOn="{StaticResource BarItemListDefaultStyle}" TargetType="{x:Type dxbc:BarItemList}" />
    <Style BasedOn="{StaticResource BarItemListItemDefaultStyle}" TargetType="{x:Type dxbc:BarItemListItem}" />
    <!--END WPF-->
    <Style BasedOn="{StaticResource GalleryControlDefaultStyle}" TargetType="{x:Type dxb:GalleryControl}" />
    <Style BasedOn="{StaticResource GalleryItemControlDefaultStyle}" TargetType="{x:Type dxb:GalleryItemControl}" />
    <Style BasedOn="{StaticResource GalleryItemGroupControlDefaultStyle}" TargetType="{x:Type dxb:GalleryItemGroupControl}" />
    <Style BasedOn="{StaticResource GalleryItemGroupsControlDefaultStyle}" TargetType="{x:Type dxb:GalleryItemGroupsControl}" />
    <Style BasedOn="{StaticResource GalleryItemHoverToolTipDefaultStyle}" TargetType="{x:Type dxb:GalleryItemHoverToolTip}" />
    <!--BEGIN WPF-->
    <Style BasedOn="{StaticResource GalleryDefaultStyle}" TargetType="{x:Type dxb:Gallery}" />
    <!--END WPF-->
    <Style BasedOn="{StaticResource TextSplitterControlDefaultStyle}" TargetType="{x:Type dxb:TextSplitterControl}" />
    <Style BasedOn="{StaticResource GlyphSideControlDefaultStyle}" TargetType="{x:Type dxb:GlyphSideControl}" />
    <Style BasedOn="{StaticResource PopupMenuColumnSplitterDefaultStyle}" TargetType="{x:Type dxb:PopupMenuColumnSplitter}" />
    <Style BasedOn="{StaticResource BarManagerThemeDependentValuesProviderDefaultStyle}" TargetType="{x:Type dxb:BarManagerThemeDependentValuesProvider}"/>
    
</ResourceDictionary>