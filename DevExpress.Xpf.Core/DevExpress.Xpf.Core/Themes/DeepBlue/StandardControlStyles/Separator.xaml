<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:dx="clr-namespace:DevExpress.Xpf.Core"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxtEx="clr-namespace:DevExpress.Xpf.Utils.Themes"
    xmlns:dxt="clr-namespace:DevExpress.Xpf.Utils.Themes">

    <ControlTemplate x:Key="{dxt:SeparatorThemeKey ResourceKey=ControlTemplate}" TargetType="{x:Type Separator}">
        <StackPanel SnapsToDevicePixels="True">
            <Border x:Name="separator" Height="1" Background="#FF8AACDC" />
            <Border x:Name="separator2" Height="1" Opacity="0.25" Background="#FFFFFFFF" />
        </StackPanel>
    </ControlTemplate>

    
</ResourceDictionary>