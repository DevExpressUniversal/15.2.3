<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxtEx="clr-namespace:DevExpress.Xpf.Utils.Themes"
                    xmlns:dx="clr-namespace:DevExpress.Xpf.Core"
                    xmlns:dxt="clr-namespace:DevExpress.Xpf.Utils.Themes">
    
    <Style x:Key="{dxt:SuperTipThemeKey ResourceKey=ItemContentStyle}">
        
    </Style>
    
    <Style x:Key="{dxt:SuperTipThemeKey ResourceKey=HeaderContentStyle}">
        
    </Style>

    <Style x:Key="{dxt:SuperTipThemeKey ResourceKey=SeparatorContentStyle}">

    </Style>

    <Style x:Key="{dxt:SuperTipThemeKey ResourceKey=ItemLayoutStyle}">

    </Style>

    <Style x:Key="{dxt:SuperTipThemeKey ResourceKey=HeaderLayoutStyle}">

    </Style>
    
    <DataTemplate x:Key="{dxt:SuperTipThemeKey ResourceKey=ItemContentTemplate}">
        <TextBlock Text="{Binding}" Foreground="Black" />
    </DataTemplate>

    <DataTemplate x:Key="{dxt:SuperTipThemeKey ResourceKey=HeaderContentTemplate}">
        <TextBlock Text="{Binding}" FontWeight="Bold" Foreground="Black" />
    </DataTemplate>

    <DataTemplate x:Key="{dxt:SuperTipThemeKey ResourceKey=SeparatorContentTemplate}">
        <Border Background="Black" Height="1" />
    </DataTemplate>
    
    <ControlTemplate x:Key="{dxt:SuperTipThemeKey ResourceKey=BorderTemplate}">
        <ContentPresenter />
    </ControlTemplate>

</ResourceDictionary>