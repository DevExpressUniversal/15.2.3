<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:dxt="clr-namespace:DevExpress.Xpf.Core.ConditionalFormatting.Themes"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="{dxt:FormatThemeKey ResourceKey=FormatRedTextBrush}">#FF9C0006</SolidColorBrush>
    <SolidColorBrush x:Key="{dxt:FormatThemeKey ResourceKey=FormatRedFillBrush}">#FFFFC7CE</SolidColorBrush>
    <SolidColorBrush x:Key="{dxt:FormatThemeKey ResourceKey=FormatYellowTextBrush}">#FF9C6500</SolidColorBrush>
    <SolidColorBrush x:Key="{dxt:FormatThemeKey ResourceKey=FormatYellowFillBrush}">#FFFFEB9C</SolidColorBrush>
    <SolidColorBrush x:Key="{dxt:FormatThemeKey ResourceKey=FormatGreenTextBrush}">#FF006100</SolidColorBrush>
    <SolidColorBrush x:Key="{dxt:FormatThemeKey ResourceKey=FormatGreenFillBrush}">#FFC6EFCE</SolidColorBrush>

    <Color x:Key="{dxt:FormatThemeKey ResourceKey=ColorScaleRed}">#FFF8696B</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=ColorScaleYellow}">#FFFFEB84</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=ColorScaleGreen}">#FF63BE7B</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=ColorScaleBlue}">#FF5A8AC6</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=ColorScaleWhite}">White</Color>

    <Color x:Key="{dxt:FormatThemeKey ResourceKey=DataBarNegativeColor}">#FFFF555A</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=DataBarBlueColor}">#FF638EC6</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=DataBarGreenColor}">#FF63C384</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=DataBarRedColor}">#FFFF555A</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=DataBarOrangeColor}">#FFFFB628</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=DataBarLightBlueColor}">#FF008AEF</Color>
    <Color x:Key="{dxt:FormatThemeKey ResourceKey=DataBarPurpleColor}">#FFD6007B</Color>

    <LinearGradientBrush x:Key="{dxt:FormatThemeKey ResourceKey=DataBarNegativeBrush_Gradient}" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#FFFFD8D8" Offset="0" />
        <GradientStop Color="{StaticResource {dxt:FormatThemeKey ResourceKey=DataBarNegativeColor}}" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:FormatThemeKey ResourceKey=DataBarBlueBrush_Gradient}" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="{StaticResource {dxt:FormatThemeKey ResourceKey=DataBarBlueColor}}" Offset="0" />
        <GradientStop Color="#FFE6EDF5"  Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:FormatThemeKey ResourceKey=DataBarGreenBrush_Gradient}" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="{StaticResource {dxt:FormatThemeKey ResourceKey=DataBarGreenColor}}" Offset="0" />
        <GradientStop Color="#FFE6F5EB" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:FormatThemeKey ResourceKey=DataBarRedBrush_Gradient}" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="{StaticResource {dxt:FormatThemeKey ResourceKey=DataBarRedColor}}" Offset="0" />
        <GradientStop Color="#FFFFE4E5" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:FormatThemeKey ResourceKey=DataBarOrangeBrush_Gradient}" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="{StaticResource {dxt:FormatThemeKey ResourceKey=DataBarOrangeColor}}" Offset="0" />
        <GradientStop Color="#FFFFF3DD" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:FormatThemeKey ResourceKey=DataBarLightBlueBrush_Gradient}" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="{StaticResource {dxt:FormatThemeKey ResourceKey=DataBarLightBlueColor}}" Offset="0" />
        <GradientStop Color="#FFD7ECFB" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:FormatThemeKey ResourceKey=DataBarPurpleBrush_Gradient}" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="{StaticResource {dxt:FormatThemeKey ResourceKey=DataBarPurpleColor}}" Offset="0" />
        <GradientStop Color="#FFF7D7EA" Offset="1" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="{dxt:FormatThemeKey ResourceKey=DataBarZeroLineColor}" Color="Black"/>
</ResourceDictionary>