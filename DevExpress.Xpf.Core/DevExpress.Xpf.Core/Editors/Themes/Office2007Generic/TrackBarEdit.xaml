<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxtEx="clr-namespace:DevExpress.Xpf.Utils.Themes"
	xmlns:dxc="clr-namespace:DevExpress.Xpf.Core"
	xmlns:dx="clr-namespace:DevExpress.Xpf.Core"
	xmlns:dxe="clr-namespace:DevExpress.Xpf.Editors"
	xmlns:dxet="clr-namespace:DevExpress.Xpf.Editors.Themes"
	xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--BEGIN WPF-->
    <ResourceDictionary.MergedDictionaries>
        <dxtEx:ResourceDictionaryEx Source="{dxc:EditorsResource Themes/Generic/TrackBarEdit.xaml}" />
    </ResourceDictionary.MergedDictionaries>
    <!--END WPF-->
    <Color x:Key="BaseColor#D0FFC13C">#D0FFC13C</Color>
    <Color x:Key="BaseColor#00FFC13C">#00FFC13C</Color>
    <sys:Double x:Key="{dxet:TrackBarEditThemeKey ResourceKey=LeftStepButtonSize}">16</sys:Double>
    <sys:Double x:Key="{dxet:TrackBarEditThemeKey ResourceKey=RightStepButtonSize}">16</sys:Double>
    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=HorizontalSelectionRangeBackgroundTemplate}" TargetType="ContentControl">
        <Rectangle Height="9" Margin="-5,0,-5,1">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="{DynamicResource BaseColor#00FFC13C}" Offset="0" />
                    <GradientStop Color="{DynamicResource BaseColor#00FFC13C}" Offset="1" />
                    <GradientStop Color="{DynamicResource BaseColor#D0FFC13C}" Offset="0.5" />
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=VerticalSelectionRangeBackgroundTemplate}" TargetType="ContentControl">
        <Rectangle x:Name="PART_SelectionRange" Margin="0,-5,1,-5" Width="9">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                    <GradientStop Color="{DynamicResource BaseColor#00FFC13C}" Offset="0" />
                    <GradientStop Color="{DynamicResource BaseColor#00FFC13C}" Offset="1" />
                    <GradientStop Color="{DynamicResource BaseColor#D0FFC13C}" Offset="0.5" />
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
    </ControlTemplate>
</ResourceDictionary>