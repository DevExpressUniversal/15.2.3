<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxtEx="clr-namespace:DevExpress.Xpf.Utils.Themes"
    xmlns:dx="clr-namespace:DevExpress.Xpf.Core"
    xmlns:dxe="clr-namespace:DevExpress.Xpf.Editors"
    xmlns:dxedp="clr-namespace:DevExpress.Xpf.Editors.DataPager"
    xmlns:dxei="clr-namespace:DevExpress.Xpf.Editors.Internal">

    <Style x:Key="ButtonEditButtonGlyphStyle" TargetType="FrameworkElement">
        <Setter Property="FlowDirection" Value="LeftToRight"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>
    <dx:ColorToBrushConverter x:Key="ColorToBrushConverter" />
    <dxe:ChipSizeToSizeConverter x:Key="ChipSizeToSizeConverter" />
    <dxedp:DataPagerBoolToVisibilityInvertConverter x:Key="DataPagerBoolToVisibilityInvertConverter"/>
    <dxe:HideBorderSideToThicknessConverter x:Key="HideBorderSideToThicknessConverter" />
    <dxei:TrackBarEditFocusElementVisibilityConverter x:Key="TrackBarEditFocusElementVisibilityConverter"/>
    <dx:EditorStringIdConverter x:Key="EditorStringIdConverter"/>

</ResourceDictionary>
