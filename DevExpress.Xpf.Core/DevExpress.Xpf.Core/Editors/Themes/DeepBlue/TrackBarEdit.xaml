            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="IsDefault" Margin="0,0,-1,-1">
                <Ellipse HorizontalAlignment="Stretch" Margin="1,1,0,0" VerticalAlignment="Stretch" Width="Auto" Height="Auto" Fill="#26000000" StrokeThickness="0" SnapsToDevicePixels="True" />
