                            <GradientStop Color="#FF3D66A5" Offset="1" />
                        </LinearGradientBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <Ellipse HorizontalAlignment="Stretch" Margin="2" VerticalAlignment="Stretch" Height="Auto" Width="Auto" Stroke="{x:Null}" SnapsToDevicePixels="True" StrokeThickness="0">
                    <Ellipse.Fill>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFD8E6F6" Offset="0" />
                            <GradientStop Color="#FFB4D0FC" Offset="1" />
                            <GradientStop Color="#FFB2CAEE" Offset="0.5" />
                            <GradientStop Color="#FF8EB5F2" Offset="0.5" />
                        </LinearGradientBrush>
                    </Ellipse.Fill>
                </Ellipse>
            </Grid>
            <Grid x:Name="IsMouseOver" Margin="0,0,-1,-1" Opacity="0">
                <Ellipse HorizontalAlignment="Stretch" Margin="1,1,0,0" VerticalAlignment="Stretch" Width="Auto" Height="Auto" Fill="#26000000" StrokeThickness="0" SnapsToDevicePixels="True" />
                <Ellipse HorizontalAlignment="Stretch" Margin="1" VerticalAlignment="Stretch" Height="Auto" Width="Auto" Stroke="{x:Null}" SnapsToDevicePixels="True" StrokeThickness="0">
                    <Ellipse.Fill>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FF03235F" Offset="0.009" />
                            <GradientStop Color="#FF3D66A5" Offset="1" />
                        </LinearGradientBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <Ellipse HorizontalAlignment="Stretch" Margin="2" VerticalAlignment="Stretch" Height="Auto" Width="Auto" Stroke="{x:Null}" SnapsToDevicePixels="True" StrokeThickness="0">
                    <Ellipse.Fill>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFEAF1FF" Offset="0" />
                            <GradientStop Color="#FFDAE9FF" Offset="1" />
                            <GradientStop Color="#FFDAE7FC" Offset="0.5" />
                            <GradientStop Color="#FFB8D3FD" Offset="0.5" />
                        </LinearGradientBrush>
                    </Ellipse.Fill>
                </Ellipse>
            </Grid>
            <Grid x:Name="IsPressed" Margin="0,0,-1,-1" Opacity="0">
                <Ellipse HorizontalAlignment="Stretch" Margin="1,1,0,0" VerticalAlignment="Stretch" Width="Auto" Height="Auto" Fill="#26000000" StrokeThickness="0" SnapsToDevicePixels="True" />
                <Ellipse HorizontalAlignment="Stretch" Margin="1" VerticalAlignment="Stretch" Height="Auto" Width="Auto" Stroke="{x:Null}" SnapsToDevicePixels="True" StrokeThickness="0">
                    <Ellipse.Fill>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FF03235F" Offset="0.009" />
                            <GradientStop Color="#FF3D66A5" Offset="1" />
                        </LinearGradientBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <Ellipse HorizontalAlignment="Stretch" Margin="2" VerticalAlignment="Stretch" Height="Auto" Width="Auto" Stroke="{x:Null}" SnapsToDevicePixels="True" StrokeThickness="0">
                    <Ellipse.Fill>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FF35558E" Offset="0" />
                            <GradientStop Color="#FF90ADD9" Offset="1" />
                        </LinearGradientBrush>
                    </Ellipse.Fill>
                </Ellipse>
            </Grid>
            <Viewbox HorizontalAlignment="Center" Margin="1,1,0,0" VerticalAlignment="Center" Width="9" Height="3">
                <Canvas Width="9" Height="3">
                    <Rectangle Width="9" Height="3" Canvas.Left="0" Canvas.Top="0" Stretch="Fill" Fill="#BF2D4D87" />
                    <Rectangle Width="7.00001" Height="1" Canvas.Left="0.999977" Canvas.Top="1" Stretch="Fill" Fill="#FFFFFFFF" />
                </Canvas>
            </Viewbox>
        </Grid>
    </ControlTemplate>
