        <GradientStop Color="#FF89B7EE" Offset="0" />
        <GradientStop Color="#FFDCEAFF" Offset="1" />
        <GradientStop Color="#FFB8D3FD" Offset="0.529" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="HorizontalThumbBorder1HotBrush" EndPoint="0.5,-0.002" StartPoint="0.5,1.002">
