    <ControlTemplate x:Key="{dxet:TrackBarEditThumbThemeKey ResourceKey=HorizontalThumbTemplate}" TargetType="{x:Type Thumb}">
        <!--<ControlTemplate.Resources>
			<Storyboard x:Key="Storyboard1"/>
		</ControlTemplate.Resources>-->
        <Border x:Name="Root" Width="11" Height="22" Padding="1,0,1,1" Background="#26000000" CornerRadius="3,3,3,3" Margin="0,0,0,0">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="Background" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource HorizontalThumbBorderHotBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border1" Storyboard.TargetProperty="Background" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource HorizontalThumbBorder1HotBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="Background" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource HorizontalThumbBorderHotBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border1" Storyboard.TargetProperty="Background" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource HorizontalThumbBorder1HotBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" To="0.5" />
                        </Storyboard>
					</VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Width="Auto" Height="Auto" Background="{DynamicResource {dxt:BrushesThemeKey ResourceKey=EditorButtonBorderGradient}}" CornerRadius="2,2,2,2" Padding="1,1,1,1">
                <Grid Width="Auto" Height="Auto" Background="{x:Null}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.5*" />
                        <RowDefinition Height="0.5*" />
                    </Grid.RowDefinitions>
                    <Border Grid.RowSpan="2" CornerRadius="1,1,1,1" x:Name="border">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF578BC8" Offset="0" />
                                <GradientStop Color="#FFBDD7FF" Offset="1" />
                                <GradientStop Color="#FF88B1F0" Offset="0.529" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                    <Border Grid.RowSpan="1" Background="{DynamicResource {dxt:BrushesThemeKey ResourceKey=ButtonNormalGlare}}" CornerRadius="1,1,1,1" x:Name="border1" />
                </Grid>
            </Border>
        </Border>
    </ControlTemplate>
