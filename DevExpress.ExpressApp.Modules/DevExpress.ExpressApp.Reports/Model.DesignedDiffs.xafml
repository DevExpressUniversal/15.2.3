<?xml version="1.0" encoding="utf-8"?>
<Application>
  <ActionDesign>
    <Actions>
      <Action Id="ShowInReport" PaintStyle="Caption"/>
    </Actions>
  </ActionDesign>
  <SchemaModules>
    <SchemaModule Name="SystemModule" Version="13.1.6.0" IsNewNode="True" />
  </SchemaModules>
  <Views>
    <DetailView Id="ReportData_DetailView">
      <Layout>
        <LayoutGroup Id="Main">
          <LayoutGroup Id="SimpleEditors">
            <LayoutGroup Id="ReportData">
              <LayoutItem Id="ReportName" Index="0" ViewItem="ReportName" />
              <LayoutItem Id="DataTypeCaption" Index="1" ViewItem="DataTypeCaption" />
              <LayoutItem Id="IsInplaceReport" Index="2" ViewItem="IsInplaceReport" />
            </LayoutGroup>
            <LayoutGroup Id="XPObject" Removed="True" />
          </LayoutGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <ListView Id="ReportData_ListView" Caption="Reports" />
  </Views>
</Application>
