    <Style x:Key="ContextMenuBaseItem" TargetType="{x:Type dxb:BarItem}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Border Padding="0,5" MinWidth="150" Margin="0,0,15,0">
                        <ContentPresenter Content="{Binding}" />
                    </Border>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ContextMenuSubMenuItem" TargetType="{x:Type dxb:BarItem}" BasedOn="{StaticResource ContextMenuBaseItem}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Border Padding="0,5" Margin="0,0,15,0">
                        <ContentPresenter Content="{Binding}" />
                    </Border>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ContextMenuEditItem" TargetType="{x:Type dxb:BarEditItem}" BasedOn="{StaticResource ContextMenuBaseItem}">
        <Setter Property="EditWidth" Value="170" />
        <Setter Property="EditHorizontalAlignment" Value="Right" />
    </Style>
    <Style x:Key="ContextMenuLinkItem" TargetType="{x:Type dxb:BarButtonItem}" BasedOn="{StaticResource ContextMenuBaseItem}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <DataTemplate.Resources>
                        <Style x:Key="LinkStyle" TargetType="TextBlock">
                            <Setter Property="Foreground" Value="Blue" />
                            <Setter Property="Cursor" Value="Hand" />
                            <Setter Property="Padding" Value="0,5" />
                        </Style>
                    </DataTemplate.Resources>
                    <DataTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="TextBlock.TextDecorations" Value="Underline" TargetName="text" />
                        </Trigger>
                    </DataTemplate.Triggers>
                    <TextBlock x:Name="text" Text="{TemplateBinding Content}" Style="{StaticResource LinkStyle}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
