    <Style x:Key="{x:Static xrdiagram:XRDiagramControlBarItemNames.FormattingRules}" TargetType="dxb:BarButtonItem" BasedOn="{StaticResource ContextMenuLinkItem}">
        <Setter Property="dx:XamlHelper.Name" Value="{x:Static xrdiagram:XRDiagramControlBarItemNames.FormattingRules}" />
        <Setter Property="Content" Value="Edit Formatting Rules..." />
        <Setter Property="IsEnabled" Value="{Binding Converter={designereditors:SelectionModelToIsEnabledConverter}}" />
        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
            <Setter.Value>
                <DataTemplate>
                    <ContentControl>
                        <dxe:DialogServiceEditorBehavior Commands="{dxe:UICommandContainerGenerator Buttons=OKCancel}" Value="{Binding Diagram.SelectionToolsModel[FormattingRules]}" Content="{Binding Path=Diagram.(designer:ReportDesigner.Document).ReportModel}">
                            <dxe:DialogServiceEditorBehavior.DialogServiceTemplate>
                                <DataTemplate>
                                    <ContentControl>
                                        <dxdiag:CollectionDialogService DialogWindowStartupLocation="CenterOwner">
                                            <dxdiag:CollectionDialogService.DialogStyle>
                                                <Style TargetType="dx:DXDialogWindow" BasedOn="{StaticResource DXDialogWindowDialogStyleBase}">
                                                    <Setter Property="Width" Value="600" />
                                                    <Setter Property="Height" Value="350" />
                                                    <Setter Property="MinWidth" Value="420" />
                                                    <Setter Property="MinHeight" Value="240" />
