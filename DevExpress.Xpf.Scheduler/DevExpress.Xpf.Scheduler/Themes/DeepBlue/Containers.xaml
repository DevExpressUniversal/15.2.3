    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=VerticalContentContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">					
                    <dxschd:RoundedBorderControl BorderBrush="#FF668CC4" RoundBorderThickness="1,0,1,1" CornerRadius="0,0,3,3" UseLayoutRounding="True" SnapsToDevicePixels="True">
                        <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" UseLayoutRounding="True" SnapsToDevicePixels="True" />
                    </dxschd:RoundedBorderControl>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="{dxscht:DayViewThemeKey ResourceKey=AllDayAreaContentContainerStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid>
                        <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" UseLayoutRounding="True" SnapsToDevicePixels="True" />
                        <Border Background="Transparent" BorderBrush="#FF668CC4" IsHitTestVisible="False" BorderThickness="1,0,1,0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekViewGroupByNoneContentContainerStyle}" TargetType="{x:Type ContentControl}" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid>
                        <Border BorderThickness="1,1,1,1" BorderBrush="#FF668CC4" CornerRadius="3,3,3,3" SnapsToDevicePixels="True" UseLayoutRounding="True"/>
                        <dxschd:RoundedBorderControl RoundBorderThickness="0,0,1,1" BorderBrush="#FF668CC4" CornerRadius="3,3,3,3" SnapsToDevicePixels="True" UseLayoutRounding="True">						
                            <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" />
                        </dxschd:RoundedBorderControl>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekViewGroupByResourceContentContainerStyle}" TargetType="{x:Type ContentControl}" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid>
                        <!--<Border BorderThickness="1,1,1,1" BorderBrush="Red" Grid.RowSpan="2" CornerRadius="0,0,3,3" SnapsToDevicePixels="True" UseLayoutRounding="True" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Border>-->
                        <dxschd:RoundedBorderControl  RoundBorderThickness="0,0,1,1" BorderBrush="#FF668CC4" CornerRadius="0,0,3,3" SnapsToDevicePixels="True" UseLayoutRounding="True">
                            <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Margin="0,0,0,1" />
                        </dxschd:RoundedBorderControl>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="{dxscht:WeekViewThemeKey ResourceKey=ScrollBarStyle}" TargetType="{x:Type ScrollBar}">
        <Setter Property="Margin" Value="0,0,1,0"/>
    </Style>

