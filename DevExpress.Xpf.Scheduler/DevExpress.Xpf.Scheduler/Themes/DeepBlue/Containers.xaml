    </Style>

    <Style x:Key="{dxscht:TimelineViewThemeKey ResourceKey=TimelineViewGroupByNoneContainerStyle}" TargetType="{x:Type ContentControl}" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <dxschd:RoundedBorderControl BorderBrush="#FF668CC4" RoundBorderThickness="1,0,1,1" CornerRadius="0,0,3,3" SnapsToDevicePixels="True" UseLayoutRounding="True">
                        <dxschd:RoundedBorderControl.Resources>
                            <dxschd:ResourceNavigatorVisibilityEmptyMarginResolver x:Key="TimelineViewThemeKey_ResourceNavigatorVisibilityMarginResolver"/>
                        </dxschd:RoundedBorderControl.Resources>
                        <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" >
                        </ContentPresenter>
                    </dxschd:RoundedBorderControl>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="{dxscht:TimelineViewThemeKey ResourceKey=TimelineViewGroupByResourceContainerStyle}" TargetType="{x:Type ContentControl}" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <dxschd:RoundedBorderControl RoundBorderThickness="0,0,1,1" BorderBrush="#FF668CC4" CornerRadius="0,0,3,0" SnapsToDevicePixels="True" UseLayoutRounding="True">
                        <dxschd:RoundedBorderControl.Resources>
                            <dxschd:ResourceNavigatorVisibilityMarginResolver x:Key="TimelineViewThemeKey_ResourceNavigatorVisibilityMarginResolver">
                                <dxschd:ResourceNavigatorVisibilityMarginResolver.MarginWhenVisible>
                                    <Thickness>0</Thickness>
                                </dxschd:ResourceNavigatorVisibilityMarginResolver.MarginWhenVisible>
                                <dxschd:ResourceNavigatorVisibilityMarginResolver.MarginWhenInvisible>
                                    <Thickness>0,0,1,0</Thickness>
                                </dxschd:ResourceNavigatorVisibilityMarginResolver.MarginWhenInvisible>
                            </dxschd:ResourceNavigatorVisibilityMarginResolver>
                        </dxschd:RoundedBorderControl.Resources>
                        <ContentPresenter x:Name="ContentPresenterBeforeRoundedBorderControl" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" >
                        </ContentPresenter>
                    </dxschd:RoundedBorderControl>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="{dxscht:TimelineViewThemeKey ResourceKey=GroupByDateContainerStyle}" TargetType="{x:Type ContentControl}" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <dxschd:RoundedBorderControl CornerRadius="3" SnapsToDevicePixels="True" UseLayoutRounding="True">
                        <ContentPresenter />
                    </dxschd:RoundedBorderControl>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="{dxscht:TimelineViewThemeKey ResourceKey=AdornedBorderStyle}" TargetType="{x:Type Border}" >
        <Setter Property="CornerRadius" Value="3"/>
        <Setter Property="BorderBrush" Value="#FF668CC4"/>
        <Setter Property="BorderThickness" Value="1"/>
    </Style>
