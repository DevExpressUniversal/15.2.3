                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_PressedGlareStop2}" Offset="1"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_PressedGlareStop3}" Offset="0.837"/>
                        </LinearGradientBrush>
                        <SolidColorBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=VerticalLineBrush}" Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HeadersVerticalLineBrush}"/>
                    </ContentControl.Resources>
                    <ContentControl.Template>
                        <ControlTemplate TargetType="{x:Type ContentControl}">
                            <dxschd:HeaderBorder x:Name="border1" Grid.RowSpan="2" Background="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=VerticalLineBrush}}" DefaultCornerRadius="3,3,3,3" DefaultPadding="1" SnapsToDevicePixels="True" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" >
                                <Grid Width="Auto" Height="Auto" SnapsToDevicePixels="True" UseLayoutRounding="False">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="0.5*"/>
                                        <RowDefinition Height="0.5*"/>
                                    </Grid.RowDefinitions>
                                    <dxschd:HeaderBorder SnapsToDevicePixels="True" UseLayoutRounding="True" Width="Auto" Height="Auto" Grid.RowSpan="2" DefaultCornerRadius="2,2,2,2" x:Name="border1_1" Background="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalGradient}}" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" />
