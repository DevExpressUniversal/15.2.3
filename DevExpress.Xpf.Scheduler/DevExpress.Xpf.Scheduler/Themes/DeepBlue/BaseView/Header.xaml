                                    <dxschd:HeaderGlareBorder SnapsToDevicePixels="True" UseLayoutRounding="True" Grid.Row="0" DefaultPadding="0,0,0,1" DefaultCornerRadius="2,2,5,5" x:Name="border1_2" Background="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalGlare}}" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" />

                                    <Rectangle x:Name="leftGlare" HorizontalAlignment="{Binding Path=(dxschd:HeaderControl.GlareAlignment), RelativeSource={RelativeSource Mode=TemplatedParent}}" Margin="0,0,0,0" Width="1" Stroke="{x:Null}" StrokeThickness="0" Grid.RowSpan="2" SnapsToDevicePixels="True" >
                                        <Rectangle.Fill>
