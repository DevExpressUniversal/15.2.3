                                                <GradientStop Color="#57CDE2FF" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Rectangle.Fill>
                                    </Rectangle>
                                    <Border Grid.RowSpan="2" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" SnapsToDevicePixels="True" >
                                        <ContentPresenter Margin="4"/>
                                    </Border>
                                </Grid>
                            </dxschd:HeaderBorder>
                        </ControlTemplate>
                    </ContentControl.Template>
                </dx:XPFContentControl>
            </dxschd:SchedulerColorConvertControl>
        </Border>
