<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxsch="clr-namespace:DevExpress.Xpf.Scheduler"
                    xmlns:dxschd="clr-namespace:DevExpress.Xpf.Scheduler.Drawing"
                    xmlns:dxscht="clr-namespace:DevExpress.Xpf.Scheduler.ThemeKeys"
                    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">

    <dxschd:ElementPositionVerticalWeekHeaderConverter x:Key="elementPositionVerticalWeekHeaderConverter" />
    <dxschd:ColorResourcesConverter x:Key="colorResourcesConverter" />


    <!-- Vertical Header-->
    <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=VerticalHeaderNormalTemplate}" TargetType="{x:Type dxschd:VerticalHeaderControl}">
        <Border>
            <dxschd:SchedulerColorConvertControl x:Name="clrConvCtrl" ControlBrush="{TemplateBinding ResourceColor}" DisableResourceColor="{TemplateBinding DisableResourceColor}" SnapsToDevicePixels="True" Visibility="Visible">
                <dxschd:SchedulerColorConvertControl.BaseColors>
                    <dxschd:ColorCollection>
                        <dxschd:ColorDefinition Value="#FF193B74" Name="ColorDefinition_NormalGradientStop1"/>
                        <dxschd:ColorDefinition Value="#FF7499CE" Name="ColorDefinition_NormalGradientStop2"/>
                        <dxschd:ColorDefinition Value="#FF244985" Name="ColorDefinition_NormalGradientStop3"/>
                        <dxschd:ColorDefinition Value="#FF2C5494" Name="ColorDefinition_NormalGlareStop1"/>
                        <dxschd:ColorDefinition Value="#FF6086BE" Name="ColorDefinition_NormalGlareStop2"/>
                        <dxschd:ColorDefinition Value="#FFA2C5F4" Name="ColorDefinition_NormalGlareStop3"/>
                        <dxschd:ColorDefinition Value="#FF3A629F" Name="ColorDefinition_NormalGlareStop4"/>
                        <dxschd:ColorDefinition Value="#FF193B74" Name="ColorDefinition_HotGradientStop1"/>
                        <dxschd:ColorDefinition Value="#FF9AD5F6" Name="ColorDefinition_HotGradientStop2"/>
                        <dxschd:ColorDefinition Value="#FF2C5597" Name="ColorDefinition_HotGradientStop3"/>
                        <dxschd:ColorDefinition Value="#FF3863A7" Name="ColorDefinition_HotGlareStop1"/>
                        <dxschd:ColorDefinition Value="#FF7CABDA" Name="ColorDefinition_HotGlareStop2"/>
                        <dxschd:ColorDefinition Value="#FFC2DBFD" Name="ColorDefinition_HotGlareStop3"/>
                        <dxschd:ColorDefinition Value="#FF4F78B5" Name="ColorDefinition_HotGlareStop4"/>
                        <dxschd:ColorDefinition Value="#FF193B74" Name="ColorDefinition_PressedGradientStop1"/>
                        <dxschd:ColorDefinition Value="#FF2C5597" Name="ColorDefinition_PressedGradientStop2"/>
                        <dxschd:ColorDefinition Value="#FF2C5494" Name="ColorDefinition_PressedGlareStop1"/>
                        <dxschd:ColorDefinition Value="#FF2C4B8E" Name="ColorDefinition_PressedGlareStop2"/>
                        <dxschd:ColorDefinition Value="#FF416CAB" Name="ColorDefinition_PressedGlareStop3"/>
                        <dxschd:ColorDefinition Value="#FF062B63" Name="ColorDefinition_HeadersVerticalLineBrush"/>
                    </dxschd:ColorCollection>
                </dxschd:SchedulerColorConvertControl.BaseColors>

                <dx:XPFContentControl x:Name="HeaderContent" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" SnapsToDevicePixels="True" UseLayoutRounding="True" dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}">
                    <ContentControl.Resources>
                        <LinearGradientBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalGradient}" EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_NormalGradientStop1}" Offset="0"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_NormalGradientStop2}" Offset="1"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_NormalGradientStop3}" Offset="0.529"/>
                        </LinearGradientBrush>
                        <LinearGradientBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalGlare}" EndPoint="-0.002,0.5" StartPoint="1.002,0.5">
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_NormalGlareStop1}" Offset="0"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_NormalGlareStop2}" Offset="0.885"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_NormalGlareStop3}" Offset="1"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_NormalGlareStop4}" Offset="0.317"/>
                        </LinearGradientBrush>
                        <LinearGradientBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderHotGradient}" EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HotGradientStop1}" Offset="0"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HotGradientStop2}" Offset="1"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HotGradientStop3}" Offset="0.529"/>
                        </LinearGradientBrush>
                        <LinearGradientBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderHotGlare}" EndPoint="-0.002,0.5" StartPoint="1.002,0.5">
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HotGlareStop1}" Offset="0"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HotGlareStop2}" Offset="0.885"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HotGlareStop3}" Offset="1"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HotGlareStop4}" Offset="0.317"/>
                        </LinearGradientBrush>
                        <LinearGradientBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderPressedGradient}" EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_PressedGradientStop1}" Offset="0"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_PressedGradientStop2}" Offset="1"/>
                        </LinearGradientBrush>
                        <LinearGradientBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderPressedGlare}" EndPoint="-0.002,0.5" StartPoint="1.002,0.5">
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_PressedGlareStop1}" Offset="0"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_PressedGlareStop2}" Offset="1"/>
                            <GradientStop Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_PressedGlareStop3}" Offset="0.837"/>
                        </LinearGradientBrush>
                        <SolidColorBrush x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=VerticalLineBrush}" Color="{Binding ElementName=clrConvCtrl, Path=Container, Converter={StaticResource colorResourcesConverter}, ConverterParameter=ColorDefinition_HeadersVerticalLineBrush}"/>
                    </ContentControl.Resources>
                    <ContentControl.Template>
                        <ControlTemplate TargetType="{x:Type ContentControl}">
                            <dxschd:HeaderBorder x:Name="border1" Grid.ColumnSpan="2" Background="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=VerticalLineBrush}}" DefaultCornerRadius="3,3,3,3" DefaultPadding="1" SnapsToDevicePixels="True" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" >
                                <Grid Width="Auto" Height="Auto" SnapsToDevicePixels="True" UseLayoutRounding="False">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="0.5*"/>
                                        <ColumnDefinition Width="0.5*"/>
                                    </Grid.ColumnDefinitions>
                                    <dxschd:HeaderBorder SnapsToDevicePixels="True" UseLayoutRounding="True" Width="Auto" Height="Auto" Grid.ColumnSpan="2" DefaultCornerRadius="2,2,2,2" x:Name="border1_1" Background="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalGradient}}" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" />
                                    <dxschd:VerticalHeaderGlareBorder SnapsToDevicePixels="True" UseLayoutRounding="True" Grid.Column="0" DefaultPadding="0,0,0,1" DefaultCornerRadius="2,5,5,2" x:Name="border1_2" Background="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalGlare}}" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" />

                                    <Rectangle x:Name="leftGlare" HorizontalAlignment="Right" Margin="0,0,0,0" Width="1" Stroke="{x:Null}" StrokeThickness="0" Grid.ColumnSpan="2" SnapsToDevicePixels="True" >
                                        <Rectangle.Fill>
                                            <LinearGradientBrush EndPoint="0,0.5" StartPoint="1,0.5" >
                                                <GradientStop Color="#00C0DAFB" Offset="0"/>
                                                <GradientStop Color="#57CDE2FF" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Rectangle.Fill>
                                    </Rectangle>
                                    <Border Grid.ColumnSpan="2" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" SnapsToDevicePixels="True" >
                                        <ContentPresenter Margin="4"/>
                                    </Border>
                                </Grid>
                            </dxschd:HeaderBorder>
                        </ControlTemplate>

                    </ContentControl.Template>
                </dx:XPFContentControl>
            </dxschd:SchedulerColorConvertControl>
        </Border>
    </ControlTemplate>

    <!--Normal Header-->
    <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalTemplate}" TargetType="{x:Type dxschd:HeaderControl}">
        <Border>
            <dxschd:SchedulerColorConvertControl x:Name="clrConvCtrl" ControlBrush="{TemplateBinding ResourceColor}" DisableResourceColor="{TemplateBinding DisableResourceColor}" SnapsToDevicePixels="True" Visibility="Visible">
                <dxschd:SchedulerColorConvertControl.BaseColors>
