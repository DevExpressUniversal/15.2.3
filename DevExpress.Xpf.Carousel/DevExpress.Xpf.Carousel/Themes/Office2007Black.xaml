    <ControlTemplate TargetType="{x:Type ButtonBase}" x:Key="{dxt:CarouselNavigatorThemeKey ResourceKey=ButtonTemplate}">
        <Grid x:Name="grid" Width="Auto" Height="Auto">
            <Border x:Name="border" Width="Auto" Height="Auto" Background="{DynamicResource BaseBrush#FF898785}" Padding="0,1,1,1">
                <Border x:Name="border2" Width="Auto" Height="Auto" Padding="1,1,1,1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="{DynamicResource BaseColor#FFF8FAFB}" Offset="0" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFFFFFF}" Offset="1" />
                            <GradientStop Color="{DynamicResource BaseColor#FFF3F5F7}" Offset="0.5" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="border1" Width="Auto" Height="Auto">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="{DynamicResource BaseColor#FFFEFFFF}" Offset="0" />
                                <GradientStop Color="{DynamicResource BaseColor#FFFAFFFF}" Offset="1" />
                                <GradientStop Color="{DynamicResource BaseColor#FFE0E5EB}" Offset="0.5" />
                                <GradientStop Color="{DynamicResource BaseColor#FFCBD5DF}" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter x:Name="content" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
										Margin="{TemplateBinding Padding}"
										VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
										SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
										RecognizesAccessKey="True" />
                    </Border>
                </Border>
            </Border>
            <Border Margin="0,1,1,1"
							x:Name="border3"
							Opacity="0.25"
							Visibility="Hidden"
							BorderBrush="{DynamicResource BaseBrush#FF41B7F9}"
							BorderThickness="2,2,2,2"
							CornerRadius="0,0,0,0" />
        </Grid>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True" />
                    <Condition Property="ToggleButton.IsChecked" Value="false" />
                </MultiTrigger.Conditions>
                <Setter Property="Background" TargetName="border" Value="{DynamicResource BaseBrush#FFC1A965}" />
                <Setter Property="Background" TargetName="border1">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="{DynamicResource BaseColor#FFFFFBE5}" Offset="0" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFCEBB7}" Offset="1" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFADF8F}" Offset="0.5" />
                            <GradientStop Color="{DynamicResource BaseColor#FFF4C233}" Offset="0.5" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="Background" TargetName="border2">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="{DynamicResource BaseColor#FFFFFEFB}" Offset="0" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFFF5D7}" Offset="1" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFFF6DC}" Offset="0.5" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPressed" Value="True" />
                    <Condition Property="ToggleButton.IsChecked" Value="false" />
                </MultiTrigger.Conditions>
                <Setter Property="Background" TargetName="border2">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="{DynamicResource BaseColor#FFC18C5D}" Offset="0" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFFC14C}" Offset="1" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFFAB49}" Offset="0.5" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFF8F08}" Offset="0.5" />
                            <GradientStop Color="{DynamicResource BaseColor#FFFCB06A}" Offset="0.1" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="Background" TargetName="border1" Value="{x:Null}" />
                <Setter Property="Background" TargetName="border">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="{DynamicResource BaseColor#FF7B6645}" Offset="0" />
                            <GradientStop Color="{DynamicResource BaseColor#FFA78E66}" Offset="1" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsKeyboardFocused" Value="True" />
                    <Condition Property="IsPressed" Value="False" />
                    <Condition Property="IsMouseOver" Value="False" />
                </MultiTrigger.Conditions>
                <Setter Property="Visibility" TargetName="border3" Value="Visible" />
            </MultiTrigger>
            <Trigger Property="ToggleButton.IsChecked" Value="True">
                <Setter Property="Background" TargetName="border2">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="{DynamicResource BaseColor#FFA9B0B7}" Offset="0" />
                            <GradientStop Color="{DynamicResource BaseColor#FFD5DFE4}" Offset="1" />
                            <GradientStop Color="{DynamicResource BaseColor#FFBFC6CA}" Offset="0.5" />
                            <GradientStop Color="{DynamicResource BaseColor#FFB1B6BD}" Offset="0.5" />
                            <GradientStop Color="{DynamicResource BaseColor#FFCFD4DA}" Offset="0.1" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="Background" TargetName="border1" Value="{x:Null}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" TargetName="content" Value="0.35" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
