                        <Path SnapsToDevicePixels="True" Stretch="Fill" Fill="#FF313234" Height="8" Width="9" Margin="0,0,1,0" Data="F1 M 1351,909L 1347,905L 1347,909L 1346,909L 1346,901L 1347,901L 1347,905L 1351,901L 1351,905L 1355,901L 1355,909L 1351,905L 1351,909 Z " HorizontalAlignment="Center" VerticalAlignment="Center" />
