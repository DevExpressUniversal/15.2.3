<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
    xmlns:dxwui="clr-namespace:DevExpress.Xpf.WindowsUI"                    
    xmlns:dxwuit="clr-namespace:DevExpress.Xpf.WindowsUI.ThemeKeys"
    xmlns:dxwuii="clr-namespace:DevExpress.Xpf.WindowsUI.Internal"
    xmlns:dxwuin="clr-namespace:DevExpress.Xpf.WindowsUI.Navigation">
    <Style TargetType="{x:Type dxwui:NavigationFrame}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dxwui:NavigationFrame}">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" Padding="{TemplateBinding Padding}">
                        <dxwuii:ViewPresenter Content="{TemplateBinding Content}" AnimationType="{TemplateBinding AnimationType}" AnimationDirection="{TemplateBinding AnimationDirection}" AnimationSpeedRatio="{TemplateBinding AnimationSpeedRatio}" AnimationSelector="{TemplateBinding AnimationSelector}" AnimationDelay="{TemplateBinding AnimationDelay}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="AllowMerging" Value="{x:Null}" />
    </Style>
</ResourceDictionary>