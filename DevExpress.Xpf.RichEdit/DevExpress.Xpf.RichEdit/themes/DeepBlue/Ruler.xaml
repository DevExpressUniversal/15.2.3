    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=TabTypeToggleBackgroundControlTemplate}}" />
  </Style>
  <!--TabTypeToggleHotZoneControl-->
  <Style TargetType="{x:Type dxrec:TabTypeToggleHotZoneControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=TabTypeToggleHotZoneControlTemplate}}" />
  </Style>
  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=ActiveAreaControlTemplate}" TargetType="dxrec:RulerActiveAreaControl">
    <Border BorderThickness="1,0">
      <Border.BorderBrush>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
          <GradientStop Color="#0045639B" Offset="0" />
          <GradientStop Color="#5945639B" Offset="1" />
        </LinearGradientBrush>
      </Border.BorderBrush>
      <Border.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
          <GradientStop Color="Transparent" Offset="0" />
          <GradientStop Color="#3FFFFFFF" Offset="1" />
        </LinearGradientBrush>
      </Border.Background>
    </Border>
    <!--<Rectangle Fill="White" HorizontalAlignment="Stretch"/>-->
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=HorizontalSpaceAreaControlTemplate}" TargetType="dxrec:HorizontalRulerSpaceAreaControl">
    <!--<Border BorderThickness="1,0">
            <Border.BorderBrush>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#0045639B" Offset="0"/>
                    <GradientStop Color="#5945639B" Offset="1"/>
                </LinearGradientBrush>
            </Border.BorderBrush>
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Transparent" Offset="0"/>
                    <GradientStop Color="#3FFFFFFF" Offset="1"/>
                </LinearGradientBrush>
            </Border.Background>
        </Border>-->
  </ControlTemplate>
  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=VerticalSpaceAreaControlTemplate}" TargetType="dxrec:VerticalRulerSpaceAreaControl">
    <Border BorderThickness="0,1">
      <Border.BorderBrush>
        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
          <GradientStop Color="#0045639B" Offset="0" />
          <GradientStop Color="#5945639B" Offset="1" />
        </LinearGradientBrush>
      </Border.BorderBrush>
      <Border.Background>
        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
          <GradientStop Color="Transparent" Offset="0" />
          <GradientStop Color="#3FFFFFFF" Offset="1" />
        </LinearGradientBrush>
      </Border.Background>
    </Border>
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=TickMarkControlTemplate}" TargetType="dxrec:RulerTickMarkControl">
    <Rectangle Fill="#7F45639B" HorizontalAlignment="Stretch" />
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=DefaultTabControlTemplate}" TargetType="dxrec:DefaultTabControl">
    <Rectangle Fill="#5945639B" HorizontalAlignment="Stretch" />
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=DisabledHotZoneControlTemplate}" TargetType="dxrec:DisabledHotZoneControl">
    <Rectangle Fill="Blue" HorizontalAlignment="Stretch" Width="9" Height="7" />
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=LeftIndentHotZoneControlTemplate}" TargetType="dxrec:HorizontalHotZoneControl">
    <Grid HorizontalAlignment="Left" VerticalAlignment="Top">
      <Grid Height="7" Width="9" Visibility="{TemplateBinding EnabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 547,359L 551,355L 552,355L 556,359L 556,362L 547,362L 547,359 Z " UseLayoutRounding="False" />
        <Path Stretch="Fill" Data="F1 M 548,359L 551,356L 552,356L 555,359L 555,361L 548,361L 548,359 Z " Margin="1" UseLayoutRounding="False">
          <Path.Fill>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
              <GradientStop Color="#FFE6EFFB" Offset="0" />
              <GradientStop Color="#FFBED7FF" Offset="1" />
            </LinearGradientBrush>
          </Path.Fill>
        </Path>
      </Grid>
      <Grid Height="7" Width="9" Opacity="0.35" Visibility="{TemplateBinding DisabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 547,359L 551,355L 552,355L 556,359L 556,362L 547,362L 547,359 Z " UseLayoutRounding="False" />
        <Path Stretch="Fill" Data="F1 M 548,359L 551,356L 552,356L 555,359L 555,361L 548,361L 548,359 Z " Margin="1" UseLayoutRounding="False">
          <Path.Fill>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
              <GradientStop Color="#FFE6EFFB" Offset="0" />
              <GradientStop Color="#FFBED7FF" Offset="1" />
            </LinearGradientBrush>
          </Path.Fill>
        </Path>
      </Grid>
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=FirstLineIndentHotZoneControlTemplate}" TargetType="dxrec:FirstLineIndentHotZoneControl">
    <Grid HorizontalAlignment="Left" VerticalAlignment="Top">
      <Grid Height="7" Width="9" Margin="0,2,0,0" Visibility="{TemplateBinding EnabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 572,353L 568,357L 567,357L 563,353L 563,350L 572,350L 572,353 Z " UseLayoutRounding="False" />
        <Path Stretch="Fill" Data="F1 M 571,353L 568,356L 567,356L 564,353L 564,351L 571,351L 571,353 Z " Margin="1" UseLayoutRounding="False">
          <Path.Fill>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
              <GradientStop Color="#FFE6EFFB" Offset="0" />
              <GradientStop Color="#FFBED7FF" Offset="1" />
            </LinearGradientBrush>
          </Path.Fill>
        </Path>
      </Grid>
      <Grid Height="7" Width="9" Margin="0,2,0,0" Opacity="0.35" Visibility="{TemplateBinding DisabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 572,353L 568,357L 567,357L 563,353L 563,350L 572,350L 572,353 Z " UseLayoutRounding="False" />
        <Path Stretch="Fill" Data="F1 M 571,353L 568,356L 567,356L 564,353L 564,351L 571,351L 571,353 Z " Margin="1" UseLayoutRounding="False">
          <Path.Fill>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
              <GradientStop Color="#FFE6EFFB" Offset="0" />
              <GradientStop Color="#FFBED7FF" Offset="1" />
            </LinearGradientBrush>
          </Path.Fill>
        </Path>
      </Grid>
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=LeftIndentBottomHotZoneControlTemplate}" TargetType="dxrec:LeftIndentBottomHotZoneControl">
    <Grid HorizontalAlignment="Left" VerticalAlignment="Top">
      <Grid Height="6" Width="9" Margin="0,0,0,0" Visibility="{TemplateBinding EnabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 518,356L 527,356L 527,362L 518,362L 518,356 Z " UseLayoutRounding="False" />
        <Path Stretch="Fill" Data="F1 M 519,356L 526,356L 526,361L 519,361L 519,356 Z " Margin="1,0,1,1" UseLayoutRounding="False">
          <Path.Fill>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
              <GradientStop Color="#FFE6EFFB" Offset="0" />
              <GradientStop Color="#FFBED7FF" Offset="1" />
            </LinearGradientBrush>
          </Path.Fill>
        </Path>
      </Grid>
      <Grid Height="6" Width="9" Opacity="0.35" Margin="0,0,0,0" Visibility="{TemplateBinding DisabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 518,356L 527,356L 527,362L 518,362L 518,356 Z " UseLayoutRounding="False" />
        <Path Stretch="Fill" Data="F1 M 519,356L 526,356L 526,361L 519,361L 519,356 Z " Margin="1,0,1,1" UseLayoutRounding="False">
          <Path.Fill>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
              <GradientStop Color="#FFE6EFFB" Offset="0" />
              <GradientStop Color="#FFBED7FF" Offset="1" />
            </LinearGradientBrush>
          </Path.Fill>
        </Path>
      </Grid>
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=RightTabHotZoneControlTemplate}" TargetType="dxrec:RightTabHotZoneControl">
    <Grid HorizontalAlignment="Left" VerticalAlignment="Top">
      <Grid Height="8" Width="8" Margin="0,-1,0,0" Visibility="{TemplateBinding EnabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 511,330L 513,330L 513,336L 508,336L 508,334L 511,334L 511,330 Z " UseLayoutRounding="False" Width="5" Height="6" VerticalAlignment="Bottom" HorizontalAlignment="Right" />
      </Grid>
      <Grid Height="8" Width="8" Margin="0,-1,0,0" Opacity="0.35" Visibility="{TemplateBinding DisabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 511,330L 513,330L 513,336L 508,336L 508,334L 511,334L 511,330 Z " UseLayoutRounding="False" Width="5" Height="6" VerticalAlignment="Bottom" HorizontalAlignment="Right" />
      </Grid>
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=LeftTabHotZoneControlTemplate}" TargetType="dxrec:LeftTabHotZoneControl">
    <Grid>
      <Grid Height="8" Width="8" Margin="0,1,0,0" Visibility="{TemplateBinding EnabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 517,330L 515,330L 515,336L 520,336L 520,334L 517,334L 517,330 Z " UseLayoutRounding="False" HorizontalAlignment="Left" VerticalAlignment="Top" Width="5" Height="6" />
      </Grid>
      <Grid Height="8" Width="8" Margin="0,1,0,0" Opacity="0.35" Visibility="{TemplateBinding DisabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 517,330L 515,330L 515,336L 520,336L 520,334L 517,334L 517,330 Z " UseLayoutRounding="False" HorizontalAlignment="Left" VerticalAlignment="Top" Width="5" Height="6" />
      </Grid>
    </Grid>
  </ControlTemplate>
    <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=HorizontalTableHotZoneControlTemplate}" TargetType="dxrec:HorizontalTableHotZoneControl">
        <Grid>
            <Grid Height="9" Width="7" Visibility="{TemplateBinding EnabledVisibility}" Margin="0,3,0,0">
                <Rectangle Stretch="Fill" Fill="#FFE3EDFB"/>
                <Path Stretch="Fill" Fill="#80174388" Data="F1 M 66,181L 67,181L 67,190L 66,190L 66,181 Z M 68,181L 69,181L 69,190L 68,190L 68,181 Z M 70,181L 71,181L 71,190L 70,190L 70,181 Z " Margin="1,0"/>
                <Path Stretch="Fill" Fill="#80174388" Data="F1 M 65,181L 72,181L 72,182L 65,182L 65,181 Z M 72,183L 72,184L 65,184L 65,183L 72,183 Z M 65,185L 72,185L 72,186L 65,186L 65,185 Z M 65,187L 72,187L 72,188L 65,188L 65,187 Z M 65,189L 72,189L 72,190L 65,190L 65,189 Z "/>
            </Grid>
            <Grid Height="9" Width="7" Visibility="{TemplateBinding DisabledVisibility}" Margin="0,3,0,0">
                <Rectangle Stretch="Fill" Fill="#FFE3EDFB"/>
                <Path Stretch="Fill" Fill="#80174388" Data="F1 M 66,181L 67,181L 67,190L 66,190L 66,181 Z M 68,181L 69,181L 69,190L 68,190L 68,181 Z M 70,181L 71,181L 71,190L 70,190L 70,181 Z " Margin="1,0"/>
                <Path Stretch="Fill" Fill="#80174388" Data="F1 M 65,181L 72,181L 72,182L 65,182L 65,181 Z M 72,183L 72,184L 65,184L 65,183L 72,183 Z M 65,185L 72,185L 72,186L 65,186L 65,185 Z M 65,187L 72,187L 72,188L 65,188L 65,187 Z M 65,189L 72,189L 72,190L 65,190L 65,189 Z "/>
            </Grid>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=CenterTabHotZoneControlTemplate}" TargetType="dxrec:CenterTabHotZoneControl">
    <Grid>
      <Grid Height="8" Width="8" Margin="0,-1,0,0" Visibility="{TemplateBinding EnabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 538,330L 540,330L 540,334L 543,334L 543,336L 540,336L 535,336L 535,334L 538,334L 538,330 Z " UseLayoutRounding="False" Width="8" Height="6" HorizontalAlignment="Center" VerticalAlignment="Bottom" />
      </Grid>
      <Grid Height="8" Width="8" Margin="0,-1,0,0" Opacity="0.35" Visibility="{TemplateBinding DisabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 538,330L 540,330L 540,334L 543,334L 543,336L 540,336L 535,336L 535,334L 538,334L 538,330 Z " UseLayoutRounding="False" Width="8" Height="6" HorizontalAlignment="Center" VerticalAlignment="Bottom" />
      </Grid>
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=DecimalTabHotZoneControlTemplate}" TargetType="dxrec:DecimalTabHotZoneControl">
    <Grid>
      <Grid Height="8" Width="8" Margin="0,-1,0,0" Visibility="{TemplateBinding EnabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 548,330L 550,330L 550,334L 553,334L 553,336L 550,336L 545,336L 545,334L 548,334L 548,330 Z M 551,331L 553,331L 553,333L 551,333L 551,331 Z " UseLayoutRounding="False" Width="8" Height="6" HorizontalAlignment="Center" VerticalAlignment="Bottom" />
      </Grid>
      <Grid Height="8" Width="8" Margin="0,-1,0,0" Opacity="0.35" Visibility="{TemplateBinding DisabledVisibility}">
        <Path Stretch="Fill" Fill="#BF445078" Data="F1 M 548,330L 550,330L 550,334L 553,334L 553,336L 550,336L 545,336L 545,334L 548,334L 548,330 Z M 551,331L 553,331L 553,333L 551,333L 551,331 Z " UseLayoutRounding="False" Width="8" Height="6" HorizontalAlignment="Center" VerticalAlignment="Bottom" />
      </Grid>
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=TabTypeToggleBackgroundControlTemplate}" TargetType="dxrec:TabTypeToggleBackgroundControl">
    <Border BorderThickness="0,0,1,0" BorderBrush="#7F45639B" Margin="-5,0,5,0" />
  </ControlTemplate>
  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=TabTypeToggleHotZoneControlTemplate}" TargetType="dxrec:TabTypeToggleHotZoneControl">
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=HorizontalRulerBackgroundControlTemplate}" TargetType="{x:Type ContentControl}">
    <Border x:Name="ruler_horz" BorderThickness="0,0,0,1" BorderBrush="#7F45639B" Margin="0,-6,0,6">
      <Rectangle Fill="Transparent" />
    </Border>
  </ControlTemplate>

  <ControlTemplate x:Key="{dxret:RichEditRulerThemeKey ResourceKey=VerticalRulerBackgroundControlTemplate}" TargetType="{x:Type ContentControl}">
    <Border x:Name="ruler_vert" BorderThickness="0,0,1,0" BorderBrush="#7F45639B" Margin="-5,0,5,0">
      <Rectangle Fill="Transparent" />
    </Border>
  </ControlTemplate>
  <Style x:Key="{dxret:RichEditRulerThemeKey ResourceKey=HorizontalRulerContentStyle}" TargetType="{x:Type Canvas}">
    <Setter Property="Margin" Value="0,5,0,6" />
  </Style>
  <Style x:Key="{dxret:RichEditRulerThemeKey ResourceKey=VerticalRulerContentStyle}" TargetType="{x:Type Canvas}">
    <Setter Property="Margin" Value="5,0,6,0" />
  </Style>
  <Style x:Key="{dxret:RichEditRulerThemeKey ResourceKey=HorizontalRulerShadowStyle}" TargetType="{x:Type Border}">
    <Setter Property="BorderThickness" Value="0,1,0,0" />
    <Setter Property="BorderBrush" Value="#7F45639B" />
    <Setter Property="Height" Value="6" />
    <Setter Property="Margin" Value="0,-3,0,0" />
  </Style>
  <Style x:Key="{dxret:RichEditRulerThemeKey ResourceKey=VerticalRulerShadowStyle}" TargetType="{x:Type Border}">
    <Setter Property="BorderThickness" Value="1,0,0,0" />
    <Setter Property="BorderBrush" Value="#7F45639B" />
    <Setter Property="Width" Value="6" />
    <Setter Property="Margin" Value="-3,0,0,-3" />
  </Style>
</ResourceDictionary>
