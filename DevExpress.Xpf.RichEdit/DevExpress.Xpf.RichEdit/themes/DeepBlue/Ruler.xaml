  <!--RulerActiveAreaControl-->
  <Style TargetType="{x:Type dxrec:RulerActiveAreaControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=ActiveAreaControlTemplate}}" />
  </Style>
  <!--HorizontalRulerSpaceAreaControl-->
  <Style TargetType="{x:Type dxrec:HorizontalRulerSpaceAreaControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=HorizontalSpaceAreaControlTemplate}}" />
  </Style>
  <!--VerticalRulerSpaceAreaControl-->
  <Style TargetType="{x:Type dxrec:VerticalRulerSpaceAreaControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=VerticalSpaceAreaControlTemplate}}" />
  </Style>
  <!--RulerTickMarkControl-->
  <Style TargetType="{x:Type dxrec:RulerTickMarkControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=TickMarkControlTemplate}}" />
  </Style>
  <!--RulerNumberTickMarkControl-->
  <Style TargetType="{x:Type dxrec:RulerNumberTickMarkControl}">
    <Setter Property="FontSize" Value="12" />
    <Setter Property="FontFamily" Value="Arial" />
    <Setter Property="FontStyle" Value="Normal" />
    <Setter Property="FontWeight" Value="Normal" />
    <Setter Property="Foreground" Value="#FF6E6E6E" />    
  </Style>
  <!--DefaultTabControl-->
  <Style TargetType="{x:Type dxrec:DefaultTabControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=DefaultTabControlTemplate}}" />
  </Style>
  <!--DisabledHotZoneControl-->
  <Style TargetType="{x:Type dxrec:DisabledHotZoneControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=DisabledHotZoneControlTemplate}}" />
  </Style>
  <!--LeftIndentHotZoneControl-->
  <Style TargetType="{x:Type dxrec:LeftIndentHotZoneControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=LeftIndentHotZoneControlTemplate}}" />
  </Style>
  <!--RightIndentHotZoneControl-->
  <Style x:Key="{x:Type dxrec:RightIndentHotZoneControl}" TargetType="{x:Type dxrec:RightIndentHotZoneControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=LeftIndentHotZoneControlTemplate}}" />
    <Setter Property="Margin" Value="0,0,0,0" />
  </Style>
  <!--FirstLineIndentHotZoneControl-->
  <Style TargetType="{x:Type dxrec:FirstLineIndentHotZoneControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=FirstLineIndentHotZoneControlTemplate}}" />
  </Style>
  <!--LeftIndentBottomHotZoneControl-->
  <Style TargetType="{x:Type dxrec:LeftIndentBottomHotZoneControl}">
