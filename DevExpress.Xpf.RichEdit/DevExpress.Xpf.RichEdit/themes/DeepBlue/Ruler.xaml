  <Style TargetType="{x:Type dxrec:RightTabHotZoneControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=RightTabHotZoneControlTemplate}}" />
  </Style>
  <!--LeftTabHotZoneControl-->
  <Style TargetType="{x:Type dxrec:LeftTabHotZoneControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=LeftTabHotZoneControlTemplate}}" />
  </Style>
    <!--HorizontalTableHotZoneControl-->
    <Style TargetType="{x:Type dxrec:HorizontalTableHotZoneControl}">
        <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=HorizontalTableHotZoneControlTemplate}}" />
    </Style>
    <!--CenterTabHotZoneControl-->
  <Style TargetType="{x:Type dxrec:CenterTabHotZoneControl}">
    <Setter Property="Template" Value="{DynamicResource {dxret:RichEditRulerThemeKey ResourceKey=CenterTabHotZoneControlTemplate}}" />
  </Style>
  <!--DecimalTabHotZoneControl-->
