<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxrec="clr-namespace:DevExpress.Xpf.RichEdit.Controls.Internal"
    >

    <!--XpfRichEditImageSelection-->
    <Style TargetType="{x:Type dxrec:XpfRichEditImageSelection}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dxrec:XpfRichEditImageSelection">
                    <Grid IsHitTestVisible="False" x:Name="root" SnapsToDevicePixels="True" UseLayoutRounding="True" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--BEGIN WPF-->
        <Setter Property="Focusable" Value="False" />
        <!--END WPF-->
    </Style>

</ResourceDictionary>
