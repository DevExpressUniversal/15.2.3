<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxreui="clr-namespace:DevExpress.Xpf.RichEdit.UI"
    xmlns:dxret="clr-namespace:DevExpress.Xpf.RichEdit.Themes"
    >

    <!--PageNumberWindow-->
    <Style TargetType="{x:Type dxreui:PageNumberWindow}">
        <Setter Property="Template" Value="{DynamicResource {dxret:RichEditControlThemeKey ResourceKey=PageNumberWindowControlTemplate}}" />
        <!--BEGIN WPF-->
        <Setter Property="Focusable" Value="False" />
        <!--END WPF-->
    </Style>

</ResourceDictionary>
