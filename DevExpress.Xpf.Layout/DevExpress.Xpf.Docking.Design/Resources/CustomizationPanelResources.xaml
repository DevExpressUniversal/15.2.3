                    <Grid x:Name="grid" Width="12" Height="12" Background="#00FF0000" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                        <Path x:Name="path" Data="F1 M 1264,948L 1260,952L 1256,948L 1264,948 Z " Fill="#FF92959B" Stretch="Fill" UseLayoutRounding="False" Width="8" Height="4" HorizontalAlignment="Center" VerticalAlignment="Center" />
