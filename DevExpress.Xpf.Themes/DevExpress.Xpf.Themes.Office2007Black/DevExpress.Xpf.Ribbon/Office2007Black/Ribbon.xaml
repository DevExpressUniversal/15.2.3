                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#7F656565" Offset="1" />
                    <GradientStop Color="#FF656565" />
                </LinearGradientBrush>
            </Border.Background>
            <Border x:Name="border2" Margin="0,0,0,1" CornerRadius="3">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FF535353" Offset="0" />
