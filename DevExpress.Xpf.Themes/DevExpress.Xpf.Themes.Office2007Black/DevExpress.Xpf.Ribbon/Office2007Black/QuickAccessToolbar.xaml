                                    <GradientStop Color="#FFF0A85C " />
                                </LinearGradientBrush>
                            </Border.Background>
                            <Border SnapsToDevicePixels="True" x:Name="pressed2" BorderThickness="0" CornerRadius="0" Margin="1">
                                <Border.Background>
