                <Path.Fill>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#ff4e4f4f" Offset="0" />
                        <GradientStop Color="#ff282828" Offset="1" />
                    </LinearGradientBrush>
                </Path.Fill>
                </Path>
                <Path Stretch="Fill" Data="F1 M 250.078,191L 250.078,209L 245.058,209C 244.758,201.676 241.176,195.199 235.743,191L 250.078,191 Z " HorizontalAlignment="Right" Margin="2,1,1,1" SnapsToDevicePixels="True" VerticalAlignment="Stretch">
                    <Path.Fill>
