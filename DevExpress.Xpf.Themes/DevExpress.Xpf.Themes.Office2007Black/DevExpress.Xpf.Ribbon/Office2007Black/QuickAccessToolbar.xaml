                            </LinearGradientBrush>
                        </Border.Background>
                        <Border SnapsToDevicePixels="True" x:Name="hover2" BorderThickness="0" CornerRadius="0" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
