                                    <GradientStop Color="#FFFFE794 " Offset="1" />
                                    <GradientStop Color="#FFFFE797 " Offset="0.5" />
                                    <GradientStop Color="#FFFFD758 " Offset="0.5" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid>
                <Grid SnapsToDevicePixels="True" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center">
                    <StackPanel SnapsToDevicePixels="True" HorizontalAlignment="Center" VerticalAlignment="Center" Width="5" Orientation="Vertical" Margin="4,0,4,1">
                        <Grid SnapsToDevicePixels="True" Width="6" Height="4">
