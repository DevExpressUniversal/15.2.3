<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:ve="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/docking/themekeys" xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <LinearGradientBrush x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=CutBrushVertical}" EndPoint="1,0.5" StartPoint="0,0.5" cs:Name="LinearGradientBrush_6440">
        <GradientStop Color="#FFFFFFFF" Offset="1" cs:Name="GradientStop_0005" />
        <GradientStop Color="#F0FFFFFF" Offset="0.5" cs:Name="GradientStop_0006" />
        <GradientStop Color="#00FFFFFF" Offset="0" cs:Name="GradientStop_0007" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=CutBrushHorizontal}" EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_6441">
        <GradientStop Color="#FFFFFFFF" Offset="1" cs:Name="GradientStop_0008" />
        <GradientStop Color="#F0FFFFFF" Offset="0.5" cs:Name="GradientStop_0009" />
        <GradientStop Color="#00FFFFFF" Offset="0" cs:Name="GradientStop_0010" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItem}" EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0426">
        <GradientStop Color="#FFEFF5FB" Offset="0" cs:Name="GradientStop_0001" />
        <GradientStop Color="#FFD2D6DF" Offset="1" cs:Name="GradientStop_0002" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItemCaption}" EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0427">
        <GradientStop Color="#FFE4E5EC" Offset="0" cs:Name="GradientStop_0003" />
        <GradientStop Color="#FFD6D9E2" Offset="1" cs:Name="GradientStop_0004" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}" Color="#FF515D7A" cs:Name="SolidColorBrush_0001" />
    <ControlTemplate x:Key="{dxt:OtherResourcesThemeKey ResourceKey=HiddenItemTemplate}">
        <Grid Height="Auto">
            <Border x:Name="Item" CornerRadius="2" Opacity="0.5" Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}">
                <Border Margin="1" CornerRadius="1" Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}" />
            </Border>
            <Border>
                <StackPanel VerticalAlignment="Center" Orientation="Horizontal">
                    <Image x:Name="PART_Image" Height="16" Width="16" Margin="8,6,0,6" />
                    <TextBlock Text="{Binding Path=CustomizationCaption}" Margin="8,6" />
                </StackPanel>
            </Border>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxt:DockElementsThemeKey ResourceKey=DragCursor}" TargetType="ve:DragCursorControl">
        <ControlTemplate.Resources>
            <ControlTemplate TargetType="ContentControl" x:Key="ItemTemplate">
                <Grid>
                    <Grid x:Name="Item" VerticalAlignment="Top" Opacity="0.75">
                        <Border Background="#FF000000" Margin="2,2,0,0" CornerRadius="3" Opacity="0.15" cs:Name="Border_0001" />
                        <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}" Margin="0,0,2,2" CornerRadius="2" Height="28">
                            <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}" Margin="1" CornerRadius="1" />
                        </Border>
                    </Grid>
                    <Border x:Name="CaptionContainer" MinHeight="28" Margin="0,0,2,2" VerticalAlignment="Top" HorizontalAlignment="Center">
                        <ContentPresenter Margin="8,2" />
                    </Border>
                </Grid>
            </ControlTemplate>
            <ControlTemplate TargetType="ContentControl" x:Key="PanelTemplate">
                <Grid>
                    <Grid x:Name="Panel" Opacity="0.75" Margin="0,8,2,2">
                        <Border Background="#FF000000" Margin="2,2,0,0" CornerRadius="3" Opacity="0.15" cs:Name="Border_0002" />
                        <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}" Margin="0,0,2,2" CornerRadius="2">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="16" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemCaption}}" Margin="1" CornerRadius="1,1,0,0" Grid.Row="0" Opacity="0.75" />
                                <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}" Margin="1,0,1,1" CornerRadius="0,0,1,1" Grid.Row="1" />
                            </Grid>
                        </Border>
                    </Grid>
                    <Border x:Name="CaptionContainer" MinHeight="28" Margin="0,0,2,2" VerticalAlignment="Center" HorizontalAlignment="Center">
                        <ContentPresenter Margin="8,6" />
                    </Border>
                </Grid>
            </ControlTemplate>
        </ControlTemplate.Resources>
        <ContentControl Template="{StaticResource PanelTemplate}" x:Name="content">
            <ve:TemplatedCaptionControl x:Name="PART_CaptionControl" LayoutItem="{Binding}" Foreground="#FF1E395B" cs:Name="CaptionControl_0001" />
        </ContentControl>
        <ControlTemplate.Triggers>
            <Trigger Property="CursorType" Value="Item">
                <Setter Property="Template" TargetName="content" Value="{StaticResource ItemTemplate}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
