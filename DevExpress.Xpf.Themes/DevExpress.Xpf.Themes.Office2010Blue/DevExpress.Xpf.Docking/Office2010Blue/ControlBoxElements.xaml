                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0365">
                            <GradientStop Color="#FFC28E35" Offset="0" cs:Name="GradientStop_0008" />
                            <GradientStop Color="#FFC2A44D" Offset="1" cs:Name="GradientStop_0009" />
