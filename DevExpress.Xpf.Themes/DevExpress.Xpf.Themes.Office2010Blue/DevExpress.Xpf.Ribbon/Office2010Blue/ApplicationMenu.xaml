<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys" xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationFramework" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF" cs:Name="Border_0001" />
    </ControlTemplate>
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2007}" TargetType="{x:Type ContentControl}">
        <Grid SnapsToDevicePixels="True">
            <Border x:Name="Shadow" Background="#FF000000" Margin="3,3,0,0" CornerRadius="3" Opacity="0.1" cs:Name="Border_0002" />
            <Border x:Name="Border" Background="#FF8492A6" Margin="0,0,3,3" cs:Name="Border_0003">
                <Grid Margin="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" MinHeight="22" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Border x:Name="Header" BorderThickness="0,1">
                        <Border.BorderBrush>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0527">
                                <GradientStop Color="#FFE1EDFA" Offset="0" cs:Name="GradientStop_0001" />
                                <GradientStop Color="#FF94ADCC" Offset="1" cs:Name="GradientStop_0002" />
                            </LinearGradientBrush>
                        </Border.BorderBrush>
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0528">
                                <GradientStop Color="#FFF6FBFF" Offset="0" cs:Name="GradientStop_0003" />
                                <GradientStop Color="#FFDCE6F4" Offset="1" cs:Name="GradientStop_0004" />
                                <GradientStop Color="#FFE7EEF8" Offset="0.5" cs:Name="GradientStop_0005" />
                                <GradientStop Color="#FFDCE6F4" Offset="0.5" cs:Name="GradientStop_0006" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                    <Border Background="#FFFFFFFF" Grid.Row="1" cs:Name="Border_0004">
                        <ContentPresenter />
                    </Border>
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(interop:BrowserInteropHelper.IsBrowserHosted)}" Value="True">
                <Setter Property="Visibility" TargetName="Shadow" Value="Collapsed" />
                <Setter Property="Margin" TargetName="Border" Value="0" />
                <Setter Property="CornerRadius" TargetName="Border" Value="0" />
                <Setter Property="CornerRadius" TargetName="Header" Value="0" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBottomPaneTemplate}" TargetType="{x:Type ContentControl}">
        <Border x:Name="Footer" BorderBrush="#FFD5DEE8" BorderThickness="0,1,0,0" cs:Name="Border_0005">
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0529">
                    <GradientStop Color="#FFF6FBFF" Offset="0" cs:Name="GradientStop_0007" />
                    <GradientStop Color="#FFE3EBFA" Offset="1" cs:Name="GradientStop_0008" />
                </LinearGradientBrush>
            </Border.Background>
            <ContentPresenter Margin="2" HorizontalAlignment="Right" SnapsToDevicePixels="True" />
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuRightPaneTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FFFFFFFF" BorderBrush="#FFC0C3C6" BorderThickness="1,0,0,0" HorizontalAlignment="Stretch" cs:Name="Border_0007">
            <ContentPresenter />
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuContentControlBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Grid SnapsToDevicePixels="True">
            <Border Background="#FFFFCC00" BorderBrush="#FFFFCC00" BorderThickness="0,1,0,0" cs:Name="Border_0009">
                <Border.OpacityMask>
                    <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5" cs:Name="LinearGradientBrush_0530">
                        <GradientStop Color="#FF000000" Offset="0" cs:Name="GradientStop_0009" />
                        <GradientStop Color="#00000000" Offset="1" cs:Name="GradientStop_0010" />
                    </LinearGradientBrush>
                </Border.OpacityMask>
            </Border>
            <ContentPresenter Content="{TemplateBinding Content}" Margin="0,2,0,0" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2010}" TargetType="{x:Type ContentControl}">
        <ContentControl Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=ApplicationMenuBarControlBorderTemplate2007}}">
            <ContentPresenter />
        </ContentControl>
    </ControlTemplate>
    <SolidColorBrush x:Key="{dxrt:RibbonThemeKey ResourceKey=ApplicationMenuRightPaneForeground}" Color="#FF333333" cs:Name="SolidColorBrush_0001" />
</ResourceDictionary>