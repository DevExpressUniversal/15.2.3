<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=LeftPartTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Background="#FF849DBD" cs:Name="Border_0001">
            <Grid Margin="0,1,0,0">
                <Border>
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0693">
                            <GradientStop Color="#FFDBE7F6" Offset="0" cs:Name="GradientStop_0001" />
                            <GradientStop Color="#FFB4C5D9" Offset="1" cs:Name="GradientStop_0002" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border BorderBrush="#7FFFFFFF" BorderThickness="0,1,0,1" cs:Name="Border_0004" />
                </Border>
            </Grid>
            <ContentPresenter Margin="2,2,1,1" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=RightPartTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Background="#FF849DBD" cs:Name="Border_0003">
            <Grid Margin="0,1,0,0">
                <Border>
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0694">
                            <GradientStop Color="#FFDBE7F6" Offset="0" cs:Name="GradientStop_0003" />
                            <GradientStop Color="#FFB4C5D9" Offset="1" cs:Name="GradientStop_0004" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border BorderBrush="#7FFFFFFF" BorderThickness="0,1,0,1" cs:Name="Border_0005" />
                </Border>
            </Grid>
            <ContentPresenter Margin="2,2,1,1" />
        </Grid>
    </ControlTemplate>
