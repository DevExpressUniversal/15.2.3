    <ControlTemplate x:Key="{dxbt:BarItemSeparatorThemeKey ResourceKey=InMenuTemplate}" TargetType="{x:Type dxb:BarItemLinkSeparatorControl}">
        <Grid SnapsToDevicePixels="True">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Border Width="{Binding Path=InMenuContentOffset, RelativeSource={RelativeSource TemplatedParent}}" />
            <Rectangle Stroke="#FF94ADCC" StrokeDashArray="1 3" Height="1" VerticalAlignment="Center" Margin="7,0,2,1" Grid.Column="1" SnapsToDevicePixels="True" cs:Name="Rectangle_0001" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxbt:BarItemSeparatorThemeKey ResourceKey=Template}" TargetType="{x:Type dxb:BarItemLinkSeparatorControl}">
        <dxc:PixelSnapper>
            <Border Background="#FF94ADCC" BorderBrush="#CFFFFFFF" BorderThickness="1" Width="3" Margin="0,1,1,1" cs:Name="Border_0002">
                <Border.OpacityMask>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0089">
                        <GradientStop Color="#00000000" Offset="0" cs:Name="GradientStop_0001" />
                        <GradientStop Color="#00000000" Offset="1" cs:Name="GradientStop_0002" />
                        <GradientStop Color="#FF1E395B" Offset="0.5" cs:Name="GradientStop_0003" />
                    </LinearGradientBrush>
                </Border.OpacityMask>
            </Border>
        </dxc:PixelSnapper>
    </ControlTemplate>
