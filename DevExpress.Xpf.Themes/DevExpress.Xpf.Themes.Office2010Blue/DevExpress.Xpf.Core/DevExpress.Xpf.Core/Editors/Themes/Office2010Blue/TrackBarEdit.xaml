                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid>
                    <Path Stretch="Fill" Fill="{StaticResource SliderThumb.DisabledState.Border1}" Data="F1 M 146,357L 156,357L 151,352L 146,357 Z" />
                    <Path Stretch="Fill" Fill="{StaticResource SliderThumb.DisabledState.Border2}" Data="F1 M 147,357L 155,357L 151,353L 147,357 Z" Margin="1,1,1,0" />
