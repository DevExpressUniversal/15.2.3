                </Grid.RowDefinitions>
                <Grid>
                    <Path Stretch="Fill" Data="F1 M 146,357L 156,357L 151,352L 146,357 Z">
                        <Path.Fill>
                            <SolidColorBrush Color="{StaticResource SliderThumb.DefaultState.Glyph.BorderBrush}" />
                        </Path.Fill>
                    </Path>
                    <Path Stretch="Fill" Data="F1 M 147,357L 155,357L 151,353L 147,357 Z" Margin="1,1,1,0">
                        <Path.Fill>
                            <SolidColorBrush Color="{StaticResource SliderThumb.DefaultState.Glyph.Background.0}" />
                        </Path.Fill>
                    </Path>
                </Grid>
                <Border Background="{StaticResource SliderThumb.DefaultState.Border1}" Padding="1,0,1,1" Grid.Row="1">
                    <Border Background="{StaticResource SliderThumb.DefaultState.Border2}" />
                </Border>
            </Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid>
                    <Path Stretch="Fill" Data="F1 M 146,357L 156,357L 151,352L 146,357 Z">
                        <Path.Fill>
                            <SolidColorBrush Color="{StaticResource SliderThumb.HoverState.Glyph.BorderBrush}" />
                        </Path.Fill>
                    </Path>
                    <Path Stretch="Fill" Data="F1 M 147,357L 155,357L 151,353L 147,357 Z" Margin="1,1,1,0">
                        <Path.Fill>
                            <SolidColorBrush Color="{StaticResource SliderThumb.HoverState.Glyph.Background1.0}" />
                        </Path.Fill>
                    </Path>
                    <Path Stretch="Fill" Data="F1 M 148,357L 154,357L 151,354L 148,357 Z" Margin="2,2,2,0">
                        <Path.Fill>
                            <SolidColorBrush Color="{StaticResource SliderThumb.HoverState.Glyph.Background2.0}" />
                        </Path.Fill>
                    </Path>
                </Grid>
                <Border Background="{StaticResource SliderThumb.HoverState.Border1}" Padding="1,0,1,1" Grid.Row="1">
                    <Border Background="{StaticResource SliderThumb.HoverState.Border2}" Padding="1,0,1,1">
                        <Border Background="{StaticResource SliderThumb.HoverState.Border3}" />
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid>
                    <Path Stretch="Fill" Data="F1 M 146,357L 156,357L 151,352L 146,357 Z">
                        <Path.Fill>
                            <SolidColorBrush Color="{StaticResource SliderThumb.PressedState.Glyph.BorderBrush.0}" />
                        </Path.Fill>
                    </Path>
                    <Path Stretch="Fill" Data="F1 M 147,357L 155,357L 151,353L 147,357 Z" Margin="1,1,1,0">
                        <Path.Fill>
                            <SolidColorBrush Color="{StaticResource SliderThumb.PressedState.Glyph.Background1.0}" />
                        </Path.Fill>
                    </Path>
                    <Path Stretch="Fill" Data="F1 M 148,357L 154,357L 151,354L 148,357 Z" Margin="2,2,2,0">
