            <dxn:RenderPath Name="GlyphExpanded" Stretch="Fill" Fill="#FF666666" Data="F1 M 372.5,127C 376.09,127 379,129.91 379,133.5C 379,137.09 376.09,140 372.5,140C 368.91,140 366,137.09 366,133.5C 366,129.91 368.91,127 372.5,127 Z M 372.5,128C 369.462,128 367,130.463 367,133.5C 367,136.538 369.462,139 372.5,139C 375.537,139 378,136.538 378,133.5C 378,130.463 375.537,128 372.5,128 Z M 370,136L 370,135L 373,135L 370,132L 370,131L 371,131L 374,134L 374,131L 375,131L 375,135L 375,136L 375,136L 374,136L 370,136 Z " Width="13" Height="13" Visibility="Hidden" cs:Name="RenderPath_0004" />
        </dxn:RenderPanel>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="MouseStates">
                <dxn:RenderStateTrigger Name="Normal" />
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter Property="Fill" TargetName="GlyphExpanded" Value="#FF373A3D" cs:Name="RenderSetter_0004" />
                    <dxn:RenderSetter Property="Fill" TargetName="GlyphCollapsed" Value="#FF373A3D" cs:Name="RenderSetter_0005" />
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="Pressed">
                    <dxn:RenderSetter Property="Fill" TargetName="GlyphExpanded" Value="#FF373A3D" cs:Name="RenderSetter_0006" />
                    <dxn:RenderSetter Property="Fill" TargetName="GlyphCollapsed" Value="#FF373A3D" cs:Name="RenderSetter_0007" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="CheckedStates">
                <dxn:RenderStateTrigger Name="Checked">
                    <dxn:RenderSetter Property="Visibility" TargetName="GlyphExpanded" Value="Visible" />
                    <dxn:RenderSetter Property="Visibility" TargetName="GlyphCollapsed" Value="Hidden" />
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="Unchecked" />
            </dxn:RenderStateGroup>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>
</ResourceDictionary>