                <dxn:RenderPath Stretch="Fill" Fill="#FF666666" Data="F1 M 6,0C 9.31371,0 12,2.68629 12,6C 12,9.31371 9.31371,12 6,12C 2.68629,12 -6.35783e-007,9.31371 -6.35783e-007,6C -6.35783e-007,2.68629 2.68629,0 6,0 Z M 2,5L 2,7L 5,7L 5,10L 7,10L 7,7L 10,7L 10,5L 7,5L 7,2L 5,2L 5,5L 2,5 Z " cs:Name="RenderPath_0001" />
            </dxn:RenderPanel>
            <dxn:RenderPanel Name="PART_Remove" Width="12" Height="12" Opacity="0.5" Visibility="Collapsed">
                <dxn:RenderBorder Background="#00000000" CornerRadius="256" Margin="1" cs:Name="RenderBorder_0005" />
                <dxn:RenderPath Name="Path" Stretch="Fill" Fill="#FF666666" Data="F1 M 1.75736,1.75736C 4.1005,-0.585785 7.89949,-0.585785 10.2426,1.75736C 12.5858,4.10051 12.5858,7.8995 10.2426,10.2426C 7.89949,12.5858 4.1005,12.5858 1.75736,10.2426C -0.585787,7.8995 -0.585787,4.10051 1.75736,1.75736 Z M 2.46447,8.12132L 3.87868,9.53553L 6,7.41421L 8.12132,9.53553L 9.53553,8.12132L 7.41421,6L 9.53553,3.87868L 8.12132,2.46447L 6,4.58579L 3.87868,2.46447L 2.46447,3.87868L 4.58579,6L 2.46447,8.12132 Z " cs:Name="RenderPath_0002" />
            </dxn:RenderPanel>
        </dxn:RenderPanel>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CommonStates">
                <dxn:RenderStateTrigger Name="Normal" />
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter TargetName="PART_Add" Property="Opacity" Value="1" />
                    <dxn:RenderSetter TargetName="PART_Remove" Property="Opacity" Value="0.75" />
                    <dxn:RenderSetter TargetName="Path" Property="Fill" Value="#FF990000" cs:Name="RenderSetter_0003" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="ButtonKind">
                <dxn:RenderStateTrigger Name="Add" />
                <dxn:RenderStateTrigger Name="Remove">
                    <dxn:RenderSetter TargetName="PART_Remove" Property="Visibility" Value="Visible" />
                    <dxn:RenderSetter TargetName="PART_Add" Property="Visibility" Value="Collapsed" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>
