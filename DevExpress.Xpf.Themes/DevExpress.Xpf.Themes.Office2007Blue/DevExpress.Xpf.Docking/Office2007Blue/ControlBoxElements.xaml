                                    <Path Stretch="Fill" Fill="#5AFFFFFF" Data="F1 M 1376,794L 1369,787L 1369,787L 1362,794L 1366,794L 1369,791L 1369,791L 1372,794L 1376,794 Z " HorizontalAlignment="Stretch" Margin="0,4,0,0" VerticalAlignment="Stretch" />
                                    <Path Stretch="Fill" Fill="#FF839BBB" Data="F1 M 1369,784L 1369,784L 1369,784L 1374,789L 1372,789L 1369,786L 1366,789L 1364,789L 1369,784L 1369,784 Z M 1369,788L 1369,788L 1369,788L 1374,793L 1372,793L 1369,790L 1366,793L 1364,793L 1369,788L 1369,788 Z " HorizontalAlignment="Stretch" Margin="2,1,2,1" VerticalAlignment="Stretch" x:Name="path1" />
                                </Grid>
                            </Grid>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
