                                    <Path Stretch="Fill" Fill="#5AFFFFFF" Data="F1 M 1385,807L 1392,814L 1392,814L 1399,807L 1395,807L 1392,810L 1392,810L 1389,807L 1385,807 Z " HorizontalAlignment="Stretch" Margin="0,0,0,4" VerticalAlignment="Stretch" />
                                    <Path Stretch="Fill" Fill="#FF839BBB" Data="F1 M 1392,817L 1387,812L 1389,812L 1392,815L 1395,812L 1397,812L 1392,817 Z M 1392,813L 1387,808L 1389,808L 1392,811L 1395,808L 1397,808L 1392,813 Z " HorizontalAlignment="Stretch" Margin="2,1,2,1" VerticalAlignment="Stretch" x:Name="path" />
                                </Grid>
