<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/docking" xmlns:system="clr-namespace:System;assembly=mscorlib" xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:ve="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/docking/themekeys" xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/docking/platform" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal">
    <!-- MDIButton -->
    <ControlTemplate x:Key="{dxt:MDIButtonElementsThemeKey ResourceKey=ButtonBorder}" TargetType="dxb:ItemBorderControl">
        <Grid Width="18" Height="18">
            <Grid x:Name="PART_Hover" Opacity="0.0">
                <Border x:Name="hover" Background="#FF8EA7C4" CornerRadius="2">
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="White" Offset="0" />
                                <GradientStop Color="#FFE0EEFF" Offset="1" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FFF5F9FF" Offset="0" />
                                    <GradientStop Color="#FFD2E3F7" Offset="1" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PART_Pressed" Opacity="0.0">
                <Border x:Name="pressed" Background="#FF8EA7C4" CornerRadius="2" Visibility="Collapsed">
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFDDE8F5" Offset="1" />
                                <GradientStop Color="#FFE0EEFF" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FFD6E1F1" Offset="1" />
                                    <GradientStop Color="#FFD0DFEF" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="State">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetProperty="(Control.Opacity)" Storyboard.TargetName="PART_Hover" To="1" Duration="0" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetProperty="(Control.Opacity)" Storyboard.TargetName="PART_Pressed" To="1" Duration="0" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked" />
                    <VisualState x:Name="HoverChecked" />
                    <VisualState x:Name="Focused" />
                    <VisualState x:Name="Customization" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>
    <Style x:Key="{dxt:MDIButtonElementsThemeKey ResourceKey=ButtonBorderStyle}" TargetType="dxb:ItemBorderControl">
        <Setter Property="Template" Value="{DynamicResource {dxt:MDIButtonElementsThemeKey ResourceKey=ButtonBorder}}" />
    </Style>
    <!-- Glyphs -->
    <DataTemplate x:Key="{dxt:GlyphElementsThemeKey ResourceKey=MDIButtonClose}">
        <Border Background="Transparent" Margin="-4">
            <Path x:Name="path" Stretch="Fill" Fill="#FF6F87A3" Data="F1 M 1220,881L 1222,881L 1218,885L 1222,889L 1220,889L 1217,886L 1214,889L 1212,889L 1216,885L 1212,881L 1214,881L 1217,884L 1220,881 Z " HorizontalAlignment="Center" Height="8" Margin="0" UseLayoutRounding="False" VerticalAlignment="Center" Width="10" />
        </Border>
        <DataTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Fill" TargetName="path" Value="#FF52698F" />
            </Trigger>
        </DataTemplate.Triggers>
    </DataTemplate>
    <DataTemplate x:Key="{dxt:GlyphElementsThemeKey ResourceKey=MDIButtonMinimize}">
        <Border Background="Transparent" Margin="-4">
            <Rectangle x:Name="path" Fill="#FF6F87A3" Height="2" Width="10" UseLayoutRounding="True" HorizontalAlignment="Center" Margin="0,8,0,0" VerticalAlignment="Center" />
