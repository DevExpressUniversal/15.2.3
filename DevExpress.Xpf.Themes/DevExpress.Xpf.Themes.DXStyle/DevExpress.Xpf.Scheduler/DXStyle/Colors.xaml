		<dxsch:SchedulerColorSchema CellLight="#FFEFFFE5" Cell="#FFDEEED4" CellLightBorderDark="#FFC5D5BB" CellLightBorder="#FFCEDEC4" CellBorder="#FFC5D5BB" CellBorderDark="#FFCEDEC4" />
		<dxsch:SchedulerColorSchema CellLight="#FFFDFFE5" Cell="#FFECEED4" CellLightBorderDark="#FFD0D0D0" CellLightBorder="#FFDADADA" CellBorder="#FFD0D0D0" CellBorderDark="#FFDADADA" />
	</dxsch:SchedulerColorSchemaCollection>

	<SolidColorBrush x:Key="ResourceNavigatorGlyphBrush" Color="#FF201F35" />

	<dxschd:ColorCollection x:Key="HeaderBaseColors">
		<dxschd:ColorDefinition Value="#FFCACBD3" Name="ColorDefinition_HeaderBaseBorderColor" />
		<dxschd:ColorDefinition Value="#FFF2F3F4" Name="ColorDefinition_NormalGradientStop1" />
		<dxschd:ColorDefinition Value="#FFEDEEEF" Name="ColorDefinition_NormalGradientStop2" />
		<dxschd:ColorDefinition Value="#FFEFF0F1" Name="ColorDefinition_NormalGradientStop3" />
		<dxschd:ColorDefinition Value="#FFEFE8F9" Name="ColorDefinition_AltNormalGradientStop1" />
		<dxschd:ColorDefinition Value="#FFECE3F8" Name="ColorDefinition_AltNormalGradientStop2" />
		<dxschd:ColorDefinition Value="#FFEDE5F9" Name="ColorDefinition_AltNormalGradientStop3" />
	</dxschd:ColorCollection>

	<SolidColorBrush x:Key="TimeRulerTextForeground" Color="#FF80838F" />

	<!-- Resource Navigator -->

	<SolidColorBrush x:Key="HorizontalResourceNavigatorBackground" Color="#FFF5F5F5" />
	<SolidColorBrush x:Key="VerticalResourceNavigatorBackground" Color="#FFF5F5F5" />

	<LinearGradientBrush x:Key="HorizontalButtonStyle.HoverState.Border1" EndPoint="0.5,1" StartPoint="0.5,0">
