		<dxsch:SchedulerColorSchema CellLight="#FFE5FFEB" Cell="#FFD4EEDA" CellLightBorderDark="#FFBBD5C1" CellLightBorder="#FFC4DECA" CellBorder="#FFBBD5C1" CellBorderDark="#FFC4DECA" />
		<dxsch:SchedulerColorSchema CellLight="#FFFFE5F2" Cell="#FFEED4E1" CellLightBorderDark="#FFD5BBC8" CellLightBorder="#FFDEC4D1" CellBorder="#FFD5BBC8" CellBorderDark="#FFDEC4D1" />
