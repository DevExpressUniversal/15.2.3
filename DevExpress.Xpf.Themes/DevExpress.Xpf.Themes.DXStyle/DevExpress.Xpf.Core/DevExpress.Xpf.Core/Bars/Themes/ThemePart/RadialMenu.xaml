<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys">

    <SolidColorBrush x:Key="RadialMenuLevelControl_BackColor"  Color="#FFB2B5C1" />
    <SolidColorBrush x:Key="RadialMenuLevelControl_InnerColor" Color="#FFFFFFFF" />
    
    <SolidColorBrush x:Key="CenterButton_StrokeColor" Color="#FF80838F" /> 
    <SolidColorBrush x:Key="CenterButton_InnerColor"  Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="CenterButton_HoverColor"  Color="#3F80838F" />

    <SolidColorBrush x:Key="BarItemInRadialMenu_MenuColor" Color="#FF80838F" /> 
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathHover" Color="#FF4E515D" />
    
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=NormalImageColorizeColor}">#FF201F35</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=HoverImageColorizeColor}">#FF201F35</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=PressedImageColorizeColor}">#FF201F35</Color> 
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=DisabledImageColorizeColor}">#7F201F35</Color>

</ResourceDictionary>