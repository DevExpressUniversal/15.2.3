    <!--BEGIN WPF-->
    <ResourceDictionary.MergedDictionaries>
        <dxtEx:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.DXStyle.v15.2;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/DXStyle/ButtonEdit.xaml" />
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.DXStyle.v15.2;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/DXStyle/Brushes.xaml" />
        <dxtEx:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.DXStyle.v15.2;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/InplaceBaseEdit.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <!--END WPF-->   
    
    <Visibility x:Key="Visible">Visible</Visibility>
    <dxn:RenderTemplate x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=RenderCheckBoxTemplate}">
        <dxn:RenderPanel Name="PART_PaddingTarget" LayoutProvider="{dxn:DockPanelLayoutProvider LastChildFill=True}">
                <dxn:RenderPanel Dock="Left" Name="CheckBox" Width="14" Height="14" Margin="0,3,0,3" VerticalAlignment="Center" FlowDirection="LeftToRight">
                    <dxn:RenderBorder Name="FocusBorder" Margin="-1" BorderBrush="#FF6699FF" BorderThickness="2" Opacity="0.35" CornerRadius="1" Visibility="Collapsed" />                
                    <dxn:RenderBorder Name="OuterBorder" 
                                      Background="{StaticResource CheckEdit.DefaultState.Border2}"  
                                      BorderBrush="{StaticResource CheckEdit.DefaultState.Border1}"                                   
                                      BorderThickness="1" >
                        <dxn:RenderBorder Name="InnerBorder" Margin="1" 
                                          Background="{StaticResource CheckEdit.DefaultState.Border4}"   
                                          BorderBrush="{StaticResource CheckEdit.DefaultState.Border3}"                                       
                                          BorderThickness="1" />
                    </dxn:RenderBorder>
                    <dxn:RenderBorder Name="TriState" 
                                      BorderBrush="{StaticResource CheckEdit.IndeterminateState.Border1}" 
                                      Background="{StaticResource CheckEdit.IndeterminateState.Border2}"
                                      HorizontalAlignment="Center"  
                                      VerticalAlignment="Center" 
                                      BorderThickness="1"
                                      Width="10" Height="10" Visibility="Collapsed" />
                    <dxn:RenderPath Name="CheckElement" 
                                    Stretch="Fill" 
                                    Fill="#FF201F35" 
                                    Data="F1 M 1442,967L 1444,967L 1446,970L 1447,970L 1454,959L 1456,959L 1447,973L 1446,973L 1442,967 Z"
                                    Margin="1,-1,-1,1" 
                                    Width="14" 
                                    Height="14" 
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Visibility="Collapsed" />
                </dxn:RenderPanel>
                <dxn:RenderContentPresenter Name="PART_ContentPresenter" Margin="4,2,0,2" Visibility="Collapsed" />
        </dxn:RenderPanel>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CommonStates">
                <dxn:RenderStateTrigger Name="Normal" />
                <dxn:RenderStateTrigger Name="Pressed">
                    <dxn:RenderSetter TargetName="OuterBorder" Property="Background" Value="{StaticResource CheckEdit.PressedState.Border2}" />
                    <dxn:RenderSetter TargetName="OuterBorder" Property="BorderBrush" Value="{StaticResource CheckEdit.PressedState.Border1}" />
                    <dxn:RenderSetter TargetName="InnerBorder" Property="Background" Value="{StaticResource CheckEdit.PressedState.Border4}" />
                    <dxn:RenderSetter TargetName="InnerBorder" Property="BorderBrush" Value="{StaticResource CheckEdit.PressedState.Border3}" />
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter TargetName="OuterBorder" Property="Background" Value="{StaticResource CheckEdit.HoverState.Border2}" />
                    <dxn:RenderSetter TargetName="OuterBorder" Property="BorderBrush" Value="{StaticResource CheckEdit.HoverState.Border1}" />
                    <dxn:RenderSetter TargetName="InnerBorder" Property="Background" Value="{StaticResource CheckEdit.HoverState.Border4}" />
                    <dxn:RenderSetter TargetName="InnerBorder" Property="BorderBrush" Value="{StaticResource CheckEdit.HoverState.Border3}" />
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="Disabled">
                    <dxn:RenderSetter TargetName="CheckBox" Property="Opacity" Value="0.35" />
                    <dxn:RenderSetter TargetName="PART_ContentPresenter" Property="Opacity" Value="0.35" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="CheckStates">
                <dxn:RenderStateTrigger Name="Checked">
                    <dxn:RenderSetter TargetName="CheckElement" Property="Visibility" Value="{StaticResource Visible}" />
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="Unchecked" />
                <dxn:RenderStateTrigger Name="Indeterminate">
                    <dxn:RenderSetter TargetName="TriState" Property="Visibility" Value="{StaticResource Visible}" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="FocusStates">
                <dxn:RenderStateTrigger Name="Unfocused" />
                <dxn:RenderStateTrigger Name="Focused">
                    <dxn:RenderSetter TargetName="FocusBorder" Property="Visibility" Value="{StaticResource Visible}" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="ContentStates">
                <dxn:RenderStateTrigger Name="HasNoContent" />
                <dxn:RenderStateTrigger Name="HasContent">
                    <dxn:RenderSetter TargetName="PART_ContentPresenter" Property="Visibility" Value="{StaticResource Visible}" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>


    <SolidColorBrush x:Key="HoverBrush" Color="#FF000000"/>
    <!--NORMAL-->
    <SolidColorBrush Color="#00000000" x:Key="Button.Normal.Outer.BorderBrush"/>
    <SolidColorBrush Color="#00000000" x:Key="Button.Normal.Outer.Background"/>
    <SolidColorBrush Color="#00000000" x:Key="Button.Normal.Inner.Background"/>            

    <dxn:RenderTemplate x:Key="{dxet:InplaceBaseEditThemeKey ResourceKey=RenderButtonTemplate}">
        <dxn:RenderPanel>
            <dxn:RenderBorder Name="Outer" BorderBrush="{StaticResource Button.Normal.Outer.BorderBrush}" BorderThickness="1" Background="{StaticResource Button.Normal.Outer.Background}" Padding="1">
                <dxn:RenderBorder Name="Inner" Background="{StaticResource Button.Normal.Inner.Background}" />
            </dxn:RenderBorder>
            <dxn:RenderContentPresenter Name="PART_ContentPresenter" VerticalAlignment="Center" HorizontalAlignment="Center" />
        </dxn:RenderPanel>
        <dxn:RenderTemplate.Triggers>
            <dxn:RenderStateGroup Name="CommonStates">
                <dxn:RenderStateTrigger Name="Normal" />
                <dxn:RenderStateTrigger Name="MouseOver">
                    <dxn:RenderSetter TargetName="Outer" Property="BorderBrush" Value="{StaticResource ButtonEdit.HoverState.Border1}" />
                    <dxn:RenderSetter TargetName="Outer" Property="Background" Value="{StaticResource ButtonEdit.HoverState.Border2}" />
                    <dxn:RenderSetter TargetName="Inner" Property="Background" Value="{StaticResource ButtonEdit.HoverState.Border3}" />
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="Pressed">
                    <dxn:RenderSetter TargetName="Outer" Property="BorderBrush" Value="{StaticResource ButtonEdit.PressedState.Border1}" />
                    <dxn:RenderSetter TargetName="Outer" Property="Background" Value="{StaticResource ButtonEdit.PressedState.Border2}" />
                    <dxn:RenderSetter TargetName="Inner" Property="Background" Value="{StaticResource ButtonEdit.PressedState.Border3}" />
                    <dxn:RenderSetter TargetName="Outer" Property="Padding" Value="1,1,1,0" />
                </dxn:RenderStateTrigger>
                <dxn:RenderStateTrigger Name="Disabled" />
            </dxn:RenderStateGroup>
            <dxn:RenderStateGroup Name="CheckedStates">
                <dxn:RenderStateTrigger Name="Unchecked" />
                <dxn:RenderStateTrigger Name="Checked">
                    <dxn:RenderSetter TargetName="Outer" Property="BorderBrush" Value="{StaticResource ButtonEdit.PressedState.Border1}" />
                    <dxn:RenderSetter TargetName="Outer" Property="Background" Value="{StaticResource ButtonEdit.PressedState.Border2}" />
                    <dxn:RenderSetter TargetName="Inner" Property="Background" Value="{StaticResource ButtonEdit.PressedState.Border3}" />
                    <dxn:RenderSetter TargetName="Outer" Property="Padding" Value="1,1,1,0" />
                </dxn:RenderStateTrigger>
            </dxn:RenderStateGroup>
            <dxn:RenderTrigger Property="GlyphKind" Value="User">
                <dxn:RenderSetter TargetName="PART_ContentPresenter" Property="Margin" Value="5,0" />
            </dxn:RenderTrigger>
            <dxn:RenderTrigger Property="GlyphKind" Value="None">
                <dxn:RenderSetter TargetName="PART_ContentPresenter" Property="Margin" Value="5,0" />
            </dxn:RenderTrigger>
        </dxn:RenderTemplate.Triggers>
    </dxn:RenderTemplate>

