    <ControlTemplate TargetType="{x:Type local:CarouselNavigator}" x:Key="{dxt:CarouselNavigatorThemeKey ResourceKey=NavigatorTemplate}">
        <Border d:LayoutOverrides="Width, Height" x:Name="navigator">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <RepeatButton Margin="0,0,1,0"
                                        Name="FirstItem"
                                        Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                                        Grid.Column="0"
                                        Command="dxc:CarouselPanel.FirstItem" 
                                        CommandTarget="{Binding Path=Carousel, RelativeSource={RelativeSource TemplatedParent}}"
                                        >
                    <Grid Width="19" Height="9" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Path SnapsToDevicePixels="True" Stretch="Fill" Data="F1 M 588,565L 588,574L 580,570L 580,569L 588,565 Z M 597,565L 597,574L 589,570L 589,569L 597,565 Z " Width="17" HorizontalAlignment="Right" VerticalAlignment="Stretch" Margin="0,0,0,0">
                            <Path.Fill>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FF041C43" Offset="0" />
                                    <GradientStop Color="#FF1B4FA3" Offset="1" />
                                </LinearGradientBrush>
                            </Path.Fill>
                        </Path>
                        <Rectangle SnapsToDevicePixels="True" Stroke="{x:Null}" HorizontalAlignment="Left" Margin="0,0,0,0" Width="1" Height="Auto" VerticalAlignment="Stretch">
                            <Rectangle.Fill>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FF041C43" Offset="0" />
                                    <GradientStop Color="#FF1B4FA3" Offset="1" />
                                </LinearGradientBrush>
                            </Rectangle.Fill>
                        </Rectangle>
                    </Grid>
                </RepeatButton>
                <RepeatButton Margin="0,0,1,0"
                                        Name="PrevPage"
                                        Grid.Column="1"
                                        Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                                        Command="dxc:CarouselPanel.PreviousPage" 
                                        CommandTarget="{Binding Path=Carousel, RelativeSource={RelativeSource TemplatedParent}}"
                                        >
                    <Path SnapsToDevicePixels="True" Stretch="Fill" Data="F1 M 588,565L 588,574L 580,570L 580,569L 588,565 Z M 597,565L 597,574L 589,570L 589,569L 597,565 Z " Width="17" Height="9" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Path.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF041C43" Offset="0" />
                                <GradientStop Color="#FF1B4FA3" Offset="1" />
                            </LinearGradientBrush>
                        </Path.Fill>
                    </Path>
                </RepeatButton>
                <RepeatButton Grid.Column="2"
                    Margin="0,0,1,0"
                                        Name="PrevItem"
                                        Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                                        Command="dxc:CarouselPanel.Previous" 
                                        CommandTarget="{Binding Path=Carousel, RelativeSource={RelativeSource TemplatedParent}}"
                                        >
                    <Path SnapsToDevicePixels="True" Stretch="Fill" Data="F1 M 588,565L 580,569L 580,570L 588,574L 588,565 Z " Margin="0,0,1,0" Width="8" Height="9" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Path.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF041C43" Offset="0" />
                                <GradientStop Color="#FF1B4FA3" Offset="1" />
                            </LinearGradientBrush>
                        </Path.Fill>
                    </Path>
                </RepeatButton>
                <RepeatButton Grid.Column="3"
                    Margin="0,0,1,0"
                                        Name="NextItem"
                                            Command="dxc:CarouselPanel.Next" 
                                            Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                                        CommandTarget="{Binding Path=Carousel, RelativeSource={RelativeSource TemplatedParent}}"
                                        >
                    <Path SnapsToDevicePixels="True" Stretch="Fill" Data="F1 M 590,589L 598,585L 598,584L 590,580L 590,589 Z " Width="8" Height="9" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Path.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF041C43" Offset="0" />
                                <GradientStop Color="#FF1B4FA3" Offset="1" />
                            </LinearGradientBrush>
                        </Path.Fill>
                    </Path>
                </RepeatButton>
                <RepeatButton Grid.Column="4"
                    Margin="0,0,1,0"
                                        Name="NextPage"
                                        Interval="{Binding Path=Carousel.AnimationTime, RelativeSource={RelativeSource TemplatedParent}}"
                                        Command="dxc:CarouselPanel.NextPage" 
                                        CommandTarget="{Binding Path=Carousel, RelativeSource={RelativeSource TemplatedParent}}"
                                        >
                    <Path SnapsToDevicePixels="True" Stretch="Fill" Data="F1 M 589,574L 589,565L 597,569L 597,570L 589,574 Z M 580,574L 580,565L 588,569L 588,570L 580,574 Z " Width="17" Height="9" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Path.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF041C43" Offset="0" />
                                <GradientStop Color="#FF1B4FA3" Offset="1" />
                            </LinearGradientBrush>
                        </Path.Fill>
                    </Path>
                </RepeatButton>
                <RepeatButton Grid.Column="5"
                                        Name="LastItem"
                                        Command="dxc:CarouselPanel.LastItem" 
                                        CommandTarget="{Binding Path=Carousel, RelativeSource={RelativeSource TemplatedParent}}"
                                        >
                    <Grid Width="19" Height="9">
                        <Path SnapsToDevicePixels="True" Stretch="Fill" Data="F1 M 589,574L 589,565L 597,569L 597,570L 589,574 Z M 580,574L 580,565L 588,569L 588,570L 580,574 Z " Margin="0,0,2,0" HorizontalAlignment="Center" VerticalAlignment="Center">
                            <Path.Fill>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FF041C43" Offset="0" />
                                    <GradientStop Color="#FF1B4FA3" Offset="1" />
                                </LinearGradientBrush>
                            </Path.Fill>
                        </Path>
                        <Rectangle SnapsToDevicePixels="True" Stroke="{x:Null}" HorizontalAlignment="Right" Margin="0,0,0,0" VerticalAlignment="Stretch" Width="1" Height="Auto">
                            <Rectangle.Fill>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FF041C43" Offset="0" />
                                    <GradientStop Color="#FF1B4FA3" Offset="1" />
                                </LinearGradientBrush>
                            </Rectangle.Fill>
                        </Rectangle>
                    </Grid>
                </RepeatButton>
            </Grid>
        </Border>
    </ControlTemplate>
