<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduler" xmlns:dxschd="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/internal" xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/themekeys" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxt="clr-namespace:DevExpress.Xpf.Utils.Themes;assembly=DevExpress.Xpf.Core.v15.2" xmlns:dxtk="clr-namespace:DevExpress.Xpf.Themes;assembly=DevExpress.Xpf.Core.v15.2" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys">


    <ControlTemplate x:Key="{dxscht:SchedulerControlThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <!--BEGIN TOSLONLY REPLACE 'ContextMenu="{x:Null}"''-->
            <Border ContextMenu="{x:Null}">
        <!--END TOSLONLY REPLACE 'ContextMenu="{x:Null}"''-->
                <Grid Width="Auto" Height="Auto">
                        <Grid Width="Auto" Height="Auto">							
                            <Border Background="#FF6E6E6E" Opacity="1" BorderBrush="#FF6E6E6E" BorderThickness="1" CornerRadius="4" />
                            
                            <dxschd:RoundedBorderControl RoundBorderThickness="1,1,1,1" BorderBrush="#FF6E6E6E" CornerRadius="4,4,4,4" SnapsToDevicePixels="True" UseLayoutRounding="True">
                                <ContentPresenter Margin="1,1,1,1" />
                            </dxschd:RoundedBorderControl>
                </Grid>
                    </Grid>
                </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxscht:SchedulerControlThemeKey ResourceKey=InvisibleBorderTemplate}" TargetType="{x:Type ContentControl}">
        <!--BEGIN TOSLONLY REPLACE 'ContextMenu="{x:Null}"''-->
        <Border ContextMenu="{x:Null}">
        <!--END TOSLONLY REPLACE 'ContextMenu="{x:Null}"''-->
            <Grid>
                <Grid>
                    <Border Background="#FF6E6E6E" BorderBrush="#FF6E6E6E" />
                    <ContentPresenter />
                </Grid>
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="{dxscht:SchedulerUIControlThemeKey ResourceKey=ColorMarkerStyle}" TargetType="{x:Type FrameworkElement}">
        <Setter Property="Margin" Value="2,1,0,1" />
    </Style>
    <!--ONLY SL
    <Style x:Key="{dxscht:SchedulerUIControlThemeKey ResourceKey=TextBlockStyle}" TargetType="{x:Type TextBlock}" BasedOn="{StaticResource BaseTextBlockStyle}">
        <Setter Property="Margin" Value="4,1,4,1"/>
    ONLY SL-->
    <!--BEGIN WPF-->
    <Style x:Key="{dxscht:SchedulerUIControlThemeKey ResourceKey=TextBlockStyle}" TargetType="{x:Type TextBlock}" BasedOn="{StaticResource {dxet:TextEditThemeKey ResourceKey=TextBlockStyle}}">
        <!--END WPF-->
        <Setter Property="Padding" Value="0" />
    </Style>
    <Style x:Key="{dxscht:SchedulerUIControlThemeKey ResourceKey=ItemTextBlockStyle}" TargetType="{x:Type TextBlock}">
        <Setter Property="Margin" Value="4,1,4,1" />
    </Style>
</ResourceDictionary>