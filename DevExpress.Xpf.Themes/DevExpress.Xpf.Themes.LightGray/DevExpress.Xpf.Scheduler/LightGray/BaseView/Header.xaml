<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxschd="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/internal" xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduler" xmlns:dxscht="http://schemas.devexpress.com/winfx/2008/xaml/scheduler/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
    
    <dxschd:ElementPositionVerticalWeekHeaderConverter x:Key="elementPositionVerticalWeekHeaderConverter" />


    <!--Vertical Header-->
    <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=VerticalHeaderNormalTemplate}" TargetType="{x:Type dxschd:VerticalHeaderControl}">
        <Border>
            <Border.Resources>
                <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=TemplateCore}" TargetType="{x:Type ContentControl}" >
                    <dxschd:HeaderBorder Background="#FFA1A1A1" DefaultPadding="0,1,0,1" DefaultCornerRadius="0" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}">
                        <dxschd:HeaderBorder Background="#FF6E6E6E" MinHeight="24" SnapsToDevicePixels="True" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" DefaultCornerRadius="0">
                            <ContentPresenter HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="12,0,12,0"/>
                        </dxschd:HeaderBorder>
                    </dxschd:HeaderBorder>
                </ControlTemplate>
            </Border.Resources>
            <ContentControl x:Name="HeaderContent" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Template="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=TemplateCore}}" SnapsToDevicePixels="True" UseLayoutRounding="True" dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}">
            </ContentControl>
        </Border>
    </ControlTemplate>


    <!--Normal Header-->
    <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalTemplate}" TargetType="{x:Type dxschd:HeaderControl}">
        <Border>
            <Border.Resources>
                <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=TemplateCore}" TargetType="{x:Type ContentControl}" >
                    <dxschd:HeaderBorder Background="#FFA1A1A1" DefaultPadding="1,0,1,0" DefaultCornerRadius="0" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}">
                        <dxschd:HeaderBorder Background="#FF6E6E6E" MinHeight="24" SnapsToDevicePixels="True" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" DefaultCornerRadius="0">
                            <ContentPresenter HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="12,0,12,0"/>
                        </dxschd:HeaderBorder>
                    </dxschd:HeaderBorder>
                </ControlTemplate>
            </Border.Resources>
            <ContentControl x:Name="HeaderContent" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Template="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=TemplateCore}}" SnapsToDevicePixels="True" UseLayoutRounding="True" dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}">
            </ContentControl>
        </Border>
    </ControlTemplate>

    <!--Alternate Header-->
    <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=HeaderAlternateTemplate}" TargetType="{x:Type dxschd:HeaderControl}">
        <Border>
            <Border.Resources>
                <ControlTemplate x:Key="{dxscht:SchedulerViewThemeKey ResourceKey=TemplateCore}" TargetType="{x:Type ContentControl}" >
                    <dxschd:HeaderBorder Background="#FFA1A1A1" DefaultPadding="1,0,1,0" DefaultCornerRadius="0" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}">
                        <dxschd:HeaderBorder Background="#FF6E6E6E" MinHeight="24" SnapsToDevicePixels="True" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" DefaultCornerRadius="0">
                            <ContentPresenter HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="12,0,12,0"/>
                        </dxschd:HeaderBorder>
                    </dxschd:HeaderBorder>
                </ControlTemplate>
            </Border.Resources>
            <ContentControl x:Name="HeaderContent" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Template="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=TemplateCore}}" SnapsToDevicePixels="True" UseLayoutRounding="True" dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}">
            </ContentControl>
        </Border>
    </ControlTemplate>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Scheduler.Drawing.HeaderControl}" TargetType="{x:Type dxschd:HeaderControl}">
        <Setter Property="NormalTemplate" Value="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderNormalTemplate}}"/>
        <Setter Property="AlternateTemplate" Value="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=HeaderAlternateTemplate}}"/>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Scheduler.Drawing.VerticalHeaderControl}" TargetType="{x:Type dxschd:VerticalHeaderControl}">
        <Setter Property="NormalTemplate" Value="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=VerticalHeaderNormalTemplate}}"/>
        <Setter Property="AlternateTemplate" Value="{DynamicResource {dxscht:SchedulerViewThemeKey ResourceKey=VerticalHeaderNormalTemplate}}"/>
    </Style>

    <!--Normal CellHeader-->
    <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=CellHeaderNormalTemplate}" TargetType="{x:Type Control}">
        <Grid Background="Transparent" >
            <dxschd:MonthViewDayHeaderControl x:Name="MonthViewDayHeaderControl" Margin="5,0,5,0" Date="{Binding IntervalStart}" DateFormats="{Binding DateFormats}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" SnapsToDevicePixels="True">
                <TextBlock SnapsToDevicePixels="True" Opacity="0.5" HorizontalAlignment="Right" Foreground="Black"/>
            </dxschd:MonthViewDayHeaderControl>
        </Grid>
    </ControlTemplate>

    <!--Alternate CellHeader-->
    <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=CellHeaderAlternateTemplate}" TargetType="{x:Type Control}">
        <Grid Background="#A5DADADA" >
            <dxschd:MonthViewDayHeaderControl x:Name="MonthViewDayHeaderControl" Margin="5,0,5,0" Date="{Binding IntervalStart}" DateFormats="{Binding DateFormats}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" SnapsToDevicePixels="True">
                <TextBlock SnapsToDevicePixels="True" Foreground="White" HorizontalAlignment="Right"/>
            </dxschd:MonthViewDayHeaderControl>
        </Grid>
    </ControlTemplate>

    <!--Normal WeekCell-->
    <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekCellNormalTemplate}" TargetType="{x:Type dxschd:VisualDateCell}">
        <Grid SnapsToDevicePixels="True" UseLayoutRounding="True">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <dxschd:MultiColorBorder Grid.Row="0" Grid.RowSpan="2" DefaultBorderThickness="1,0,1,1" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" OuterBorderBrush="{DynamicResource {dxscht:SchedulerControlThemeKey ResourceKey=DefaultBorderBrush}}" InnerBorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"/>
            <dxschd:CellContent x:Name="PART_CONTENT" Background="Transparent" Grid.Row="1" dxsch:SchedulerControl.HitTestType="Cell" IsHitTestVisible="True" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"/>
            <dxschd:VisualDateCellHeader IsAlternate="{Binding Content.IsAlternate, RelativeSource={RelativeSource TemplatedParent}}" Content="{TemplateBinding Content}" dxsch:SchedulerControl.HitTestType="DayHeader" Style="{Binding Path=Content.View.HorizontalWeekDateHeaderStyle, RelativeSource={RelativeSource TemplatedParent}}" dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"/>
        </Grid>
    </ControlTemplate>

    <!--Alternate  WeekCell-->
    <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=WeekCellAlternateTemplate}" TargetType="{x:Type dxschd:VisualDateCell}">
        <Grid SnapsToDevicePixels="True" UseLayoutRounding="True">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>

            <dxschd:MultiColorBorder Grid.Row="0" Grid.RowSpan="2" DefaultBorderThickness="1,0,1,1" 
                                                ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                                                OuterBorderBrush="{DynamicResource {dxscht:SchedulerControlThemeKey ResourceKey=DefaultBorderBrush}}" InnerBorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"/>
            <dxschd:CellContent
                                x:Name="PART_CONTENT"
                                Background="#A5DADADA" Grid.Row="1"                                
                                dxsch:SchedulerControl.HitTestType="Cell"
                                IsHitTestVisible="True"  
                                ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"/>
            <dxschd:VisualDateCellHeader
                                IsAlternate="{Binding Content.IsAlternate, RelativeSource={RelativeSource TemplatedParent}}"
                                Content="{TemplateBinding Content}"                                
                                dxsch:SchedulerControl.HitTestType="DayHeader"
                                Style="{Binding Path=Content.View.HorizontalWeekDateHeaderStyle, RelativeSource={RelativeSource TemplatedParent}}"
                                dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                                />
        </Grid>
    </ControlTemplate>

    <!--Normal VerticalWeekCell-->
    <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=VerticalWeekCellNormalTemplate}" TargetType="{x:Type dxschd:VisualDateCell}">
        <Grid SnapsToDevicePixels="True" UseLayoutRounding="True">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <!--<dxschd:CellBorder Grid.Row="0" Grid.RowSpan="2" DefaultBorderThickness="1" DefaultMargin="1,0,0,0" BorderBrush="{TemplateBinding BorderBrush}" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True" UseLayoutRounding="True"/>-->
            <dxschd:MultiColorBorder Grid.Row="0" Grid.RowSpan="2" DefaultBorderThickness="1,0,1,1" 
                                                ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                                                OuterBorderBrush="{DynamicResource {dxscht:SchedulerControlThemeKey ResourceKey=DefaultBorderBrush}}" InnerBorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"/>
            <dxschd:CellContent
                            x:Name="PART_CONTENT"
                            Background="Transparent" Grid.Row="1"                                
                            dxsch:SchedulerControl.HitTestType="Cell"
                            IsHitTestVisible="True" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"/>
            <dxschd:VisualDateCellHeader
                            IsAlternate="{Binding Content.IsAlternate, RelativeSource={RelativeSource TemplatedParent}}"
                            Content="{TemplateBinding Content}"                                
                            dxsch:SchedulerControl.HitTestType="DayHeader"
                            Style="{Binding Path=Content.View.VerticalWeekDateHeaderStyle, RelativeSource={RelativeSource TemplatedParent}}"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                            />
        </Grid>
    </ControlTemplate>


    <!--Alternate VerticalWeekCell-->
    <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=VerticalWeekCellAlternateTemplate}" TargetType="{x:Type dxschd:VisualDateCell}">
        <Grid SnapsToDevicePixels="True" UseLayoutRounding="True">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <!--<dxschd:CellBorder Grid.Row="0" Grid.RowSpan="2" DefaultBorderThickness="1" DefaultMargin="1,0,0,0" BorderBrush="{TemplateBinding BorderBrush}" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True" UseLayoutRounding="True"/>-->
            <dxschd:MultiColorBorder Grid.Row="0" Grid.RowSpan="2" DefaultBorderThickness="1,0,1,1" 
                                                ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                                                OuterBorderBrush="{DynamicResource {dxscht:SchedulerControlThemeKey ResourceKey=DefaultBorderBrush}}" InnerBorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"/>
            <dxschd:CellContent
                            x:Name="PART_CONTENT"
                            Background="Transparent" Grid.Row="1"                                
                            dxsch:SchedulerControl.HitTestType="Cell"
                            IsHitTestVisible="True" ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"/>
            <dxschd:VisualDateCellHeader
                            IsAlternate="{Binding Content.IsAlternate, RelativeSource={RelativeSource TemplatedParent}}"
                            Content="{TemplateBinding Content}"                                
                            dxsch:SchedulerControl.HitTestType="DayHeader"
                            Style="{Binding Path=Content.View.VerticalWeekDateHeaderStyle, RelativeSource={RelativeSource TemplatedParent}}"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}}"
                            />
        </Grid>
    </ControlTemplate>


    <!--Normal VerticalCellHeader-->
    <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=VerticalCellHeaderNormalTemplate}" TargetType="{x:Type dxschd:VisualDateCellHeader}">
        <Grid Background="Transparent">
            <dxschd:HeaderControl
                            HorizontalAlignment="Stretch" 
                            HorizontalContentAlignment="Right"
                            IsAlternate="{Binding IsAlternate}"
                            ResourceColor="{Binding Content.Brushes.ResourceHeader, RelativeSource={RelativeSource TemplatedParent}}"     
                            Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource elementPositionVerticalWeekHeaderConverter}}"
                            />
        </Grid>
    </ControlTemplate>

    <!--Alternate VerticalCellHeader-->
    <ControlTemplate x:Key="{dxscht:WeekViewThemeKey ResourceKey=VerticalCellHeaderAlternateTemplate}" TargetType="{x:Type dxschd:VisualDateCellHeader}">
        <Grid Background="Transparent">
            <dxschd:HeaderControl
                            HorizontalAlignment="Stretch" 
                            HorizontalContentAlignment="Right"
                            IsAlternate="{Binding IsAlternate}"
                            ResourceColor="{Binding Content.Brushes.ResourceHeader, RelativeSource={RelativeSource TemplatedParent}}"     
                            Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"
                            dxschd:SchedulerItemsControl.ElementPosition="{Binding Path=(dxschd:SchedulerItemsControl.ElementPosition), RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource elementPositionVerticalWeekHeaderConverter}}"
                            />
        </Grid>
    </ControlTemplate>


</ResourceDictionary>