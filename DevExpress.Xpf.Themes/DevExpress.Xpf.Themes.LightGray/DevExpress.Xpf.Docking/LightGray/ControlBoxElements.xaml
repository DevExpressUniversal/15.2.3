                                    <Path x:Name="path1" Stretch="Fill" Fill="#FF7B808E" RenderTransformOrigin="0.5,0.5" Data="F1 M 555,361.5C 555,365.642 551.642,369 547.5,369C 543.358,369 540,365.642 540,361.5C 540,357.358 543.358,354 547.5,354C 551.642,354 555,357.358 555,361.5 Z M 542,361.5C 542,364.538 544.462,367 547.5,367C 550.537,367 553,364.538 553,361.5C 553,358.463 550.537,356 547.5,356C 544.462,356 542,358.463 542,361.5 Z M 548,365L 547,365L 544,359L 551,359L 548,365 Z">
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="180" />
                                        </Path.RenderTransform>
