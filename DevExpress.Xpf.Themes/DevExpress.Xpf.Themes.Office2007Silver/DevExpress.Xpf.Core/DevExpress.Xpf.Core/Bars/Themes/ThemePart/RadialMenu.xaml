<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys">

    <SolidColorBrush x:Key="RadialMenuLevelControl_BackColor" Color="#FFA4AAB2" />
    <SolidColorBrush x:Key="RadialMenuLevelControl_InnerColor" Color="#FFFFFFFF" />
    
    <SolidColorBrush x:Key="CenterButton_StrokeColor" Color="#FFA4AAB2" />
    <SolidColorBrush x:Key="CenterButton_InnerColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="CenterButton_HoverColor" Color="#3FA4AAB2" />

    <SolidColorBrush x:Key="BarItemInRadialMenu_MenuColor" Color="#FF727880" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathHover" Color="#FF40464E" /> 
    
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=NormalImageColorizeColor}">#FF000000</Color>  
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=HoverImageColorizeColor}">#FF000000</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=PressedImageColorizeColor}">#FF000000</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=DisabledImageColorizeColor}">#3F000000</Color>
  
</ResourceDictionary>  