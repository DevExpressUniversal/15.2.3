                                <GradientStop Color="#FFF8EBB2" Offset="1" cs:Name="GradientStop_0013" />
                                <GradientStop Color="#FFFFD86C" Offset="0.5" cs:Name="GradientStop_0014" />
                            </LinearGradientBrush>
