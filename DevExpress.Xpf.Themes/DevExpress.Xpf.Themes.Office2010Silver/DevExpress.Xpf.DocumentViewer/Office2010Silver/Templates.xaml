    <ControlTemplate x:Key="{dxdvt:DocumentViewerThemeKeyExtension ResourceKey=DocumentViewerBarTemplate}" TargetType="{x:Type ContentControl}">
        <Grid Margin="0,0,0,3">
            <Grid.Effect>
                <DropShadowEffect Direction="270" ShadowDepth="1" BlurRadius="3" Opacity="0.14" />
            </Grid.Effect>
            <Border BorderThickness="0,0,0,1">
                <Border.BorderBrush>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_1492">
                        <GradientStop Color="#FFC0C4C9" Offset="0" cs:Name="GradientStop_0003" />
                        <GradientStop Color="#C0C4C9" Offset="1" cs:Name="GradientStop_0004" />
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_1493">
                        <GradientStop Color="#FFFFFF" Offset="0" cs:Name="GradientStop_0005" />
                        <GradientStop Color="#E9EDF1" Offset="1" cs:Name="GradientStop_0006" />
                    </LinearGradientBrush>
                </Border.Background>
                <ContentPresenter Margin="12,4,4,4" />
            </Border>
        </Grid>
    </ControlTemplate>
