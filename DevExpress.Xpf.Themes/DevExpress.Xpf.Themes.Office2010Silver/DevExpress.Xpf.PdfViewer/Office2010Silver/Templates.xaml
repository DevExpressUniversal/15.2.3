        <GradientStop Color="#FFC0C4C9" Offset="1" cs:Name="GradientStop_0002" />
    </LinearGradientBrush>
    <ControlTemplate TargetType="{x:Type dxpdf:PdfPageControlItem}" x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPageControlItemTemplate}">
        <Grid>
            <Grid x:Name="Shadow">
                <Border BorderBrush="#FF000000" BorderThickness="1" Margin="-1" Opacity="0.05" cs:Name="Border_0001" />
                <Border BorderBrush="#FF000000" BorderThickness="1" Margin="-2" CornerRadius="1" Opacity="0.03" cs:Name="Border_0002" />
                <Border BorderBrush="#FF000000" BorderThickness="2" Margin="-3" CornerRadius="2" Opacity="0.03" cs:Name="Border_0003" />
                <Border BorderBrush="#FF000000" BorderThickness="3" Margin="-4" CornerRadius="3" Opacity="0.02" cs:Name="Border_0004" />
                <Border BorderBrush="#FF000000" BorderThickness="4" Margin="-5" CornerRadius="4" Opacity="0.02" cs:Name="Border_0005" />
                <Border BorderBrush="#FF000000" BorderThickness="5" Margin="-6" CornerRadius="5" Opacity="0.02" cs:Name="Border_0006" />
                <Border BorderBrush="#FF000000" BorderThickness="6" Margin="-7" CornerRadius="6" Opacity="0.01" cs:Name="Border_0007" />
                <Border BorderBrush="#FF000000" BorderThickness="7" Margin="-8" CornerRadius="7" Opacity="0.01" cs:Name="Border_0008" />
                <Border BorderBrush="#FF000000" BorderThickness="8" Margin="-9" CornerRadius="8" Opacity="0.01" cs:Name="Border_0009" />
                <Border BorderBrush="#26000000" BorderThickness="1" Margin="-1" cs:Name="Border_0010" />
            </Grid>
            <Border x:Name="Selection" BorderBrush="#FFFFE277" BorderThickness="3" Margin="-1" Opacity="0" cs:Name="Border_0011" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter TargetName="Selection" Property="Opacity" Value="1" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <DataTemplate x:Key="{themes:PdfViewerThemeKey ResourceKey=SelectionCanvasContentTemplate}">
        <Border VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Background="#45007FFF" BorderBrush="#FF007FFF" BorderThickness="1" cs:Name="Border_0013" />
    </DataTemplate>
</ResourceDictionary>