    </ResourceDictionary.MergedDictionaries>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=HighlightSearchTextColor}" cs:Name="Color_0001">#596098C0</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=HighlightSelectionColor}" cs:Name="Color_0006">#3F0133AF</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=CaretColor}" cs:Name="Color_0007">#FF000000</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenCaptionForegroundColor}" cs:Name="Color_0002">#BFFFFFFF</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenDocumentLinkForegroundColor}" cs:Name="Color_0003">#BFFFE277</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenOpenDocumentForegroundColor}" cs:Name="Color_0004">#BFFFFFFF</Color>
    <Color x:Key="{themes:PdfViewerThemeKey ResourceKey=LoadingDocumentLinkForegroundColor}" cs:Name="Color_0005">#FF0072C6</Color>
    <sys:Double x:Key="{themes:PdfViewerThemeKey ResourceKey=PrintDialogRightColumnWidth}">120</sys:Double>
    <GridLength x:Key="{themes:PdfViewerThemeKey ResourceKey=PrintDialogRightColumnGridLength}">120</GridLength>
    <LinearGradientBrush x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlBackgroundBrush}" EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_1491">
