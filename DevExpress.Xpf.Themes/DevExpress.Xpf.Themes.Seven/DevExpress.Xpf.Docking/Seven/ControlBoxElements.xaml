    </DataTemplate>

    <DataTemplate x:Key="{dxt:GlyphElementsThemeKey ResourceKey=MDIButtonMinimize}">
        <Grid Width="11" Height="9" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="1,0,0,0">
            <Rectangle Fill="#FFE4EDF9" StrokeThickness="0" RadiusX="1" RadiusY="1" Width="11" Height="5" VerticalAlignment="Bottom" />
            <Rectangle Fill="#FF535666" StrokeThickness="0" Margin="0,0,0,1" Width="9" Height="3" VerticalAlignment="Bottom"/>
        </Grid>
    </DataTemplate>

    <!-- GroupBox Button -->
    <Style TargetType="ve:GroupBoxControlBoxButton" x:Key="GroupPane_ExpandButton">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ve:GroupBoxControlBoxButton">
                        <Grid x:Name="Button" ve:DockPane.HitTestType="ExpandButton" Width="20" Height="20" Background="Transparent">
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="CommonStates">
                                    <VisualState x:Name="Normal"/>
                                    <VisualState x:Name="MouseOver">
                                        <Storyboard>
                                            <ColorAnimation Storyboard.TargetName="GlyphClosePath1" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Duration="0" To="#FF1CC4F7"/>
                                            <ColorAnimation Storyboard.TargetName="GlyphClosePath2" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Duration="0" To="#FF1CC4F7"/>
                                            <ColorAnimation Storyboard.TargetName="GlyphClosePath3" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Duration="0" To="#FFCDEEFB"/>
                                            <ColorAnimation Storyboard.TargetName="GlyphOpenPath1" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Duration="0" To="#FF1CC4F7"/>
                                            <ColorAnimation Storyboard.TargetName="GlyphOpenPath2" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Duration="0" To="#FF1CC4F7"/>
                                            <ColorAnimation Storyboard.TargetName="GlyphOpenPath3" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Duration="0" To="#FFCDEEFB"/>
                                            <DoubleAnimation Storyboard.TargetName="HoverHighlight" Storyboard.TargetProperty="Opacity" Duration="0" To="0.15"/>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="ExpandedStates">
                                    <VisualState x:Name="Expanded"/>
                                    <VisualState x:Name="Collapsed">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="GlyphClose" Storyboard.TargetProperty="(Control.Visibility)" dxcn:ValueSetter.Visibility="Collapsed"/>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="GlyphOpen" Storyboard.TargetProperty="(Control.Visibility)" dxcn:ValueSetter.Visibility="Visible"/>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <Grid SnapsToDevicePixels="True">
                                <Border x:Name="HoverHighlight" Width="14" Height="14" Opacity="0">
                                    <Border.Background>
                                        <RadialGradientBrush>
                                            <GradientStop Color="#FF1CC4F7" Offset="0"/>
                                            <GradientStop Color="#001CC4F7" Offset="1"/>
                                        </RadialGradientBrush>
                                    </Border.Background>
                                </Border>
                                <Grid x:Name="GlyphClose" Width="10" Height="5" Margin="0,0,0,1">
                                    <Path x:Name="GlyphClosePath1" Stretch="Fill" Fill="#FF303336" Data="F1 M 65,77L 55,77L 60,72L 65,77 Z "/>
                                    <Path x:Name="GlyphClosePath2" Stretch="Fill" Fill="#FF303336" Data="F1 M 65,77L 55,77L 60,72L 65,77 Z "/>
                                    <Path x:Name="GlyphClosePath3" Stretch="Fill" Fill="#FF87898A" Data="F1 M 63,76L 57,76L 60,73L 63,76 Z " Margin="2,1,2,1"/>
                                </Grid>
                                <Grid x:Name="GlyphOpen" Width="10" Height="5" Visibility="Collapsed">
                                    <Path x:Name="GlyphOpenPath1" Stretch="Fill" Fill="#FF818181" Data="F1 M 65,84L 55,84L 60,89L 65,84 Z "/>
                                    <Path x:Name="GlyphOpenPath2" Stretch="Fill" Fill="#FF818181" Data="F1 M 65,84L 55,84L 60,89L 65,84 Z "/>
                                    <Path x:Name="GlyphOpenPath3" Stretch="Fill" Fill="#FFF8F8F8" Data="F1 M 63,85L 57,85L 60,88L 63,85 Z " Margin="2,1,2,1"/>
                                </Grid>
                            </Grid>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <DataTemplate x:Key="{dxt:GroupPaneElementsThemeKey ResourceKey=ExpandButton}">
        <ve:GroupBoxControlBoxButton x:Name="PART_Button" Style="{StaticResource GroupPane_ExpandButton}"/>
    </DataTemplate>

    <!-- LayoutPanel Buttons -->
    <Style x:Key="DockPaneControlBoxBorder" TargetType="ve:ControlBoxButtonBorder">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ve:ControlBoxButtonBorder">
                    <Grid x:Name="Root">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="HoverState" Storyboard.TargetProperty="Opacity" Duration="0" To="1"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="PressedState" Storyboard.TargetProperty="Opacity" Duration="0" To="1"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" Duration="0" To="0.35"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="HoverState" Background="#FF7DA2CE" CornerRadius="2" Padding="1" Opacity="0">
                            <Border Padding="1" CornerRadius="1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFEBF4FD" Offset="0"/>
                                        <GradientStop Color="#FFDBEAFD" Offset="1"/>
                                    </LinearGradientBrush>
                                </Border.Background>
                                <Border CornerRadius="1">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFDCEBFC" Offset="0"/>
                                            <GradientStop Color="#FFC1DBFC" Offset="1"/>
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                        </Border>
                        <Border x:Name="PressedState" Background="#FF6482AC" CornerRadius="2" Padding="1" Opacity="0">
                            <Border Padding="1" CornerRadius="1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFE4EEF8" Offset="0"/>
                                        <GradientStop Color="#FFD2E3F6" Offset="1"/>
                                    </LinearGradientBrush>
                                </Border.Background>
                                <Border CornerRadius="1">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFCFE0F4" Offset="0"/>
                                            <GradientStop Color="#FFB6CFF2" Offset="1"/>
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                        </Border>
                        <Grid Width="17" Height="17">
                            <ContentPresenter />
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DockPane_PinButton" TargetType="ve:DockPaneControlBoxButton">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ve:DockPaneControlBoxButton">
                    <ve:ControlBoxButtonBorder Style="{StaticResource DockPaneControlBoxBorder}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver"/>
                                <VisualState x:Name="Pressed"/>
                                <VisualState x:Name="Disabled"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="PinnedStates">
                                <VisualState x:Name="Unpinned" />
                                <VisualState x:Name="Pinned">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Visibility)" Storyboard.TargetName="PinnedPath">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Visibility)" Storyboard.TargetName="UnpinnedPath">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid>
                            <Path x:Name="PinnedPath" Fill="#FF535666" Stretch="Fill"
                                      Data="F1M1252,785L1252,786 1257,786 1257,791 1252,791 1252,792 1251,792 1251,789 1248,789 1248,788 1251,788 1251,785 1252,785z M1256,787L1252,787 1252,788 1256,788 1256,787z"
                                      Width="9" Height="7" HorizontalAlignment="Center" VerticalAlignment="Center"
                                      Visibility="Collapsed"/>
                            <Path x:Name="UnpinnedPath" Fill="#FF535666" Stretch="Fill"
                                      Data="F1M1265,788L1266,788 1266,783 1271,783 1271,788 1272,788 1272,789 1269,789 1269,792 1268,792 1268,789 1265,789 1265,788z M1267,784L1267,788 1268,788 1268,784 1267,784z"
                                      Margin="0,1,0,0" Width="7" Height="9" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>
                    </ve:ControlBoxButtonBorder>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ve:DockPane.HitTestType" Value="PinButton" />
    </Style>

