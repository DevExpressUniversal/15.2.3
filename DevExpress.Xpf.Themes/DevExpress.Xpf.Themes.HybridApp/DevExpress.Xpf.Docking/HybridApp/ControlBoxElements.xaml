    <Style x:Key="DockPane_HideButton" TargetType="ve:DockPaneControlBoxButton">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ve:DockPaneControlBoxButton">
                    <ve:ControlBoxButtonBorder Style="{StaticResource DockPaneControlBoxBorder}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="DockStates">
                                <VisualState x:Name="Left" />
                                <VisualState x:Name="Right">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(UIElement.RenderTransform).Children[1].ScaleX" Duration="0" To="-1"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Top">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(UIElement.RenderTransform).Children[0].Angle" Duration="0" To="90"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Bottom">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(UIElement.RenderTransform).Children[0].Angle" Duration="0" To="270"/>
                                        <DoubleAnimation Storyboard.TargetName="Glyph" Storyboard.TargetProperty="(UIElement.RenderTransform).Children[1].ScaleX" Duration="0" To="-1"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Path x:Name="Glyph" Stretch="Fill" Fill="#FF404040" Data="F1 M 2,9.5L 6,18.5L 6,0.5L 2,9.5 Z M 8,9.5L 12,18.5L 12,0.5L 8,9.5 Z " Margin="4" RenderTransformOrigin="0.5,0.5">
                            <Path.RenderTransform>
                                <TransformGroup>
                                    <RotateTransform />
                                    <ScaleTransform />
                                </TransformGroup>
                            </Path.RenderTransform>
                        </Path>
                    </ve:ControlBoxButtonBorder>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ve:DockPane.HitTestType" Value="HideButton" />
    </Style>
