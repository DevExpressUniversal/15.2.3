    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.HybridApp.v15.2;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/HybridApp/ButtonEdit.xaml" />
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.HybridApp.v15.2;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/HybridApp/Brushes.xaml" />
