<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys">

    <SolidColorBrush x:Key="RadialMenuLevelControl_BackColor" Color="#FF999999" />
    <SolidColorBrush x:Key="RadialMenuLevelControl_InnerColor" Color="#FFFFFFFF" />

    <SolidColorBrush x:Key="CenterButton_StrokeColor" Color="#FF3F3F3F" />
    <SolidColorBrush x:Key="CenterButton_InnerColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="CenterButton_HoverColor" Color="#7F999999" />

    <SolidColorBrush x:Key="BarItemInRadialMenu_MenuColor" Color="#FF3F3F3F" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathHover" Color="#FF3F3F3F" />

    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=NormalImageColorizeColor}">#FF3F3F3F</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=HoverImageColorizeColor}">#FF3F3F3F</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=PressedImageColorizeColor}">#FF3F3F3F</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=DisabledImageColorizeColor}">#3F3F3F3F</Color>
</ResourceDictionary>