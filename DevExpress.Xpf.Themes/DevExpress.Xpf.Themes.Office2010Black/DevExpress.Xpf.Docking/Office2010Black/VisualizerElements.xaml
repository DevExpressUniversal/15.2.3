    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}" TargetType="{x:Type ve:DocumentSelectorContainer}">
        <Style.Setters>
            <Setter Property="Foreground" Value="#FFE0E0E0" cs:Name="Setter_0001" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
                        <Grid>
                            <Grid x:Name="Shadow" Margin="2,2,-3,-3">
                                <Border Background="#FF000000" CornerRadius="8" Opacity="0.01" cs:Name="Border_0045" />
                                <Border Background="#FF000000" CornerRadius="7" Margin="1" Opacity="0.02" cs:Name="Border_0046" />
                                <Border Background="#FF000000" CornerRadius="6" Margin="2" Opacity="0.03" cs:Name="Border_0047" />
                            </Grid>
                            <Border x:Name="Window" Background="#FF484848" Margin="1" CornerRadius="5" cs:Name="Border_0048">
                                <Border Background="#FF939393" Margin="1" CornerRadius="4" cs:Name="Border_0049">
                                    <Grid Margin="1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Border x:Name="Header" CornerRadius="3,3,0,0">
                                            <Border.Background>
                                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0433">
                                                    <GradientStop Color="#FF797979" Offset="0" cs:Name="GradientStop_0001" />
                                                    <GradientStop Color="#FF717171" Offset="1" cs:Name="GradientStop_0002" />
                                                </LinearGradientBrush>
                                            </Border.Background>
                                            <StackPanel Margin="12,8,12,8" VerticalAlignment="Center" HorizontalAlignment="Left" Orientation="Horizontal">
                                                <Image Margin="0,0,6,0" Stretch="None" VerticalAlignment="Top" HorizontalAlignment="Left" Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}" />
                                                <StackPanel>
                                                    <TextBlock TextWrapping="Wrap" VerticalAlignment="Bottom" HorizontalAlignment="Left" FontWeight="Bold" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}" />
                                                    <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}" />
                                                </StackPanel>
                                            </StackPanel>
                                        </Border>
                                        <Border x:Name="Content" Background="#FF939393" Grid.Row="1" cs:Name="Border_0050">
                                            <ContentPresenter x:Name="PART_ContainerContent" Content="{Binding}" Margin="12" />
                                        </Border>
                                        <Border x:Name="Footer" Background="#FF717171" CornerRadius="0,0,3,3" MinHeight="24" Grid.Row="2" cs:Name="Border_0051">
                                            <ContentPresenter HorizontalAlignment="Left" VerticalAlignment="Center" Margin="12,0,0,0" Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}" />
                                        </Border>
                                    </Grid>
                                </Border>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
        <Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0434">
                            <GradientStop Color="#BFD7A820" Offset="0" cs:Name="GradientStop_0003" />
                            <GradientStop Color="#BFB89407" Offset="1" cs:Name="GradientStop_0004" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border Margin="1" CornerRadius="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0435">
                                <GradientStop Color="#FFFFFAE6" Offset="0" cs:Name="GradientStop_0005" />
                                <GradientStop Color="#FFFFFFED" Offset="1" cs:Name="GradientStop_0006" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border Margin="1" CornerRadius="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0436">
                                    <GradientStop Color="#FFFFEEA8" Offset="0" cs:Name="GradientStop_0007" />
                                    <GradientStop Color="#FFFFFCE0" Offset="1" cs:Name="GradientStop_0008" />
                                    <GradientStop Color="#FFFFE277" Offset="0.5" cs:Name="GradientStop_0009" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="SelectedState" Opacity="0">
                <Border CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0437">
                            <GradientStop Color="#BFA26F16" Offset="0" cs:Name="GradientStop_0010" />
                            <GradientStop Color="#BFA2832C" Offset="1" cs:Name="GradientStop_0011" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border Margin="1" CornerRadius="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_0438">
                                <GradientStop Color="#FFFFE575" Offset="0" cs:Name="GradientStop_0012" />
                                <GradientStop Color="#FFFFF37F" Offset="1" cs:Name="GradientStop_0013" />
                                <GradientStop Color="#FFFFD86C" Offset="0.5" cs:Name="GradientStop_0014" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Border Padding="{TemplateBinding Padding}">
                <ve:CaptionControl x:Name="caption" DataContext="{Binding}" Margin="3,2,3,2" />
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Opacity" TargetName="SelectedState" Value="1" />
                <Setter Property="Foreground" TargetName="caption" Value="#FF000000" cs:Name="Setter_0002" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity" TargetName="HoverState" Value="1" />
                <Setter Property="Foreground" TargetName="caption" Value="#FF000000" cs:Name="Setter_0003" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <DataTemplate x:Key="{dxt:DockPaneElementsThemeKey ResourceKey=ContentPreview}">
        <Grid Margin="0,0,-2,0" VerticalAlignment="Top" SnapsToDevicePixels="True">
