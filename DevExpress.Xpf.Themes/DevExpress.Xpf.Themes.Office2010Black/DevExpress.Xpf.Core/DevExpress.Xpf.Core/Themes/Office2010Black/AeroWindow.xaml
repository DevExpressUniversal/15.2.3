<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <sys:Double x:Key="{dxt:DXWindowThemeKey ResourceKey=AeroWindowBorderSize}">8</sys:Double>
	<Thickness x:Key="{dxt:DXWindowThemeKey ResourceKey=AeroWindowResizeBorderThickness}">9</Thickness>
    <SolidColorBrush x:Key="{dxt:DXWindowThemeKey ResourceKey=AeroWindowContainerBackground}" Color="#FF838383" cs:Name="SolidColorBrush_0001" />
    <DataTemplate x:Key="{dxt:DXWindowThemeKey ResourceKey=AeroWindowCaptionTemplate}">
        <Grid Height="Auto" MinHeight="29">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto">
                </ColumnDefinition>
                <ColumnDefinition>
                </ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Image x:Name="PART_Icon" Width="Auto" Height="Auto" Margin="6,6,0,3" Stretch="None" Grid.Column="0" VerticalAlignment="Top" Source="{Binding Path=(dxc:FloatingContainer.FloatingContainer).Icon, RelativeSource={RelativeSource AncestorType=ContentPresenter}}" />
            <dxc:FloatingContainerHeaderPanel Grid.Column="1">
                <Grid Margin="6,3,6,3" HorizontalAlignment="Left" VerticalAlignment="Center" IsHitTestVisible="False">
                    <Border Background="#FFFFFFFF" CornerRadius="6" Opacity="0.3" Margin="-2,0,-2,0" cs:Name="Border_0001">
                        <Border.Effect>
                            <BlurEffect Radius="10" />
                        </Border.Effect>
                    </Border>
                    <TextBlock x:Name="PART_Text" Background="{x:Null}" Foreground="#FF000000" Text="{Binding Path=(dxc:FloatingContainer.FloatingContainer).Caption, RelativeSource={RelativeSource AncestorType=ContentPresenter}}" cs:Name="TextBlock_0001" />
                </Grid>
            </dxc:FloatingContainerHeaderPanel>
        </Grid>
        <DataTemplate.Triggers>
            <Trigger Property="Source" SourceName="PART_Icon" Value="{x:Null}">
                <Setter TargetName="PART_Icon" Property="Visibility" Value="Collapsed" />
            </Trigger>
        </DataTemplate.Triggers>
    </DataTemplate>
    <ControlTemplate x:Key="{dxt:DXWindowThemeKey ResourceKey=AeroWindowDragWidgetTemplate}" TargetType="{x:Type Thumb}">
        <Border Background="Transparent" cs:Name="Border_0002">
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxt:DXWindowThemeKey ResourceKey=AeroWindowSizeGripTemplate}" TargetType="{x:Type Thumb}">
        <Grid Background="Transparent" Width="12" Height="12" cs:Name="Grid_0001">
            <Path Stretch="Fill" Fill="#FF484848" Data="F1 M 309.5,217C 310.329,217 311,217.672 311,218.5C 311,219.328 310.329,220 309.5,220C 308.672,220 308,219.328 308,218.5C 308,217.672 308.672,217 309.5,217 Z M 301.5,225C 302.328,225 303,225.672 303,226.5C 303,227.329 302.328,228 301.5,228C 300.672,228 300,227.329 300,226.5C 300,225.672 300.672,225 301.5,225 Z M 309.5,221C 310.328,221 311,221.672 311,222.5C 311,223.328 310.328,224 309.5,224C 308.672,224 308,223.328 308,222.5C 308,221.672 308.672,221 309.5,221 Z M 305.5,221C 306.328,221 307,221.672 307,222.5C 307,223.328 306.328,224 305.5,224C 304.672,224 304,223.328 304,222.5C 304,221.672 304.672,221 305.5,221 Z M 309.5,225C 310.329,225 311,225.672 311,226.5C 311,227.328 310.328,228 309.5,228C 308.672,228 308,227.328 308,226.5C 308,225.672 308.672,225 309.5,225 Z M 305.5,225C 306.329,225 307,225.672 307,226.5C 307,227.328 306.329,228 305.5,228C 304.672,228 304,227.328 304,226.5C 304,225.672 304.672,225 305.5,225 Z" Margin="0,0,1,1" Width="11" Height="11" UseLayoutRounding="False" cs:Name="Path_0001" />
            <Path Stretch="Fill" Fill="#7FFFFFFF" Data="F1 M 310.5,218C 311.328,218 312,218.672 312,219.5C 312,220.328 311.328,221 310.5,221C 309.672,221 309,220.328 309,219.5C 309,218.672 309.672,218 310.5,218 Z M 302.5,226C 303.328,226 304,226.672 304,227.5C 304,228.329 303.328,229 302.5,229C 301.671,229 301,228.329 301,227.5C 301,226.672 301.671,226 302.5,226 Z M 310.5,222C 311.328,222 312,222.672 312,223.5C 312,224.329 311.328,225 310.5,225C 309.671,225 309,224.329 309,223.5C 309,222.672 309.671,222 310.5,222 Z M 306.5,222C 307.328,222 308,222.672 308,223.5C 308,224.328 307.328,225 306.5,225C 305.672,225 305,224.328 305,223.5C 305,222.672 305.671,222 306.5,222 Z M 310.5,226C 311.328,226 312,226.672 312,227.5C 312,228.328 311.328,229 310.5,229C 309.672,229 309,228.328 309,227.5C 309,226.672 309.672,226 310.5,226 Z M 306.5,226C 307.328,226 308,226.672 308,227.5C 308,228.328 307.328,229 306.5,229C 305.672,229 305,228.328 305,227.5C 305,226.672 305.672,226 306.5,226 Z" Margin="1,1,0,0" Width="11" Height="11" UseLayoutRounding="False" Opacity="0.5" cs:Name="Path_0002" />
        </Grid>
    </ControlTemplate>
    <DataTemplate x:Key="{dxt:DXWindowThemeKey ResourceKey=AeroWindowTemplate}">
        <Grid>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <dxc:DXWindowHeader x:Name="PART_Header" Grid.Row="0" CornerRadius="0" Background="Transparent" Panel.ZIndex="99" cs:Name="DXWindowHeader_0001">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <ContentPresenter x:Name="PART_CaptionContentPresenter" Panel.ZIndex="100" Grid.Column="0" Content="{x:Null}" ContentTemplate="{DynamicResource {dxt:DXWindowThemeKey IsVisibleInBlend=True, ResourceKey=AeroWindowCaptionTemplate}}" />
                        <Thumb Template="{DynamicResource {dxt:DXWindowThemeKey IsVisibleInBlend=True, ResourceKey=AeroWindowDragWidgetTemplate}}" x:Name="PART_DragWidget" Grid.Column="0" />
                        <StackPanel x:Name="stackPanel" Grid.Column="1" Orientation="Horizontal" VerticalAlignment="Top" Height="{Binding Path=(dxc:DXWindow.AeroControlBoxHeight), RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxc:DXWindow}}" Width="{Binding Path=(dxc:DXWindow.AeroControlBoxWidth), RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxc:DXWindow}}" Margin="{Binding Path=(dxc:DXWindow.AeroControlBoxMargin), RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxc:DXWindow}}">
                            <Button Template="{x:Null}" x:Name="PART_Minimize" Width="0" Height="0" Focusable="False" />
                            <Button Template="{x:Null}" x:Name="PART_Restore" Width="0" Height="0" Focusable="False" />
                            <Button Template="{x:Null}" x:Name="PART_Maximize" Width="0" Height="0" Focusable="False" />
                            <Button Template="{x:Null}" x:Name="PART_CloseButton" Width="0" Height="0" Focusable="False" />
                        </StackPanel>
                    </Grid>
                </dxc:DXWindowHeader>

                <Grid x:Name="FloatingContainerBody" Margin="8,0,8,8" Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" Background="{DynamicResource {dxt:DXWindowThemeKey ResourceKey=AeroWindowContainerBackground}}" Grid.RowSpan="2">
                        <ContentPresenter x:Name="PART_ContainerContent" Content="{Binding}" />
                    </Grid>
                    <StackPanel x:Name="PART_StatusPanel" Grid.Row="1" Visibility="Collapsed">
                        <Border Background="{x:Null}" HorizontalAlignment="Right">
                            <Thumb Template="{DynamicResource {dxt:DXWindowThemeKey IsVisibleInBlend=True, ResourceKey=AeroWindowSizeGripTemplate}}" Cursor="SizeNWSE" HorizontalAlignment="Right" x:Name="PART_SizeGrip" VerticalAlignment="Bottom" Width="16" Height="16" />
                        </Border>
                    </StackPanel>
                </Grid>
            </Grid>
            <Control x:Name="PART_ResizeFrame" Template="{DynamicResource {dxt:FloatingContainerThemeKey ResourceKey=FloatingContainerResizeFrame}}" Focusable="False" Visibility="{Binding Path=(dxc:FloatingContainer.IsMaximized), Converter={dxc:BooleanToVisibilityConverter Invert=True}}" />
        </Grid>
        <DataTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dxc:FloatingContainer.FloatingContainer).AllowSizing, RelativeSource={RelativeSource Self}}" Value="false">
                <Setter Property="Visibility" TargetName="PART_StatusPanel" Value="Collapsed" />
                <Setter Property="Visibility" TargetName="PART_ResizeFrame" Value="Collapsed" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxc:FloatingContainer.IsMaximized)}" Value="True">
                <Setter Property="Margin" TargetName="FloatingContainerBody" Value="0,0,0,0" />
            </DataTrigger>
        </DataTemplate.Triggers>
    </DataTemplate>
</ResourceDictionary>