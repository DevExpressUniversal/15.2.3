<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxei="http://schemas.devexpress.com/winfx/2008/xaml/editors/internal" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:dxet="http://schemas.devexpress.com/winfx/2008/xaml/editors/themekeys" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:cs="colorscheme_ignorable_uri" mc:Ignorable="cs">
    <!--BEGIN WPF-->
    <ResourceDictionary.MergedDictionaries>
        <dxdst:ResourceDictionaryEx Source="/DevExpress.Xpf.Themes.Office2010Black.v15.2;component/DevExpress.Xpf.Core/DevExpress.Xpf.Core/Editors/Themes/Generic/GradientMultiSlider.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonTemplate}}" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>
    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonTemplate}}" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>
    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonStyle}" TargetType="{x:Type Button}">
        <Setter Property="Template" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonTemplate}}" />
        <Setter Property="Width" Value="22" />
        <Setter Property="Height" Value="22" />
    </Style>
    <Style x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterStyle}" TargetType="{x:Type ContentPresenter}">
        <Setter Property="Width" Value="14" />
        <Setter Property="Height" Value="14" />
        <Setter Property="ContentTemplate" Value="{DynamicResource ResourceKey={dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterContentTemplate}}" />
    </Style>
    <DataTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=SelectedThumbContentPresenterContentTemplate}">
        <Border BorderThickness="1" BorderBrush="#FF000000" cs:Name="Border_0001">
            <Border.Background>
                <SolidColorBrush Color="{Binding Path=Color}" />
            </Border.Background>
        </Border>
    </DataTemplate>
    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=PreviousThumbButtonTemplate}">
        <Grid x:Name="Root" Background="#00000000" Width="22" Height="22" cs:Name="Grid_0001">
            <Path x:Name="Path" Stretch="Fill" Fill="#FF3C3C3C" Data="M5,0L0,6L5,12Z" Width="5" Height="10" cs:Name="Path_0001" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="Path" Property="Fill" Value="#FF3C3C3C" cs:Name="Setter_0001" />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter TargetName="Path" Property="Fill" Value="#FF3C3C3C" cs:Name="Setter_0002" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Root" Property="Opacity" Value="0.35" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=NextThumbButtonTemplate}">
        <Grid x:Name="Root" Background="#00000000" Width="22" Height="22" cs:Name="Grid_0002">
            <Path x:Name="Path" Stretch="Fill" Fill="#FF3C3C3C" Data="M0,0L5,6L0,12Z" Width="5" Height="10" cs:Name="Path_0002" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="Path" Property="Fill" Value="#FF3C3C3C" cs:Name="Setter_0003" />
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter TargetName="Path" Property="Fill" Value="#FF3C3C3C" cs:Name="Setter_0004" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Root" Property="Opacity" Value="0.35" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=FlipThumbsButtonTemplate}">
        <Grid x:Name="Root" Background="Transparent" cs:Name="Grid_0003">
            <Grid x:Name="HoverState" Opacity="0">
                <Border x:Name="hover1" CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_1539">
                            <GradientStop Color="#BFD7A820" Offset="0" cs:Name="GradientStop_0001" />
                            <GradientStop Color="#BFB89407" Offset="1" cs:Name="GradientStop_0002" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="hover2" Margin="1" CornerRadius="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_1540">
                                <GradientStop Color="#FFFFFAE6" Offset="0" cs:Name="GradientStop_0003" />
                                <GradientStop Color="#FFFFFFED" Offset="1" cs:Name="GradientStop_0004" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border x:Name="hover3" Margin="1" CornerRadius="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_1541">
                                    <GradientStop Color="#FFFFEEA8" Offset="0" cs:Name="GradientStop_0005" />
                                    <GradientStop Color="#FFFFFCE0" Offset="1" cs:Name="GradientStop_0006" />
                                    <GradientStop Color="#FFFFE277" Offset="0.5" cs:Name="GradientStop_0007" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PressedState" Opacity="0">
                <Border x:Name="pressed1" CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_1542">
                            <GradientStop Color="#BFA26F16" Offset="0" cs:Name="GradientStop_0008" />
                            <GradientStop Color="#BFA2832C" Offset="1" cs:Name="GradientStop_0009" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="pressed2" Margin="1" CornerRadius="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" cs:Name="LinearGradientBrush_1543">
                                <GradientStop Color="#FFFFE575" Offset="0" cs:Name="GradientStop_0010" />
                                <GradientStop Color="#FFFFF37F" Offset="1" cs:Name="GradientStop_0011" />
                                <GradientStop Color="#FFFFD86C" Offset="0.5" cs:Name="GradientStop_0012" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Path x:Name="Path" Stretch="Fill" Fill="#FF000000" Data="F1 M 1,4L 2,4L 2,3L 3,3L 3,2L 4,2L 4,1L 5,1L 5,2L 5,4L 13,4L 13,4.99991L 5,4.99999L 5,7L 5,8L 4,8L 4,7L 3,7L 3,6L 2,6L 2,5L 1,5L 1,4 Z M 13,10L 12,10L 12,11L 11,11L 11,12L 10,12L 10,13L 9,13L 9,12L 9,10L 1,10L 1,9.00009L 9,9.00001L 9,7L 9,6L 10,6L 10,7.00001L 11,7.00001L 11,8.00001L 12,8.00001L 12,9L 13,9L 13,10 Z " Width="12" Height="12" Margin="5" cs:Name="Path_0003" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="HoverState" Property="Opacity" Value="1" />
                <Setter TargetName="PressedState" Property="Opacity" Value="0" />
            </Trigger>
            <!-- <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="HoverState"   Property="Opacity" Value="0" />
                <Setter TargetName="PressedState" Property="Opacity" Value="1" />
            </Trigger> -->
            <Trigger Property="IsFocused" Value="True">
                <Setter TargetName="HoverState" Property="Opacity" Value="1" />
                <Setter TargetName="PressedState" Property="Opacity" Value="0" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Root" Property="Opacity" Value="0.35" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxet:GradientMultiSliderThemeKey ResourceKey=ThumbTemplate}" TargetType="{x:Type dxei:GradientMultiSliderThumb}">
        <Grid>
            <Path x:Name="PART_DarkPath" Stretch="Fill" Fill="#FF808080" Data="F1 M 15,14L 21,7L 27,14L 27,25L 15,25L 15,14 Z " Width="12" Height="17" cs:Name="Path_0004" />
            <Path x:Name="PART_WhitePath" Stretch="Fill" Fill="#FFE0E0E0" Data="F1 M 16,14L 21,9L 26,14L 26,24L 16,24L 16,14 Z " Width="10" Height="15" cs:Name="Path_0005" />
            <Border x:Name="PART_ColorPath" Margin="0,5,0,0" Width="8" Height="8">
                <Border.Background>
                    <SolidColorBrush Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}}" />
                </Border.Background>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True"></Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <!--END WPF-->
</ResourceDictionary>