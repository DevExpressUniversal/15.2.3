<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys">

    <SolidColorBrush x:Key="RadialMenuLevelControl_BackColor"  Color="#FF515151" />
    <SolidColorBrush x:Key="RadialMenuLevelControl_InnerColor" Color="#FF222222" />
    
    <SolidColorBrush x:Key="CenterButton_StrokeColor" Color="#FF666666" />
    <SolidColorBrush x:Key="CenterButton_InnerColor" Color="#FF333333" />
    <SolidColorBrush x:Key="CenterButton_HoverColor" Color="#FF3C3C3C" />
 
    <SolidColorBrush x:Key="BarItemInRadialMenu_MenuColor" Color="#FFF78A09" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathColor" Color="#FF333333" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathHover" Color="#FFFFA323" /> 
    
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=NormalImageColorizeColor}">#BFFFFFFF</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=HoverImageColorizeColor}">#FFFFFFFF</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=PressedImageColorizeColor}">#FFFFFFFF</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=DisabledImageColorizeColor}">#3FFFFFFF</Color>

</ResourceDictionary>