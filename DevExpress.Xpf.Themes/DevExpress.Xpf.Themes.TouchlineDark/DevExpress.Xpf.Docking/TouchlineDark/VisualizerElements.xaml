    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}" TargetType="{x:Type ve:DocumentSelectorContainer}">
        <Style.Setters>
            <Setter Property="Foreground" Value="#bfffffff" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
                        <Grid>
                            <Border x:Name="Window" Background="#FF515151" cs:Name="Border_0033">
                                <Border Background="#FF333333" Margin="1" cs:Name="Border_0034">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Border x:Name="Header" Background="#FF515151" cs:Name="Border_0035">
                                            <StackPanel Margin="0,16,24,16" VerticalAlignment="Center" HorizontalAlignment="Left" Orientation="Horizontal">
                                                <Image Margin="0,0,24,0" Stretch="None" VerticalAlignment="Top" HorizontalAlignment="Left" Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}" />
                                                <StackPanel>
                                                    <TextBlock Foreground="#FFF78A09" TextWrapping="Wrap" VerticalAlignment="Bottom" HorizontalAlignment="Left" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}" />
                                                    <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}" />
                                                </StackPanel>
                                            </StackPanel>
                                        </Border>
                                        <Border x:Name="Content" Background="#FF333333" Grid.Row="1" cs:Name="Border_0036">
                                            <ContentPresenter x:Name="PART_ContainerContent" Content="{Binding}" Margin="24" />
                                        </Border>
                                        <Border x:Name="Footer" Background="#FF515151" MinHeight="48" Grid.Row="2" cs:Name="Border_0037">
                                            <ContentPresenter HorizontalAlignment="Left" VerticalAlignment="Center" Margin="24,0,0,0" Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}" />
                                        </Border>
                                    </Grid>
                                </Border>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
        <Grid>
            <Grid x:Name="HoverState" Opacity="0">
                <Border Background="#FF666666" Margin="0,0,0,1" />
            </Grid>
            <Grid x:Name="SelectedState" Opacity="0">
                <Border Background="#FFF78A09" Margin="0,0,0,1" />
            </Grid>
            <Border Padding="{TemplateBinding Padding}">
                <ve:CaptionControl x:Name="caption" DataContext="{Binding}" Margin="0,0,0,1" />
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Opacity" TargetName="SelectedState" Value="1" />
                <Setter Property="Foreground" TargetName="caption" Value="#FF000000" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity" TargetName="HoverState" Value="1" />
                <Setter Property="Foreground" TargetName="caption" Value="#FF000000" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <DataTemplate x:Key="{dxt:DockPaneElementsThemeKey ResourceKey=ContentPreview}">
