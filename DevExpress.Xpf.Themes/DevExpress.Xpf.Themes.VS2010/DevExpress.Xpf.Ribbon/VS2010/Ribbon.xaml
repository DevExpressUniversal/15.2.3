            <Border Background="#FFC9D2E1" Margin="1">
                <Border Background="#FFBCC7D8" Margin="0,0,1,1">
                    <ContentPresenter/>
                </Border>
            </Border>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=HighlightedSelectedPageBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FF9CAAC1">
            <Border Background="#FFC9D2E1" Margin="0,1,0,1">
                <Border Background="#FFBCC7D8" Margin="0,0,0,1">
                    <ContentPresenter/>
                </Border>
            </Border>
        </Border>
    </ControlTemplate>

    <Style x:Key="{dxrt:RibbonThemeKey ResourceKey=MinimizationButtonStyle}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}"/>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=MinimizationButtonTemplate}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Grid SnapsToDevicePixels="True" MinWidth="21" Margin="1">
            <Border x:Name="HoverState" Background="#FFA19D8D" Opacity="0" CornerRadius="2">
                <Border Margin="1" CornerRadius="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFFFFEF8" Offset="0" />
                            <GradientStop Color="#FFFFF4D3" Offset="1" />
                            <GradientStop Color="#FFFFF9E8" Offset="0.5" />
                            <GradientStop Color="#FFFFF4D3" Offset="0.5" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border Margin="1" CornerRadius="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFFFFBEE" Offset="0" />
                                <GradientStop Color="#FFFFE8A6" Offset="1" />
                                <GradientStop Color="#FFFFF3D1" Offset="0.5" />
                                <GradientStop Color="#FFFFE8A6" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border> 
            </Border>
            <Border x:Name="PressedState" Background="#FF939EB0" Opacity="0" CornerRadius="2">
                <Border Background="#FFACB9CC" Margin="1" CornerRadius="1">
                    <Border Background="#FFB1BED1" Margin="1" />
                </Border>
            </Border>
            <Grid x:Name="Glyph"
                  Height="8" Width="10"
                  Margin="0"
                  HorizontalAlignment="Center" VerticalAlignment="Center"
                  RenderTransformOrigin="0.5,0.5">
                <Grid.RenderTransform>
                    <RotateTransform Angle="0"/>
                </Grid.RenderTransform>
                <Path Stretch="Fill" Fill="#FF404250"
                      Data="F1 M 122,183L 127,178L 132,183L 132,184L 130,186L 129,186L 127,184L 125,186L 124,186L 122,184L 122,183 Z"
                      UseLayoutRounding="False" Opacity="0.5"/>
                <Path Stretch="Fill" Fill="#FF404250"
                      Data="F1 M 122,183L 127,178L 132,183L 132,184L 130,186L 129,186L 127,184L 125,186L 124,186L 122,184L 122,183 Z"
                      UseLayoutRounding="False"/>
                <Path Stretch="Fill" Fill="#FFEAF0F7"
                      Data="F1 M 123,184L 124,185L 125,185L 127,183L 129,185L 130,185L 131,184L 131,183L 127,179L 123,183L 123,184 Z"
                      Margin="1" UseLayoutRounding="False"/>
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="HoverState" Storyboard.TargetProperty="Opacity" To="1" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="PressedState" Storyboard.TargetProperty="Opacity" To="1" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="HoverState" Storyboard.TargetProperty="Opacity" To="1" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CheckedStates">
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Glyph"
                                                           Storyboard.TargetProperty="(Grid.RenderTransform).(RotateTransform.Angle)">
                                <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="180"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Unchecked">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Glyph"
                                                           Storyboard.TargetProperty="(Grid.RenderTransform).(RotateTransform.Angle)">
                                <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="0"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <!--BEGIN WPF-->
    <ControlTemplate x:Key="{dxrt:KeyTipControlThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type dxr:RibbonCheckedBorderControl}">
        <Grid SnapsToDevicePixels="True">
            <Border x:Name="Root" Background="#FF767676" CornerRadius="2" Padding="1">
                <Border CornerRadius="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFFAFBFD" Offset="0"/>
                            <GradientStop Color="#FFD0DEF1" Offset="1"/>
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
            </Border>
            <ContentPresenter x:Name="contentPresenter"/>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="EnabledStated">
                    <VisualState x:Name="Enabled"/>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" Duration="0" To="0.5"/>
                            <DoubleAnimation Storyboard.TargetName="contentPresenter" Storyboard.TargetProperty="Opacity" Duration="0" To="0.5"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>

    <Style x:Key="{dxrt:KeyTipControlThemeKey ResourceKey=ContentStyle}"
           TargetType="{x:Type TextBlock}">
        <Setter Property="Margin" Value="3,0,3,1"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
    </Style>
    <!--END WPF-->
