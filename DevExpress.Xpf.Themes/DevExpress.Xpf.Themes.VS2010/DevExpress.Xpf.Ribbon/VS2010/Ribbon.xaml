            </VisualStateManager.VisualStateGroups>
            <Border x:Name="NormalState" Background="#FF939EB0" CornerRadius="64" Opacity="0">
                <Border CornerRadius="64" Margin="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFFCFDFD" Offset="0" />
                            <GradientStop Color="#FFE7EAEF" Offset="1" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="64" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFF7F8F9" Offset="0" />
                                <GradientStop Color="#FFCED4DF" Offset="1" />
                                <GradientStop Color="#FFD8DDE5" Offset="0.5" />
                                <GradientStop Color="#FFCED4DF" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="HoverState" Background="#FFA19D8D" CornerRadius="24" Opacity="0">
                <Border CornerRadius="64" Margin="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFFFFEF8" Offset="0" />
                            <GradientStop Color="#FFFFF4D3" Offset="1" />
                            <GradientStop Color="#FFFFF9E8" Offset="0.5" />
                            <GradientStop Color="#FFFFF4D3" Offset="0.5" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border CornerRadius="64" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFFFFBEE" Offset="0" />
                                <GradientStop Color="#FFFFE8A6" Offset="1" />
                                <GradientStop Color="#FFFFF3D1" Offset="0.5" />
                                <GradientStop Color="#FFFFE8A6" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="PressedState" Background="#FF939EB0" CornerRadius="64" Opacity="0">
                <Border Background="#FFACB9CC" CornerRadius="64" Margin="1">
                    <Border Background="#FFB1BED1" CornerRadius="64" Margin="1" />
                </Border>
            </Border>
            <Path x:Name="Glyph" Stretch="Fill" Fill="#FF1B293E"
                  Data="F1 M 124.5,56.5002L 124.5,64.5002L 120.5,60.5002L 124.5,56.5002 Z"
                  Margin="0,0,1,0" Height="8" Width="4" VerticalAlignment="Center" HorizontalAlignment="Center" />
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=SelectedPageBorderTemplateInPopup}"
                     TargetType="{x:Type ContentControl}">
        <Border Background="#FF9CAAC1">
            <Border Background="#FFC9D2E1" Margin="1">
                <Border Background="#FFBCC7D8" Margin="0,0,1,1">
                    <ContentPresenter/>
                </Border>
            </Border>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=SelectedPageBorderTemplate}"
                     TargetType="{x:Type ContentControl}">
        <Border Background="#FF9CAAC1">
            <Border Background="#FFC9D2E1" Margin="0,1,0,1">
                <Border Background="#FFBCC7D8" Margin="0,0,0,1">
                    <ContentPresenter/>
                </Border>
            </Border>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=StandaloneHeaderBorderTemplate}"
                     TargetType="{x:Type ContentControl}">
        <Border Background="#FF9CAAC1"/>
    </ControlTemplate>

    <!--BEGIN WPF-->
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=HeaderBorderTemplateInRibbonWindow}"
                     TargetType="{x:Type ContentControl}">
        <Border x:Name="PART_Header" SnapsToDevicePixels="True">
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFA3B0C7" Offset="0" />
                    <GradientStop Color="#FFCAD2DF" Offset="1" /> 
                </LinearGradientBrush>
            </Border.Background>
        </Border>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dx:FloatingContainer.IsActive), RelativeSource={RelativeSource Mode=TemplatedParent}}"
                         Value="False">
                <Setter Property="Background" TargetName="PART_Header">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFBFCDDB" Offset="0" />
                            <GradientStop Color="#FFD7E4F2" Offset="1" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="BorderBrush" TargetName="PART_Header" Value="#FFDAE5F2"/>
            </DataTrigger>  
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <!--END WPF-->

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=TabPartBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Border Background="#FF9CAAC1" />
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=HighlightedSelectedPageBorderTemplateInPopup}"
                     TargetType="{x:Type ContentControl}">
