<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys">

    <SolidColorBrush x:Key="RadialMenuLevelControl_BackColor" Color="#FF5B6B87" />
    <SolidColorBrush x:Key="RadialMenuLevelControl_InnerColor" Color="#FFFFFFFF" />
    
    <SolidColorBrush x:Key="CenterButton_StrokeColor" Color="#FF5B6B87" />
    <SolidColorBrush x:Key="CenterButton_InnerColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="CenterButton_HoverColor" Color="#7F5B6B87" /> 

    <SolidColorBrush x:Key="BarItemInRadialMenu_MenuColor" Color="#FF293955" />
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathColor" Color="#FFFFFFFF" />  
    <SolidColorBrush x:Key="BarItemInRadialMenu_PathHover" Color="#FF000723" />
    
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=NormalImageColorizeColor}">#FF1B293E</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=HoverImageColorizeColor}">#FF1B293E</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=PressedImageColorizeColor}">#FF1B293E</Color>
    <Color x:Key="{dxbt:RadialMenuItemThemeKey ResourceKey=DisabledImageColorizeColor}">#3F1B293E</Color>

</ResourceDictionary>