    <!--BEGIN WPF-->
    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}"
           TargetType="{x:Type ve:DocumentSelectorContainer}">
        <Style.Setters>
            <Setter Property="Foreground" Value="#FF1B293E" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
                        <Grid>
                            <Grid x:Name="Shadow" Margin="-1,-1,-1,-2">
                                <Border Background="#FF000000" CornerRadius="7" Opacity="0.05"/>
                                <Border Background="#FF000000" CornerRadius="6" Margin="1" Opacity="0.05"/>
                            </Grid>
                            <Border x:Name="Window" Background="#FF8591A2" CornerRadius="5" Margin="1">
                                <Grid Margin="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Border x:Name="Header" CornerRadius="4,4,0,0">
                                        <Border.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FFFFFFFF" Offset="0"/>
                                                <GradientStop Color="#FFF5F9FF" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Border.Background>
                                        <StackPanel Margin="12,8,12,8" VerticalAlignment="Center" HorizontalAlignment="Left" Orientation="Horizontal">
                                            <Image Margin="0,0,6,0" Stretch="None" VerticalAlignment="Top" HorizontalAlignment="Left"
                                                   Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}"/>
                                            <StackPanel>
                                                <TextBlock TextWrapping="Wrap" VerticalAlignment="Bottom" HorizontalAlignment="Left" FontWeight="Bold"
                                                           Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}"/>
                                                <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Top"
                                                           Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}"/>
                                            </StackPanel>
                                        </StackPanel>
                                    </Border>
                                    <Border x:Name="Content" Background="#FFD1DAE9" BorderBrush="#FFBBC0C8" BorderThickness="0,1,0,0" Grid.Row="1">
                                        <Grid>
                                            <Border Height="24" VerticalAlignment="Top" Opacity="0.05">
                                                <Border.Background>
                                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                        <GradientStop Color="#FF000000" Offset="0"/>
                                                        <GradientStop Color="#00000000" Offset="1"/>
                                                    </LinearGradientBrush>
                                                </Border.Background>
                                            </Border>
                                            <ContentPresenter x:Name="PART_ContainerContent" Content="{Binding}" Margin="12"/>
                                        </Grid>
                                    </Border>
                                    <Border x:Name="Footer" Background="#FFFFFFFF" CornerRadius="0,0,4,4" MinHeight="24" Grid.Row="2">
                                        <ContentPresenter HorizontalAlignment="Left" VerticalAlignment="Center" Margin="12,0,0,0"
                                                              Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}"/>
                                    </Border>
                                </Grid>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
        <Grid>
            <Grid x:Name="HoverSelection" Opacity="0">
                <Border Background="#FFE4C97F" CornerRadius="2" Padding="1">
                    <Border CornerRadius="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFFFFBF0" Offset="0" />
                                <GradientStop Color="#FFFFE8A6" Offset="1" />
                                <GradientStop Color="#FFFFF4D2" Offset="0.5" />
                                <GradientStop Color="#FFFFE8A6" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="Selection" Opacity="0">
                <Border Background="#FFE4C97F" CornerRadius="2" Padding="1">
                    <Border Background="#FFFFF9E9" Padding="1" CornerRadius="1">
                        <Border Background="#FFFFE8A6" CornerRadius="1"/>
                    </Border>
                </Border>
            </Grid>
            <Border Padding="{TemplateBinding Padding}">
                <ve:CaptionControl DataContext="{Binding}" Margin="3,2,3,2"/>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Opacity" TargetName="Selection" Value="1"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity" TargetName="HoverSelection" Value="0.5"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <DataTemplate x:Key="{dxt:DockPaneElementsThemeKey ResourceKey=ContentPreview}">
        <Grid Margin="0,0,-2,0" VerticalAlignment="Top" SnapsToDevicePixels="True">
            <Border x:Name="Shadow" Background="#FF000000" Margin="2,2,0,0" Opacity="0.10"/>
            <Border Background="#FFFFFFFF" BorderBrush="#FF919191" BorderThickness="1" Margin="0,0,2,2" Padding="12">
                <ContentPresenter Content="{Binding Content, RelativeSource={RelativeSource TemplatedParent}}"/>
            </Border>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="{dxt:DocumentElementsThemeKey ResourceKey=ContentPreview}">
        <Grid Margin="0,0,-2,0" VerticalAlignment="Top" SnapsToDevicePixels="True">
            <Border x:Name="Shadow" Background="#FF000000" Margin="2,2,0,0" Opacity="0.10"/>
            <Border Background="#FFFFFFFF" BorderBrush="#FF919191" BorderThickness="1" Margin="0,0,2,2" Padding="1">
                <ContentPresenter Content="{Binding Content, RelativeSource={RelativeSource TemplatedParent}}"/>
            </Border>
        </Grid>
    </DataTemplate>
    <!--END WPF-->

</ResourceDictionary>