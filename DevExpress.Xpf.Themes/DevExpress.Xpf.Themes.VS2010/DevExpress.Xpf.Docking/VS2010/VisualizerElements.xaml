            </Grid>

            <!-- Bottom -->
            <Grid x:Name="gridBottom" Grid.Column="1" Grid.Row="2" VerticalAlignment="Top">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <ve:DockHintButton x:Name="PART_TabBottom" Style="{StaticResource button-bottom-2}" dxp:DockHintElement.DockType="Bottom" Margin="2" Width="30" Height="30"/>
                <ve:DockHintButton Style="{StaticResource button-bottom-2}" x:Name="PART_Bottom" dxp:DockHintElement.DockType="Bottom" Margin="2" Width="30" Height="30" Grid.Row="1" />
            </Grid>

            <!-- Left -->
            <Grid x:Name="gridLeft" Grid.Row="1" HorizontalAlignment="Right">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ve:DockHintButton Style="{StaticResource button-left-2}" x:Name="PART_Left" dxp:DockHintElement.DockType="Left" Margin="2" Width="30" Height="30"/>
                <ve:DockHintButton x:Name="PART_TabLeft" Style="{StaticResource button-left-2}" dxp:DockHintElement.DockType="Left" Margin="2" Width="30" Height="30" Grid.Column="1"/>                 
            </Grid>

            <!-- Right -->
            <Grid x:Name="gridRight" Grid.Row="1" Grid.Column="2" HorizontalAlignment="Left">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ve:DockHintButton x:Name="PART_TabRight" Style="{StaticResource button-right-2}" dxp:DockHintElement.DockType="Right" Margin="2" Width="30" Height="30" />
                <ve:DockHintButton Style="{StaticResource button-right-2}" x:Name="PART_Right" dxp:DockHintElement.DockType="Right" Margin="2" Width="30" Height="30" Grid.Column="1"/>
            </Grid>

            <!-- Center -->
            <Grid x:Name="gridCenter" Grid.Row="1" Grid.Column="1">
                <ve:DockHintButton x:Name="PART_Fill" Style="{StaticResource button-center}" dxp:DockHintElement.DockType="Fill"/>
            </Grid>

        </Grid>
