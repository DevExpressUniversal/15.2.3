            <Border Background="#FFFFFFFF" BorderBrush="#FF3D4149" BorderThickness="1" Margin="-3" Grid.Column="0" Grid.Row="1" />
            <Border Background="#FFFFFFFF" BorderBrush="#FF3D4149" BorderThickness="1" Margin="-3" Grid.Column="2" Grid.Row="1" />
            <Border Background="#FFFFFFFF" RenderTransformOrigin="0.5,0.5" Margin="-17" Grid.Column="1" Grid.Row="1">
                <Border.RenderTransform>
                    <TransformGroup>
                        <RotateTransform Angle="45" />
                    </TransformGroup>
                </Border.RenderTransform>
            </Border>

            <!-- Top -->
            <Grid x:Name="gridTop" Grid.Column="1" VerticalAlignment="Bottom">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
