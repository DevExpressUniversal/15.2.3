                <ColumnDefinition Width="34"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>

            <Border Background="#FF3D4149" RenderTransformOrigin="0.5,0.5" Margin="-18" Grid.Column="1" Grid.Row="1">
                <Border.RenderTransform>
                    <TransformGroup>
                        <RotateTransform Angle="45" />
                    </TransformGroup>
                </Border.RenderTransform>
            </Border>
            <Border Background="#FFFFFFFF" BorderBrush="#FF3D4149" BorderThickness="1" Margin="-3" Grid.Column="1" Grid.Row="2" />
